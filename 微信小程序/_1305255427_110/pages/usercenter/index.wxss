navigator {
    margin-top: 0px;
    border: none;
    border-radius: none;
    padding: 0;
}

.header {
    width: 100%;
    height: 340rpx;
    padding: 64rpx 40rpx 0;
    position: relative;
    box-sizing: border-box;
    background: linear-gradient(90deg,rgba(255,235,59,1),rgba(255,214,41,1));
}

.header .content {
    width: 100%;
}

.header .content .avatarBox {
    border: 4rpx solid #fff;
    width: 92rpx;
    height: 92rpx;
    border-radius: 50%;
    margin-right: 36rpx;
    float: left;
    overflow: hidden;
}

.header .content .avatar {
    display: -webkit-box;
    display: -ms-flexbox;
    display: -webkit-flex;
    display: flex;
    -webkit-box-pack: center;
    -ms-flex-pack: center;
    justify-content: center;
    -webkit-box-align: center;
    -ms-flex-align: center;
    align-items: center;
    height: 92rpx;
}

.header .content .avatar image {
    max-width: 100%;
    max-height: 100%;
}

.header .userinfo {
    font-size: 26rpx;
    font-family: PingFangSC-Regular;
    color: rgba(51,51,51,1);
    line-height: 34rpx;
    float: left;
}

.header .userinfo .mobile {
    display: block;
    font-size: 40rpx;
    font-family: DINPro-Bold;
    color: rgba(51,51,51,1);
    line-height: 42rpx;
    margin-bottom: 14rpx;
}

.header .setting {
    float: right;
    margin-top: 16rpx;
}

.header .setting image {
    width: 48rpx;
    height: 48rpx;
}

.header .centerView {
    position: absolute;
    top: 216rpx;
    left: 40rpx;
    width: 670rpx;
    background: #ffffff;
    box-shadow: 0px 4px 10px 0px rgba(0,0,0,0.08);
    border-radius: 8px;
    box-sizing: border-box;
    padding: 30px 0;
    text-align: left;
}

.header .centerView navigator {
    width: 33.3333%;
    display: inline-block;
    text-align: center;
}

.header .centerView navigator image {
    width: 88rpx;
    height: 88rpx;
}

.header .centerView navigator span {
    font-size: 26rpx;
    font-family: PingFangSC-Regular;
    color: rgba(51,51,51,1);
    line-height: 34rpx;
    display: block;
}

.bottomView {
    margin-top: 170rpx;
    padding: 0 40rpx;
    box-sizing: border-box;
}

.bottomView navigator,.bottomView .toMoon {
    border-bottom: 2rpx solid #dedede;
    padding: 36rpx 0 20rpx;
}

.bottomView navigator span,.bottomView .toMoon span {
    font-size: 30rpx;
    font-family: PingFangSC-Regular;
    color: rgba(51,51,51,1);
    line-height: 38rpx;
    margin-left: 18rpx;
    position: relative;
    top: -14rpx;
}

.bottomView navigator .icon,.bottomView .toMoon .icon {
    width: 48rpx;
    height: 48rpx;
}

.bottomView navigator .arrow,.bottomView .toMoon .arrow {
    width: 28rpx;
    height: 28rpx;
    float: right;
    margin-right: 10rpx;
    margin-top: 8rpx;
}

.index-foot {
    position: fixed;
    width: 100%;
    height: 100rpx;
    left: 0;
    bottom: 0;
    background-color: #fff;
    box-shadow: 0px 4rpx 18rpx 0px rgba(0,0,0,0.14);
    display: flex;
    justify-content: space-around;
    align-items: center;
    box-sizing: border-box;
    padding-top: 8rpx;
    z-index: 999;
}

.index-foot.active {
    padding-bottom: 68rpx;
    height: 168rpx;
}

.index-foot .foot-left {
    text-align: center;
    font-size: 0;
    position: relative;
    padding: 0 40rpx;
    margin-top: 0;
    border: none;
    border-radius: 0;
    width: initial;
    height: initial;
}

.foot-left.active_618 {
    height: 84rpx;
}

.index-foot .foot-left::after,.index-foot .foot-left::before {
    border: none;
    display: none;
}

.index-foot span {
    opacity: .6;
    color: #333;
    line-height: 32rpx;
    height: 32rpx;
    margin-top: 4rpx;
    font-size: 22rpx;
    display: block;
}

.index-foot image {
    height: 48rpx;
    width: 48rpx;
    display: inline-block;
}

.foot-left.active_618 image {
    width: 100rpx;
    height: 102rpx;
    position: absolute;
    left: 22rpx;
    top: -58rpx;
}