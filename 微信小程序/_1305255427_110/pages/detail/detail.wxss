page {
    background: #f3f2f9;
    height: 100%;
}

.product-header {
    position: fixed;
    left: 0;
    width: 100%;
    height: 50rpx;
    box-shadow: 0 2px 2px 0 rgba(0,0,0,0.20);
    z-index: 11;
    display: flex;
    align-items: center;
    padding-right: 20rpx;
    box-sizing: border-box;
}

.product-header__title {
    font-size: 40rpx;
    color: #20b35d;
    margin-left: 36rpx;
    overflow: hidden;
    white-space: nowrap;
    text-overflow: ellipsis;
}

.progress-select {
    width: 100%;
    height: 50rpx;
    position: absolute;
}

.progress-circle {
    margin-left: 36rpx;
    position: relative;
    padding: 0;
    width: 100rpx;
    height: 100rpx;
}

.progressword {
    background-color: #fff;
    background: transparent;
    position: absolute;
    align-content: center;
    width: 100%;
    font-size: 26rpx;
    line-height: 50rpx;
    height: 50rpx;
    color: #0e0e0f;
}

.progressword-span {
    font-size: 36rpx;
    color: #0E0E0F;
}

.progress-number {
    display: inline-block;
    right: 20rpx;
    position: absolute;
    align-content: center;
    height: 50rpx;
    color: #5c626d;
    opacity: 0.8;
    background: transparent;
}

.discription-text {
    text-align: left;
    color: #0e0e0f;
    padding-left: 19rpx;
    background: #fff;
    opacity: 0.8;
    background: transparent;
}

.product-header__img {
    display: none;
    width: 100%;
    height: 100%;
    box-sizing: border-box;
}

.product-header__img.show {
    display: block;
}

.icon-kaiguan {
    display: none;
    width: 100rpx;
    height: 100rpx;
    position: relative;
    font-size: 46rpx;
    text-indent: 36rpx;
    line-height: 98rpx;
    color: #20b35d;
    border: 2rpx solid #20b35d;
    box-sizing: border-box;
    border-radius: 50%;
}

.icon-kaiguan.show {
    display: block;
}

.progressbar {
    position: absolute;
    bottom: 0;
    left: 0;
    right: 0;
    width: 100%;
    height: 40rpx;
    background-color: #fff;
}

.wx-progress-bar {
    background-color: #fff;
}

.property-list {
    display: flex;
    flex-direction: column;
    justify-content: space-between;
    box-sizing: border-box;
    background: #f3f2f9;
}

.property-item {
    width: 686rpx;
    margin: 32rpx 32rpx 0 32rpx;
    background: #f3f2f9;
    display: none;
}

.icon-checkbox {
    position: absolute;
    left: 0;
    padding: 0;
}

.icon-radiobox-selected:before {
    padding-right: 30rpx;
}

.icon-radiobox-unselected:before {
    color: #000;
    padding-right: 30rpx;
}

.separator:after {
    position: absolute;
    bottom: -0.1rpx;
    left: 65rpx;
    content: '';
    width: 550rpx;
    border-bottom: solid #e0ddf0 1rpx;
}

.icon-checkbox-checked:before {
    padding-right: 30rpx;
}

.icon-checkbox-default:before {
    color: #000;
    padding-right: 30rpx;
}

.property-item--radio .property-value {
    display: none;
    padding: 0 30rpx;
    margin: 0;
}

.property-item--radio .property-value .property-value--item {
    margin-left: 0;
}

.property-item--radio .selecting+.property-value {
    display: block;
}

.selecting {
    display: block;
}

.property-name {
    display: flex;
    align-items: center;
    height: 61rpx;
    line-height: 106rpx;
    padding: 0;
    font-size: 32rpx;
    background-color: #f3f2f9;
}

.property-name .icon-edit {
    padding: 40rpx 0 40rpx 40rpx;
}

.property-name__title {
    display: inline-block;
    font-size: 32rpx;
    padding-bottom: 16rpx;
    color: #5c626d;
}

.property-name--checkbox .property-name__title {
    width: 100%;
}

.property-name--checkbox {
    margin: 32rpx 32rpx 0 32rpx;
    padding: 32rpx 0 0 0;
    border-bottom-left-radius: 0;
    border-bottom-right-radius: 0;
}

.property-name__subtitle {
    font-size: 28rpx;
}

.property-selected {
    width: 380rpx;
    overflow: hidden;
    text-overflow: ellipsis;
    white-space: nowrap;
    color: #000;
    font-weight: 500;
}

.property-selected.property-bad {
    color: #f76260;
}

.property-value {
    display: flex;
    align-items: center;
    flex-wrap: wrap;
    border-top: 1rpx solid #d9d9d9 100%;
    border-radius: 10rpx;
    background-color: #fff;
}

.property-value--checkbox {
    margin-top: 0;
    border-top-left-radius: 0;
    border-top-right-radius: 0;
}

.property-value-wap {
    padding: 0 32rpx;
}

.property-value--item {
    display: flex;
    width: 554rpx;
    line-height: 50rpx;
    justify-content: flex-start;
    align-items: center;
    padding: 32rpx 0;
    box-sizing: border-box;
    font-size: 32rpx;
    color: #0e0e0f 100%;
    position: relative;
    text-align: left;
}

.property-value--item.disabled {
    color: #c1c2c4;
    border-color: #c1c2c4;
    background: #f0f0f0;
    display: none;
}

.property-value--item__oneline {
    text-align: left;
}

.icon-show-tip-title {
    position: relative;
    width: 50rpx;
    height: 50rpx;
    display: inline-block;
    left: 24rpx;
    margin-bottom: 16rpx;
}

.icon-show-tip {
    position: absolute;
    right: -68rpx;
    top: 50%;
    margin-top: -25rpx;
    color: #20b35d;
    width: 50rpx;
    height: 50rpx;
    font-size: 48rpx;
    text-align: center;
    line-height: 50rpx;
}

.product-footer {
    position: fixed;
    bottom: 0;
    left: 0;
    width: 100%;
    height: 120rpx;
    display: flex;
    align-items: center;
    background: #fff;
}

.product-footer.hidden {
    display: none;
}

.product-footer__title {
    width: 100%;
    height: 100%;
    border-radius: 0;
    border: none;
    line-height: 119rpx;
    background-color: #ffd000;
    font-size: 32rpx;
}

.product-footer__title:before {
    border-color: #ffd000;
}

.product-footer__title.disabled {
    display: none;
    background-color: #ffd000;
    color: rgba(255,255,255,0.6);
}

.product-footer-form {
    width: 100%;
}

.ahs-popup-container {
    display: flex;
    justify-content: center;
    align-items: center;
    bottom: 0;
    right: 0;
}

button:after {
    width: 0;
}

.property-value__popup {
    position: relative;
    background: #fff;
    width: 682rpx;
    max-height: 700rpx;
    padding-bottom: 20rpx;
    border-radius: 10rpx;
}

.property-value__img-swiper {
    min-height: 400rpx;
    width: 682rpx;
    height: 396rpx;
}

.property-value__img-swiper .slide-image {
    margin: 0 auto;
    display: block;
    width: 100%;
    height: 333.6rpx;
    border-top-left-radius: 10rpx;
    border-top-right-radius: 10rpx;
}

.slide-text-container {
    width: 682rpx;
    margin-top: 20rpx;
    padding: 20rpx 30rpx;
    line-height: 42rpx;
    word-wrap: break-word;
    white-space: normal;
    box-sizing: border-box;
    overflow: auto;
    text-align: left;
}

.slide-text-container-loseimg {
    width: 682rpx;
    margin-top: 20rpx;
    padding: 20rpx 30rpx;
    line-height: 42rpx;
    word-wrap: break-word;
    white-space: normal;
    box-sizing: border-box;
    overflow: auto;
    text-align: left;
    display: flex;
    justify-content: center;
    align-items: center;
}

.property-value__popup .slide-text {
    font-size: 30rpx;
    line-height: 42rpx;
    color: #353535;
}

.popup-close {
    position: absolute;
    left: 50%;
    margin-left: -31rpx;
    bottom: -112rpx;
    height: 62rpx;
    width: 62rpx;
    border-radius: 50%;
    color: #fff;
    font-size: 60rpx;
    text-align: center;
    line-height: 62rpx;
}

.select-popup-wrap {
    position: fixed;
    top: 0;
    bottom: 0;
    left: 0;
    right: 0;
    width: 100%;
    height: 100%;
    display: flex;
    justify-content: center;
    align-items: center;
    background-color: rgba(0,0,0,0.5);
    z-index: 99;
    display: none;
}

.select-popup-wrap.show {
    display: flex;
}

.select-popup {
    width: 682rpx;
    background-color: #fff;
    border-radius: 10rpx;
    padding-top: 50rpx;
}

.select-title {
    height: 50rpx;
    line-height: 50rpx;
    font-size: 36rpx;
    color: #000;
    text-align: center;
}

.select-popup-bottom {
    border-top: 1rpx solid #e5e5e5;
    width: 100%;
    height: 100rpx;
    overflow: hidden;
}

.select-popup-bottom .btn {
    width: 50%;
    float: left;
    box-sizing: border-box;
    text-align: center;
    line-height: 100rpx;
    font-size: 36rpx;
    color: #000;
}

.select-popup-bottom .btn-cancel {
    border-right: 1px solid #e5e5e5;
}

.speaker-content {
    padding: 34rpx 0 65rpx 0;
}

.speaker-tips {
    display: flex;
    flex-direction: column;
    align-items: center;
}

.speaker-icon-wrap {
    display: flex;
    flex-direction: column;
    justify-content: center;
    text-align: center;
    width: 216rpx;
    height: 216rpx;
    border: 1rpx solid #20b35d;
    border-radius: 50%;
    color: #20b35d;
}

.speaker-icon-wrap.active {
    background-color: #dfdfdf;
}

.speaker-icon {
    font-size: 76rpx;
}

.speaker-icon-wrap text {
    margin-top: 26rpx;
    font-size: 28rpx;
    line-height: 40rpx;
}

.speaker-text {
    margin-top: 20rpx;
    height: 45rpx;
    line-height: 45rpx;
    font-size: 32rpx;
    color: #888;
}

.speaker-text text {
    color: #000;
}

.speaker-sel-wrap {
    margin-top: 56rpx;
    width: 100%;
    display: flex;
    flex-direction: row;
    justify-content: space-around;
}

.speaker-sel-item {
    width: 100rpx;
    height: 100rpx;
    line-height: 100rpx;
    text-indent: 24rpx;
    font-size: 46rpx;
    color: #333;
    border-radius: 6rpx;
    border: 1px solid #434250;
}

.speaker-sel-item.active {
    border-color: #20b35d;
    color: #20b35d;
}

.speaker-popup-bottom .speaker-confirm {
    color: #20b35d;
}

.camera-popup-wrap {
    position: fixed;
    justify-content: center;
    align-items: center;
    top: 0;
    left: 0;
    right: 0;
    bottom: 0;
    width: 100%;
    height: 100%;
    background-color: rgba(0,0,0,0.5);
    display: none;
    z-index: 99;
}

.camera-popup-wrap.show {
    display: flex;
}

.camera-popup {
    width: 682rpx;
    padding-top: 50rpx;
    border-radius: 10rpx;
    background-color: #fff;
}

.camera-title {
    line-height: 50rpx;
    text-align: center;
    color: #000;
    font-size: 36rpx;
}

.camera-content {
    display: none;
}

.camera-content.show {
    display: block;
}

.camera-content .camera-tips,.camera-content image {
    display: block;
    width: 642rpx;
    height: 350rpx;
    margin: 20rpx auto;
}

.camera-content .camera-tips {
    padding-top: 80rpx;
    border: 1rpx dashed #d9d9d9;
    border-radius: 2rpx;
    box-sizing: border-box;
}

.camera-content .camera-tips .tips-icon {
    width: 158rpx;
    height: 116rpx;
    margin: 0 auto 28rpx;
    text-align: center;
    font-size: 98rpx;
    color: #20b35d;
}

.camera-content .camera-tips .tips-text {
    font-size: 32rpx;
    line-height: 42rpx;
    color: #888;
    text-align: center;
}

.camera-bottom {
    border-top: 1rpx solid #e5e5e5;
    width: 100%;
    height: 100rpx;
    overflow: hidden;
}

.camera-bottom .btn {
    width: 50%;
    float: left;
    box-sizing: border-box;
    text-align: center;
    line-height: 100rpx;
    font-size: 36rpx;
    color: #000;
}

.camera-bottom .camera-cancel {
    border-right: 1px solid #e5e5e5;
}

.camera-bottom .camera-check {
    color: #20b35d;
}

.captch-popup-wrap {
    position: fixed;
    justify-content: center;
    align-items: center;
    top: 0;
    left: 0;
    right: 0;
    bottom: 0;
    width: 100%;
    height: 100%;
    background-color: rgba(0,0,0,0.5);
    display: none;
    z-index: 99;
}

.captch-popup-wrap.show {
    display: flex;
}

.captch-popup {
    width: 682rpx;
    padding-top: 52rpx;
    border-radius: 10rpx;
    background-color: #fff;
}

.captch-title {
    line-height: 48rpx;
    text-align: center;
    color: #000;
    font-size: 34rpx;
}

.captch-content {
    padding: 38rpx 0 42rpx;
}

.captch-input-wrap {
    width: 580rpx;
    height: 94rpx;
    box-sizing: border-box;
    border: 1rpx solid #bebcbc;
    border-radius: 10rpx;
    color: #bebebe;
    position: relative;
    margin: 0 auto;
}

.captch-input-wrap.error {
    border-color: #ff2727;
}

.captch-input {
    display: block;
    height: 92rpx;
    width: 100%;
    padding: 22rpx 150rpx 22rpx 22rpx;
    font-size: 42rpx;
    color: #2a3440;
    line-height: 50px;
    box-sizing: border-box;
}

.captch-img {
    position: absolute;
    right: 10rpx;
    top: 4rpx;
    width: 130rpx;
    height: 86rpx;
}

.captch-bottom {
    height: 100rpx;
    border-top: 1px solid #e5e5e5;
}

.check-captch {
    display: block;
    height: 100rpx;
    width: 100%;
    text-align: center;
    background-color: #fff;
    line-height: 100rpx;
    font-size: 36rpx;
    color: #ffd000;
    border: none;
    padding: 0;
}

.captch-error {
    display: none;
    padding-top: 20rpx;
    width: 600rpx;
    color: #ff2727;
    font-size: 26rpx;
    line-height: 36rpx;
    margin: 0 auto;
}

.captch-error.show {
    display: block;
}