.page-container {
    position: absolute;
    top: 64rpx;
    left: 0;
    bottom: 0;
    right: 0;
    width: 100%;
    display: flex;
    justify-content: space-between;
    background-color: #fff;
}

.top-searchbar {
    width: 100%;
    top: 13rpx;
    height: 64rpx;
    background-color: #FFF;
    display: flex;
    align-items: center;
    justify-content: center;
}

.view-searchbar {
    height: 64rpx;
    width: 690rpx;
    background-color: #fff;
    border-radius: 10rpx;
    text-align: center;
    border-width: 0;
}

.img-searchbar {
    background-color: #fff;
    border-width: 0px;
    width: 25rpx;
    height: 25rpx;
    margin-right: 10rpx;
}

.img-searchbar text {
    width: 25rpx;
    height: 25rpx;
}

.nav-serchbar {
    width: 100%;
    height: 64rpx;
    color: #B2B2B2;
    font-size: 26rpx;
    background-color: #F3F2F9;
    border-width: 0;
    margin-top: 0;
    padding: 0;
    display: flex;
    flex-direction: row;
    align-items: center;
    justify-content: center;
    border-radius: 10rpx;
}

.text-searchbar {
    margin: 10rpx 0 9rpx 10rpx;
}

.top-scroll-view {
    height: 96rpx;
}

.top-container {
    width: 746.5rpx;
    height: 97rpx;
    background-color: #fff;
    padding-left: 3.5rpx;
    white-space: nowrap;
}

.bottom-container {
    position: absolute;
    top: 94rpx;
    left: 0;
    bottom: 0;
    right: 0;
    width: 100%;
    display: flex;
    justify-content: space-between;
    background-color: #fff;
    border-top: 1rpx #E6E6E6 solid;
}

.left-container {
    width: 200rpx;
    height: 100%;
}

.right-container {
    height: 100%;
    background-color: #fff;
}

.top-item {
    height: 97rpx;
    padding-left: 26.5rpx;
    padding-right: 32.5rpx;
    display: inline-block;
    font-size: 30rpx;
    color: #5C626D;
    line-height: 97rpx;
    position: relative;
}

.top-item.active {
    font-size: 30rpx;
    color: #000;
    background-color: #fff;
}

.top-item.active:after {
    content: '';
    display: block;
    position: absolute;
    left: 18%;
    margin-top: -25rpx;
    width: 62%;
    height: 6rpx;
    background-color: #FFD100;
}

.left-item {
    height: 96rpx;
    width: 200rpx;
    background-color: #F3F2F9;
    font-size: 30rpx;
    color: #5C626D;
    line-height: 96rpx;
    text-indent: 40rpx;
    position: relative;
}

.left-item.active {
    font-size: 30rpx;
    font-weight: bold;
    color: #000;
    background-color: #fff;
}

.left-item.active:before {
    content: '';
    display: block;
    position: absolute;
    top: 30%;
    margin-top: -25rpx;
    width: 6rpx;
    height: 88rpx;
    background-color: #FFD100;
}

.right-item {
    width: 550rpx;
}

.right-item navigator {
    width: 439rpx;
    position: relative;
    margin: 0;
    border: none;
    border-radius: 0;
    padding: 30rpx 15rpx 30rpx 84rpx;
    font-size: 32rpx;
    color: #333;
    word-wrap: break-word;
    white-space: normal;
    word-break: break-all;
    overflow: auto;
}

.right-item navigator:before {
    content: '';
    position: absolute;
    left: 30rpx;
    top: 50%;
    margin-top: -16rpx;
    width: 26rpx;
    height: 33rpx;
}

.right-item .first:before {
    background-position: -24rpx -72rpx;
}

.right-item .second:before {
    background-position: -112rpx -72rpx;
}

.right-item .third:before {
    background-position: -199rpx -72rpx;
}

.hover-active {
    background-color: #fff;
}

.top-searchbarmodel {
    width: 750rpx;
    height: 89rpx;
    display: block;
    position: relative;
    top: 0;
    background-color: #FFF;
    border-bottom: 1rpx solid #EEEEEE;
}

.top-searchbarmodel text {
    display: absolute;
    float: left;
    margin: 12.35rpx 6.5rpx 13rpx 20.8rpx;
    transform: scale(.65);
}

.view-searchbarmodel {
    width: 599rpx;
    height: 64rpx;
    background-color: #F3F2F9;
    display: inline-block;
    float: left;
    margin: 12rpx 0rpx 12rpx 32rpx;
    border-radius: 10rpx;
}

.input-searchbar {
    width: 486rpx;
    height: 64rpx;
    background-color: #F3F2F9;
    display: block;
    float: left;
    font-size: 28rpx;
    border-radius: 10rpx;
}

.clear-btn {
    height: 30rpx;
    width: 30rpx;
    position: absolute;
    right: 120rpx;
    padding: 16rpx;
}

.nav-serchbar-canel {
    width: 64rpx;
    height: 45rpx;
    margin: 20rpx 15rpx 23rpx 27rpx;
    color: #FFBB00;
    font-size: 32rpx;
    display: inline-block;
    position: relative;
    padding: 0;
    border: 0;
    background-color: #FFF;
}

.nav-serchbar-canel::after {
    border: 0;
}

.scroll-container {
    width: 750rpx;
}

.container-item {
    border-bottom: 1px #EEE solid;
}

.container-item.hidden {
    display: none;
}

.main-icon-before {
    margin: 0;
    border: 0;
    padding: 0;
    border-radius: 0;
}

.container-item navigator {
    margin: 0;
    border: 0;
    padding: 30rpx 0;
    border-radius: 0;
}

.scroll-view-container {
    width: 720rpx;
    margin-left: 30rpx;
}

.scroll-view-container.hidden {
    display: none;
}

.item-name {
    font-size: 30rpx;
    display: inline-block;
    word-break: break-all;
}

.item-price {
    font-size: 22rpx;
    color: #F15307;
    line-height: 30rpx;
    padding-left: 16rpx;
    display: inline-block;
    word-break: normal;
}

.my-own-device-container {
    padding: 20rpx 20rpx 23rpx!important;
    width: 100%!important;
    box-sizing: border-box;
}

.my-own-device-container .info-container {
    padding: 16rpx 20rpx 22rpx 9rpx;
    box-sizing: border-box;
    position: relative;
    box-shadow: 0 2rpx 14rpx 0 rgba(0,0,0,0.17);
    border-radius: 6rpx;
}

.my-own-device-container .info-container .my-own-image {
    width: 100rpx;
    height: 100rpx;
    position: absolute;
    left: 9rpx;
    top: 50%;
    margin-top: -50rpx;
}

.my-own-device-container .info-container .my-intro-container {
    padding-left: 120rpx;
    overflow: hidden;
}

.my-own-device-container .info-container .my-intro-container .tip-title {
    font-size: 26rpx;
    color: #0e0e0f;
    line-height: 30rpx;
    margin-top: 7rpx;
}

.my-own-device-container .info-container .my-intro-container .right-top-container {
    position: absolute;
    background-image: linear-gradient(-90deg,#ffd629 0%,#fff03b 100%);
    border-radius: 6rpx;
    width: 140rpx;
    box-sizing: border-box;
    text-align: center;
    position: absolute;
    right: 20rpx;
    top: 16rpx;
    height: 52rpx;
    line-height: 51rpx;
    font-size: 24rpx;
    color: #0e0e0f;
}

.my-own-device-container .info-container .my-intro-container .device-name {
    font-size: 30rpx;
    color: #0e0e0f;
    line-height: 44rpx;
    margin-top: 20rpx;
    max-height: 88rpx;
    word-break: break-all;
    text-overflow: ellipsis;
    display: -webkit-box;
    -webkit-box-orient: vertical;
    -webkit-line-clamp: 2;
    overflow: hidden;
}