.no-sku-container {
    position: absolute;
    top: 0;
    right: 0;
    bottom: 0;
    left: 0;
    margin: auto;
    background-color: #f5f5f5;
}

.sku-wrapper {
    position: absolute;
    width: 100%;
    left: 0;
    top: 50%;
    transform: translateY(-68%);
}

.sku-wrapper image {
    margin: 0 auto 42rpx;
    width: 148rpx;
    height: 196rpx;
    display: block;
}

.sku-wrapper text {
    display: block;
    text-align: center;
    font-size: 30rpx;
    color: #999;
    letter-spacing: 0;
    line-height: 38rpx;
    margin-bottom: 14rpx;
}

.sku-wrapper view {
    width: 400rpx;
    height: 88rpx;
    box-sizing: border-box;
    margin: 0 auto;
    text-align: center;
    line-height: 82rpx;
    margin-bottom: 40rpx;
    border-radius: 16rpx;
    color: #333;
    font-size: 32rpx;
}

.sku-wrapper .try-again {
    margin-top: 60rpx;
    line-height: 86rpx;
    background-image: linear-gradient(-90deg,#FFD629 0%,#FFF03B 100%);
}

.sku-wrapper .back-home {
    border: 1rpx solid #ccc;
    background-color: #fff;
}