.seckill-end {
    text-align: center;
    display: flex;
    justify-content: center;
    align-items: center;
    height: 100vh;
    width: 100%;
    flex-direction: column;
}

.nodata-img {
    display: block;
    width: 80px;
    height: 84px;
}

.end-text {
    margin-top: 20px;
    color: #999;
    font-size: 14px;
}

.home-btn {
    color: #f44;
    padding: 10px 18px;
    line-height: 16px;
    font-size: 14px;
    border: 1px solid #f44;
    border-radius: 2px;
    margin-top: 30px;
    background: #fff;
}