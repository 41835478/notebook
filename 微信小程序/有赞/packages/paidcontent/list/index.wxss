@import "../../../components/copyright/index.wxss";

page {
    background: #f9f9f9;
}

.paid-list {
    margin-bottom: 50px;
    min-height: 80vh;
    min-height: calc(100vh - 140px);
}

.zan-tab__item--selected .zan-tab__title {
    width: 100%;
}

.content-part,.column-part {
    background: #fff;
    overflow-x: hidden;
}

.no-item {
    padding: 85px 0;
    font-size: 12px;
    color: #999;
    text-align: center;
    background: #fff;
}

.no-more {
    height: 40px;
    line-height: 40px;
    text-align: center;
    font-size: 12px;
    color: #ccc;
}