<template name="package">
    <view class="package-container">
        <view class="package-circle">
            <view>{{activity.total_money}}元红包已包好</view>
            <view>点击[领取]开始瓜分</view>
        </view>
        <view class="package-actions">
            <image src="https://dn-kdt-img-test.qbox.me/public_files/2018/04/03/7bfd64b55972531544fd30e99d1a7f50.png"></image>
            <view class="empty-line-as-br"></view>
            <form-button bindtap="createPackage" class="package-btn" data-new="{{true}}" type="carve-coupon">领取</form-button>
        </view>
    </view>
</template>
