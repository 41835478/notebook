.text_red {
    color: #E93B3D;
}

.text_grey {
    color: #999;
}

.vam {
    vertical-align: middle;
}

.mt10 {
    margin-top: 10rpx;
}

.mt20 {
    margin-top: 20rpx;
}

.mt25 {
    margin-top: 25rpx;
}

.error_panel {
    flex: 1;
    display: flex;
    align-items: center;
    justify-content: center;
}

.dash_line {
    position: relative;
    margin: 0 8px;
    border-bottom: 1px dashed #E5E5E5;
}

.dash_line.dots:before,.dash_line.dots:after {
    content: '';
    width: 3px;
    height: 6px;
    position: absolute;
    top: -2.5px;
    background-color: #F7F7F7;
    box-shadow: inset 0 0 6rpx 0 rgba(0,0,0,0.10);
}

.dash_line.dots:before {
    left: -8px;
    border-top-right-radius: 3px;
    border-bottom-right-radius: 3px;
}

.dash_line.dots:after {
    right: -8px;
    border-top-left-radius: 3px;
    border-bottom-left-radius: 3px;
}

.xcoupon {
    position: relative;
    background: #fff;
    box-shadow: 0 0 6rpx 0 rgba(0,0,0,0.10);
    border-radius: 12rpx;
}

.xcoupon.type_jing {
    border-top: 12rpx solid #f97F80;
}

.xcoupon.type_jing .xcoupon_price {
    color: #f97F80;
}

.xcoupon.type_jing .xcoupon_tag {
    background-color: #f97F80;
}

.xcoupon.type_jing .xcoupon_tag:before {
    border-right-color: #f97F80;
}

.xcoupon.type_dong {
    border-top: 12rpx solid #53C7CA;
}

.xcoupon.type_dong .xcoupon_price {
    color: #53C7CA;
}

.xcoupon.type_dong .xcoupon_tag {
    background-color: #53C7CA;
}

.xcoupon.type_dong .xcoupon_tag:before {
    border-right-color: #53C7CA;
}

.xcoupon.type_yun {
    border-top: 12rpx solid #7DA7CE;
}

.xcoupon.type_yun .xcoupon_price {
    color: #7DA7CE;
}

.xcoupon.type_yun .xcoupon_tag {
    background-color: #7DA7CE;
}

.xcoupon.type_yun .xcoupon_tag:before {
    border-right-color: #7DA7CE;
}

.xcoupon.type_own {
    border-top: 12rpx solid #C9A86D;
}

.xcoupon.type_own .xcoupon_price {
    color: #C9A86D;
}

.xcoupon.type_own .xcoupon_tag {
    background-color: #C9A86D;
}

.xcoupon.type_own .xcoupon_tag:before {
    border-right-color: #C9A86D;
}

.xcoupon.disabled {
    border-top: 12rpx solid #999;
}

.xcoupon.disabled .xcoupon_price {
    color: #999;
}

.xcoupon.disabled .xcoupon_tag {
    background-color: #999;
}

.xcoupon.disabled .xcoupon_tag:before {
    border-right-color: #999;
}

.xcoupon_main {
    display: flex;
    height: 212rpx;
    font-size: 24rpx;
}

.xcoupon_price {
    box-sizing: border-box;
    width: 254rpx;
    padding: 0 10rpx;
    font-size: 28rpx;
    text-align: center;
    display: flex;
    justify-content: center;
    align-items: center;
}

.xcoupon_price .multi_range .p {
    padding-bottom: 4rpx;
}

.xcoupon_price .multi_range .p:last-child {
    padding-bottom: 0;
}

.xcoupon_price .multi_range .char_unit {
    font-size: 32rpx;
}

.xcoupon_price .multi_range .point,.xcoupon_price .multi_range .price {
    font-size: 40rpx;
}

.xcoupon_price .symbol_unit {
    font-size: 28rpx;
    line-height: 1;
    padding-right: 5rpx;
}

.xcoupon_price .char_unit {
    margin-left: 5rpx;
    font-size: 40rpx;
    line-height: 1;
}

.xcoupon_price .point {
    line-height: 1;
    font-size: 90rpx;
}

.xcoupon_price .price {
    line-height: 1;
    font-weight: bold;
    font-size: 90rpx;
    font-family: "font_steelfish";
}

.xcoupon_price .price.vat {
    vertical-align: top;
    display: inline-block;
}

.xcoupon_price .p {
    font-size: 28rpx;
    line-height: 1;
}

.xcoupon_price .tip {
    color: #999;
    font-size: 20rpx;
    line-height: 1;
}

.xcoupon_msg {
    flex: 1;
    overflow: hidden;
    padding-right: 10px;
    padding-top: 20rpx;
}

.xcoupon_condition {
    min-height: 72rpx;
    line-height: 36rpx;
    color: #666;
    font-size: 24rpx;
}

.xcoupon_attrs {
    padding-top: 14rpx;
}

.xcoupon_attrs_item {
    line-height: 34rpx;
    padding: 0 10rpx;
    color: #999;
    background-color: #F8F8F8;
    margin: 0 10rpx 10rpx 0;
}

.xcoupon_date {
    line-height: 30rpx;
    font-size: 20rpx;
    color: #999;
    padding-top: 17rpx;
}

.xcoupon_tag {
    display: inline-block;
    position: relative;
    margin: 0 7px 0 4px;
    padding: 0 5px;
    vertical-align: middle;
    border-top-right-radius: 2px;
    border-bottom-right-radius: 2px;
    font-size: 10px;
    line-height: 1.4;
    color: #fff;
}

.xcoupon_tag:before {
    content: '';
    position: absolute;
    top: 0;
    left: -8px;
    width: 0;
    height: 0;
    border-style: solid;
    border-width: 7px 4px;
    border-color: transparent;
}

.xcoupon_tag:after {
    content: '';
    position: absolute;
    top: 6px;
    left: -1px;
    width: 2px;
    height: 2px;
    border-radius: 2px;
    background-color: #fff;
}

.xcoupon_coner {
    position: absolute;
    top: 5px;
    left: 0;
    padding-right: 5px;
    height: 15px;
    border-top-right-radius: 8px;
    border-bottom-right-radius: 8px;
    line-height: 15px;
    font-size: 10px;
    color: #fff;
}

.xcoupon_coner:before {
    content: '';
    position: absolute;
    top: 0;
    left: -5px;
    height: 23px;
    width: 5px;
    border-top-left-radius: 5px;
    border-bottom-left-radius: 5px;
}

.xcoupon_coner:after {
    content: '';
    position: absolute;
    top: 15px;
    left: -4px;
    width: 4px;
    height: 8px;
    border-top-left-radius: 4px;
    border-bottom-left-radius: 4px;
}

.xcoupon_coner.col_red {
    background: linear-gradient(to right,#FF9574,#E93B3D);
}

.xcoupon_coner.col_red:before {
    background-color: #FF9574;
}

.xcoupon_coner.col_red:after {
    background-color: #D02B2D;
}

.xcoupon_coner.col_yellow {
    background: linear-gradient(to right,#F5B642,#E87725);
}

.xcoupon_coner.col_yellow:before {
    background-color: #F5B642;
}

.xcoupon_coner.col_yellow:after {
    background-color: #B37A0A;
}

.xcoupon_coner.col_grey {
    background: linear-gradient(to right,#ccc,#999);
}

.xcoupon_coner.col_grey:before {
    background-color: #ccc;
}

.xcoupon_coner.col_grey:after {
    background-color: #999;
}

.xcoupon_opera {
    height: 88rpx;
    display: flex;
    font-size: 28rpx;
    color: #333;
}

.xcoupon_opera_item {
    display: flex;
    justify-content: center;
    align-items: center;
    flex: 1;
    position: relative;
}

.xcoupon_opera_item:after {
    content: '';
    width: 1px;
    height: 30rpx;
    position: absolute;
    right: 0;
    top: 50%;
    margin-top: -15rpx;
    background-color: #E5E5E5;
}

.xcoupon_opera_item:last-child:after {
    display: none;
}

.xcoupon_purchase {
    font-size: 24rpx;
    padding: 20rpx;
}

.xcoupon_purchase .p {
    line-height: 33rpx;
}

.xcoupon_purchase_list {
    display: flex;
    align-items: center;
    margin-top: 20rpx;
}

.xcoupon_purchase_list .to_use {
    flex: 1;
    display: flex;
    justify-content: flex-end;
    align-items: center;
}

.xcoupon_purchase_list .icon_arrow_right {
    width: 18rpx;
    height: 18rpx;
    position: relative;
    transform: rotate(45deg);
}

.xcoupon_purchase_list .icon_arrow_right:before,.xcoupon_purchase_list .icon_arrow_right:after {
    content: '';
    background-color: #ccc;
    border-radius: 1px;
    position: absolute;
    top: 0;
}

.xcoupon_purchase_list .icon_arrow_right:before {
    width: 18rpx;
    height: 2px;
    left: 0;
}

.xcoupon_purchase_list .icon_arrow_right:after {
    width: 2px;
    height: 18rpx;
    right: 0;
}

.xcoupon_purchase_good {
    width: 60rpx;
    height: 60rpx;
    margin-right: 20rpx;
}

.xcoupon_explain {
    font-size: 24rpx;
    padding: 14rpx 20rpx 16rpx;
}

.xcoupon_explain_item {
    line-height: 33rpx;
    margin-bottom: 8rpx;
    display: flex;
}

.xcoupon_explain_l {
    width: 80rpx;
    color: #999;
    display: flex;
    justify-content: space-between;
    margin-right: 15rpx;
}

.xcoupon_explain_r {
    color: #151515;
    flex: 1;
}

.xcoupon_reset_btn {
    margin: 0;
    padding: 0;
    font-size: inherit;
    border-radius: 0;
    color: #333;
    background-color: transparent;
}

.toast text {
    text-align: center;
}