@import "../group-quality/group-quality.wxss";

.group-tag {
    position: absolute;
    left: 0;
    right: 0;
    width: 120rpx;
    margin: 0 auto;
    text-align: center;
    background-color: #fff;
    bottom: 30rpx;
    display: inline-block;
    height: 28rpx;
    line-height: 28rpx;
    font-size: 20rpx;
    color: #E93B3D;
    border: 1rpx solid #E93B3D;
    border-radius: 20rpx;
    padding: 0 5rpx;
    overflow: hidden;
    text-overflow: ellipsis;
    white-space: nowrap;
    box-shadow: 0 4px 8px 0 rgba(233,59,61,0.10);
}

.group-image__wrap {
    position: relative;
}

.group-image__price,.group-image__oldprice {
    position: absolute;
    left: 0;
    right: 0;
    text-align: center;
    overflow: hidden;
    text-overflow: ellipsis;
    white-space: nowrap;
}

.group-image__price {
    font-size: 24rpx;
    color: #E93B3D;
    font-weight: bold;
    bottom: 0;
    padding-top: 24rpx;
    height: 36rpx;
    line-height: 36rpx;
    background-image: linear-gradient(-180deg,rgba(255,255,255,0.00) 0%,rgba(255,255,255,0.80) 66%,#FFFFFF 100%);
}

.group-image__oldprice {
    bottom: -35rpx;
    font-size: 24rpx;
    color: #ccc;
    text-decoration: line-through;
}

.group-seckill .group__item-m,.group-seckill .group__item-xs {
    height: 308rpx;
}

.group-seckill .group__item-m .group-image {
    bottom: 58rpx;
}

.group-seckill .group__item-m .group-image__cover {
    width: 120rpx;
    height: 120rpx;
}

@font-face {
    font-family:"font_steelfish";src:url("data:application/font-ttf;charset=utf-8;base64,") format("truetype");font-style:normal;font-weight:normal;
}

.group__item-m .group-title__dd {
    overflow: visible;
}

.group-countdown {
    display: inline-block;
    vertical-align: middle;
    margin-top: -2rpx;
    color: #999;
    font-size: 24rpx;
}

.group-countdown__ul {
    display: flex;
    align-items: center;
}

.group-countdown__li {
    flex: 1;
    width: 30rpx;
    height: 36rpx;
    line-height: 36rpx;
    text-align: center;
    position: relative;
    margin-left: 10rpx;
    background: url('data:image/png;base64,iVBORw0KGgoAAAANSUhEUgAAAB4AAAAeBAMAAADJHrORAAAAD1BMVEVHcEzMzMzMzMzMzM3o6O0WcBHaAAAAA3RSTlMA6Q/ldZHqAAAAH0lEQVQY02NgGGJA2AiJw2hMkG9s4oIAzsYk6x/iAABtjAagxz6xbAAAAABJRU5ErkJggg==') no-repeat center;
    background-size: 30rpx;
    border-radius: 4rpx;
    overflow: hidden;
    border: 1rpx solid #ddd;
    border-radius: 4rpx;
}

.group-countdown__text {
    color: #E93B3D;
    font-size: 28rpx;
    font-family: "font_steelfish";
}