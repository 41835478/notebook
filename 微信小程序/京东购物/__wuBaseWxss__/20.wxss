.line1 {
    overflow: hidden;
    text-overflow: ellipsis;
    white-space: nowrap;
}

.line2,.line3 {
    overflow: hidden;
    text-overflow: ellipsis;
    word-break: break-all;
    display: -webkit-box;
    -webkit-line-clamp: 2;
    -webkit-box-orient: vertical;
}

.line3 {
    -webkit-line-clamp: 3;
}

.floor-title {
    font-size: 32rpx;
    font-weight: bold;
    padding: 0 30rpx;
    position: relative;
    height: 92rpx;
    line-height: 92rpx;
    background-color: #fff;
}

.floor-title__more {
    font-size: 24rpx;
    color: #999;
    position: absolute;
    right: 30rpx;
    top: 0;
    font-weight: normal;
}

.floor-title__more::after {
    content: '';
    display: inline-block;
    vertical-align: middle;
    margin: -4rpx 0 0 15rpx;
    width: 12rpx;
    height: 20rpx;
    background: url('data:image/png;base64,iVBORw0KGgoAAAANSUhEUgAAAAwAAAAUBAMAAABL3sEiAAAAGFBMVEUAAADMzMzOzs7MzMzOzs7MzMzNzc3Ozs6UdX0wAAAACHRSTlMA/lfjTdhSPylWYOYAAAA4SURBVAjXY3A2YwABQ0EFCCUEohwFwVwmQQhXERdXBMIVhlJQQagSuAYEB2F0IMSoQIi1pSYgEgDiuAczNyKAGAAAAABJRU5ErkJggg==') no-repeat;
    background-size: 100%;
}

.bg_stamp {
    background-image: url(data:image/png;base64,);
    background-repeat: no-repeat;
    background-position: 50%;
    background-size: 50px;
}

.channel-nav.channel-nav--618 {
    margin-top: 0;
    border-radius: 0;
}

.search-bar__shake {
    position: relative;
    height: 50px;
    width: 50px;
    z-index: 1;
}

.search-bar__shake-image {
    position: absolute;
    top: 50%;
    left: 50%;
    width: 30px;
    height: 30px;
    margin: -15px 0 0 -15px;
}

.sale_curtain {
    position: fixed;
    top: 0;
    left: 0;
    right: 0;
    bottom: 0;
    z-index: 199;
    opacity: 1;
    transition: opacity 0.4s linear;
}

.sale_curtain--hide {
    display: none;
}

.sale_curtain__preImage {
    position: absolute;
    top: 0;
    left: 0;
    width: 100%;
    height: 100%;
}

.sale_curtain__image {
    position: absolute;
    top: 0;
    left: 0;
    width: 100%;
    height: 100%;
}

.sale_curtain__countdown {
    z-index: 2;
    position: absolute;
    right: 15px;
    top: 55px;
    width: 70px;
    height: 25px;
    line-height: 25px;
    border-radius: 25px;
    background-color: rgba(0,0,0,.4);
    text-align: center;
    font-size: 10px;
    color: #fff;
}

.sale_curtain__countdown--hide {
    display: none;
}

.sale_curtain__countdown view {
    display: inline;
    position: relative;
    padding: 0 5px;
}

.sale_curtain__countdown view:nth-child(2)::before {
    content: "";
    position: absolute;
    z-index: 1;
    width: 1px;
    top: 3px;
    bottom: 3px;
    left: -1px;
    background-color: #fff;
}