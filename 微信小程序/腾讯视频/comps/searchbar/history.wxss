.searchbar-history .delete {
    background: url(https://i.gtimg.cn/qqlive/images/tinyapp/search_history_delete.svg) center center no-repeat;
    background-size: 34rpx 36rpx;
    height: 36rpx;
    width: 34rpx;
    display: inline-block;
    margin-top: -6rpx;
}

.searchbar-grid {
    padding: 4rpx 32rpx;
}

.searchbar-history .row {
    margin-top: 16rpx;
}

.searchbar-history .row.first {
    margin-top: 32rpx;
}

.searchbar-history .column {
    line-height: 64rpx;
    border-radius: 10rpx;
    background-color: #f5f7f9;
    font-size: 28rpx;
    text-align: center;
    color: #666;
    overflow: hidden;
    text-overflow: ellipsis;
    white-space: nowrap;
    padding: 0 16rpx;
    width: 330rpx;
}

.searchbar-history .column:nth-child(2n+1) {
    margin-right: 16rpx;
}

.searchbar-history .column.holder {
    background-color: transparent;
    cursor: none;
}