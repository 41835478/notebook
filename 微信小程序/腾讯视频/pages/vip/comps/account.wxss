.mod_user_log {
    position: relative;
    margin-bottom: 25px;
    overflow-x: hidden;
    margin-bottom: 0;
}

.mod_user_log {
    padding: 16px 16px 16px 16px;
}

.mod_user_log .pic {
    position: relative;
    display: block;
    border-radius: 50%;
    float: left;
    width: 64px;
    height: 64px;
}

.mod_user_log .pic image {
    display: block;
    width: 100%;
    height: 100%;
    border-radius: 50%;
}

.mod_user_log .pic .avatar_icon {
    position: absolute;
    height: 32rpx;
    width: 32rpx;
    bottom: 4rpx;
    right: 4rpx;
    background-size: 32rpx 32rpx;
    background-repeat: no-repeat;
    background-position: center center;
}

.mod_user_log .pic .avatar_icon.wx {
    background-image: url(https://puui.qpic.cn/vupload/0_20171010/icon_login_wx.png/0);
}

.mod_user_log .pic .avatar_icon.qq {
    background-image: url(https://puui.qpic.cn/vupload/0_20171010/icon_login_qq.png/0);
}

.mod_user_log .msg {
    position: relative;
    height: 64px;
    margin-left: 74px;
    display: box;
    box-orient: vertical;
    box-pack: center;
    box-align: center;
    display: -webkit-flex;
    display: flex;
    justify-content: center;
    flex-direction: column;
}

.mod_user_log .msg .tit .name {
    float: left;
    max-width: 143px;
    font-size: 16px;
    font-weight: bold;
    line-height: 22px;
    color: #222;
    margin-right: 4px;
    overflow: hidden;
    text-overflow: ellipsis;
    white-space: nowrap;
}

.mod_user_log .msg .tit .u_icon_vip_year,.mod_user_log .user_log .msg .tit .u_icon_vip_year_lost,.mod_user_log .user_log .msg .tit .u_icon_vip_year_past,.mod_user_log .user_log .msg .tit .u_icon_vip,.mod_user_log .user_log .msg .tit .u_icon_vip_lost {
    float: left;
    display: block;
    margin-top: 3px;
}

@media only screen and (max-width:360px) and (min-width:321px) {
    .mod_user_log .user_log .msg .tit .name {
        max-width: 100px;
    }
}

@media only screen and (max-width:375px) and (min-width:361px) {
    .mod_user_log .user_log .msg .tit .name {
        max-width: 130px;
    }
}

@media only screen and (max-width:384px) and (min-width:376px) {
    .mod_user_log .user_log .msg .tit .name {
        max-width: 130px;
    }
}

@media only screen and (max-width:400px) and (min-width:385px) {
    .mod_user_log .user_log .msg .tit .name {
        max-width: 130px;
    }
}

@media only screen and (min-width:401px) {
    .mod_user_log .user_log .msg .tit .name {
        max-width: 175px;
    }
}

.mod_user_log .msg .info .txt {
    display: block;
    font-size: 13px;
    line-height: 19px;
    color: #979797;
    word-break: break-all;
    overflow: hidden;
    text-overflow: ellipsis;
    display: -webkit-box;
    -webkit-line-clamp: 2;
    -webkit-box-orient: vertical;
}