page {
    height: 100%;
    font-family: -apple-system-font,"Helvetica Neue",sans-serif;
}

.container {
    height: 100%;
    display: flex;
    flex-direction: column;
    align-items: center;
    justify-content: space-between;
    padding: 200rpx 0;
    box-sizing: border-box;
    font-size: 200rpx;
}

.main {
    background: #f4f4f4;
}

.flex1 {
    flex: 1;
}

.flex-c {
    display: flex;
}

.sp-txt {
    width: 32rpx;
    height: 32rpx;
    line-height: 32rpx;
    text-align: center;
    display: inline-block;
    box-sizing: border-box;
    padding: 1rpx 4rpx;
    position: relative;
    top: -2rpx;
    border-radius: 2rpx;
    color: #fff;
    font-size: 24rpx;
    vertical-align: middle;
    margin-right: 12rpx;
}

.sp-tuan {
    background-color: #27CDC0;
}

.sp-wai {
    background-color: #ff6827;
}

.sp-mai {
    background-color: #ff6c6b;
}

.sp-quan {
    background-color: #FF9505;
}

.sp-ding {
    background-color: #27CDC0;
}

.sp-pai {
    background-color: #45c25a;
}

.ellipsis-txt {
    overflow: hidden;
    white-space: nowrap;
    word-wrap: break-word;
    word-break: break-all;
    text-overflow: ellipsis;
}

.star-container {
    display: inline;
}

.score-star-icon {
    width: 30rpx;
    height: 30rpx;
    margin-right: 4rpx;
}

.location-icon {
    width: 30rpx;
    height: 30rpx;
    margin-right: 8rpx;
    vertical-align: middle;
}

.yingye-icon {
    width: 30rpx;
    height: 30rpx;
    margin-right: 8rpx;
    vertical-align: middle;
}

.icon {
    width: 30rpx;
    height: 30rpx;
    margin-right: 8rpx;
    vertical-align: middle;
    position: relative;
    top: -2rpx;
}

.arrow-icon {
    width: 22rpx;
    height: 13rpx;
    margin-left: 10rpx;
}

.customForm {
    width: 100%;
    height: 100%;
    display: inline;
}

.formButton {
    width: inherit;
    height: inherit;
    padding-left: 0;
    padding-right: 0;
    background-color: inherit;
    line-height: inherit;
    font-size: inherit;
    display: inline;
    text-align: left;
    position: static;
}

.formButton::after {
    content: "";
    display: none;
}