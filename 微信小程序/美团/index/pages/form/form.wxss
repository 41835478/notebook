.form-container {
    background: #fff;
    font-size: 30rpx;
    text-align: center;
    height: 100%;
}

.form-container .bg {
    width: 100%;
    height: 100%;
    position: absolute;
    top: 0;
    left: 0;
    bottom: 0;
    right: 0;
}

.form-container .go-button {
    left: 0;
    font-size: 32rpx;
    color: #fff;
    display: inline-block;
    width: 660rpx;
    height: 88rpx;
    line-height: 88rpx;
    background: transparent;
}

.form-container .go-button:after {
    border: none;
}