.formid-ele {
    display: block;
    position: absolute;
    width: 100%;
    height: 100%;
    top: 0;
    left: 0;
}

.formid-ele text {
    display: none;
}

.formid-ele button {
    width: 100%;
    height: 100%;
    background-color: transparent;
}

.formid-ele button::after {
    display: none;
}