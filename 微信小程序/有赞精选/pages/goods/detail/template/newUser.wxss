.nu-cell {
    display: flex;
    align-items: center;
    border-top: 1rpx solid #e5e5e5;
    border-bottom: 1rpx solid #e5e5e5;
    height: 44px;
    padding-left: 15px;
}

.nu-cell__title {
    font-size: 18px;
    color: #FF5050;
    margin-right: 10px;
}

.nu-cell__subtitle {
    font-size: 12px;
    color: #999999;
}