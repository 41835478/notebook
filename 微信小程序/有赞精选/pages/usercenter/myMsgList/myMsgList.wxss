.container {
    background: #F9F9F9;
    overflow: hidden;
    min-height: 100vh;
    box-sizing: border-box;
}

.msg-list {
    margin-top: 0;
}

.msg-list__cell {
    height: 34px;
    padding: 5px 15px;
}

.msg-list__icon {
    height: 34px;
    margin-right: 10px;
}

.msg-list__img {
    height: 34px;
    width: 34px;
    border-radius: 50%;
}

.msg-list__contact-btn {
    opacity: 0;
    position: absolute;
    white-space: nowrap;
    top: 0;
    left: 0;
    right: 0;
    bottom: 0;
}

.wx-contact-button-wrapper {
    width: 300px;
    height: 100px;
}