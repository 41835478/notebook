.msxcx-p-my-avatar {
    position: relative;
    padding: 16rpx 0 16rpx 152rpx;
    height: 120rpx;
    line-height: 120rpx;
    font-size: 32rpx;
}

.msxcx-p-my-avatar image {
    position: absolute;
    top: 16rpx;
    left: 0;
    width: 120rpx;
    height: 120rpx;
    background: #eee url(http://i8.mifile.cn/b2c-mimall-media/ce267cd209d80a8e864401b0299f1af0.jpg);
    background-size: 100% 100%;
    border-radius: 6rpx;
}

.msxcx-p-my-avatar .name {
    display: block;
    margin-top: 8rpx;
    line-height: 48rpx;
    font-size: 32rpx;
}

.msxcx-p-my-avatar .sub {
    margin-top: 8rpx;
    height: 48rpx;
    line-height: 48rpx;
    font-size: 24rpx;
    color: #999;
}

.msxcx-p-my-avatar .sub view {
    float: left;
    padding: 0 16rpx;
    background: #ebebeb;
    color: #333;
    border-radius: 6rpx;
}

.msxcx-p-my-avatar .qr {
    position: absolute;
    top: 50%;
    right: 36rpx;
    margin-top: -20rpx;
    width: 40rpx;
    height: 40rpx;
    background: url(http://i8.mifile.cn/b2c-mimall-media/bcfde1499b3ab273f3163b36b298981f.png) no-repeat;
    background-size: 100% 100%;
}

.mx-profile-btn {
    position: relative;
    height: 80rpx;
    line-height: 80rpx;
    text-align: center;
    background: #fff;
}

.mx-profile-btn::before {
    content: '';
    position: absolute;
    top: 0;
    left: 0;
    right: 0;
    border-bottom: 1px solid #e5e5e5;
    -webkit-transform: scaleY(.5);
    -webkit-transform-origin: 0 0;
}

.mx-profile-btn>view {
    display: inline-block;
}

.mx-profile-btn>view image {
    float: left;
    margin: 24rpx 8rpx 0 0;
    width: 32rpx;
    height: 32rpx;
}

.mx-profile-btn>view text {
    float: left;
}

.msxcx-p-my-tips {
    position: fixed;
    left: 50%;
    bottom: 32rpx;
    padding: 0 16rpx;
    height: 48rpx;
    line-height: 48rpx;
    font-size: 24rpx;
    text-align: center;
    background: #ccc;
    border-radius: 2rpx;
    transform: translate(-50%,0);
}

.msxcx-p-my-tips text {
    margin: 0 8rpx;
}

.msxcx-p-my-tips::after {
    content: '\EA0C';
    float: left;
    margin-right: 8rpx;
    width: 32rpx;
    font-size: 32rpx;
    font-family: 'weui';
    height: 48rpx;
    line-height: 48rpx;
}

contact-button {
    position: fixed;
    right: 32rpx;
    bottom: 32rpx;
}

.mx-fs-qr {
    z-index: 100;
    position: fixed;
    top: 0;
    left: 0;
    width: 100%;
    height: 100%;
    background: #fff;
    text-align: center;
}

.mx-fs-qr>view {
    position: relative;
    height: 100%;
    overflow: hidden;
}

.mx-fs-qr .avatar {
    z-index: 9;
    position: absolute;
    display: block;
    top: 120rpx;
    left: 50%;
    margin-left: -70rpx;
    width: 140rpx;
    height: 140rpx;
    border-radius: 70rpx;
    background: url(http://i8.mifile.cn/b2c-mimall-media/2af46025303f84a8f72b725008d17cb3.png) no-repeat;
    background-size: 100% 100%;
}

.mx-fs-qr .name {
    z-index: 9;
    position: absolute;
    top: 280rpx;
    left: 0;
    width: 100%;
    color: #999;
}

.mx-fs-qr .qr {
    display: block;
    margin: 400rpx auto 0;
    padding: 24rpx;
    width: 360rpx;
    height: 360rpx;
    border: 1rpx solid #b2b2b2;
    border-radius: 5rpx;
}

.mx-fs-qr .qr-name {
    margin-top: 16rpx;
    font-size: 32rpx;
    color: #333;
}

.mx-fs-qr .x {
    z-index: 7;
    position: absolute;
    top: 32rpx;
    right: 32rpx;
    width: 40rpx;
    height: 40rpx;
    background: url(http://i8.mifile.cn/b2c-mimall-media/1e64b6707ec1fa9e8bcf57ce6b6ea739.png) no-repeat;
    background-size: 100% 100%;
    opacity: .2;
}

.mx-fs-qr .alert {
    z-index: 8;
    position: absolute;
    top: 0;
    left: 0;
    width: 100%;
    height: 100%;
    overflow: hidden;
    background: rgba(255,255,255,.98);
    text-align: center;
}

.mx-fs-qr .alert .i {
    margin: 500rpx auto 0;
    width: 140rpx;
    height: 140rpx;
    background: url(http://i8.mifile.cn/b2c-mimall-media/c630a64aaf09d67b46a4de72010a6d22.png) no-repeat;
    background-size: 100% 100%;
}

.mx-fs-qr .alert text {
    display: block;
    margin: 32rpx auto;
    font-size: 32rpx;
}

.mx-fs-qr .alert .know {
    display: inline-block;
    padding: 8rpx 32rpx;
    border: 1px solid #b2b2b2;
    border-radius: 5rpx;
}

.mx-fs-qr .copyright {
    position: absolute;
    left: 0;
    right: 0;
    width: 100%;
    bottom: 32rpx;
    font-size: 20rpx;
    color: #d0d0d0;
}

.msxcx-widget-icon-text-item {
    position: relative;
}

.turn-right {
    position: absolute;
    width: 16rpx;
    height: 25rpx;
    top: 12rpx;
    right: 32rpx;
}

.turn-right image {
    width: 100%;
    height: 100%;
}

.msxcx-widget-icon-text-item image {
    vertical-align: middle;
}

.msxcx-widget-icon-text-item .item-1 {
    width: 43rpx;
    height: 44rpx;
}

.msxcx-widget-icon-text-item .item-2 {
    width: 45rpx;
    height: 33rpx;
    margin-top: 34rpx;
}

.msxcx-widget-icon-text-item .item-3 {
    width: 44rpx;
    height: 38rpx;
}

.msxcx-widget-icon-text-item .item-4 {
    width: 42rpx;
    height: 35rpx;
    margin-top: 32rpx;
}

.msxcx-layout-mod__hd_right {
    margin-right: 50rpx;
}

.beta {
    color: #888;
}