button[type="primary"],button[type="primary"].button-hover {
    background-color: #ff6700;
    color: #fff;
}

.breakword {
    word-wrap: break-word;
    width: 606rpx;
}

button.block {
    width: 606rpx;
}

.flex-wrp {
    display: -webkit-flex;
    flex-direction: row;
    align-items: flex-end;
    padding: 25rpx 0 0 0;
    width: 606rpx;
    margin: 0 64rpx;
    position: relative;
}

.flex-wrp input,.flex-wrp input:focus {
    flex: 4;
    height: 74rpx;
    border-bottom: 1px solid #ddd;
    padding-top: 9rpx;
    padding-right: 114rpx;
    line-height: 74rpx;
    font-size: 28rpx;
}

.flex-wrp button.getSMS {
    font-size: 24rpx;
    width: 220rpx;
    height: 82rpx;
    line-height: 82rpx;
    border: 1px solid #ff6f0f;
    color: #ff6f0f;
    margin-left: 20rpx;
}

.flex-wrp .code {
    color: #ef5b00;
    line-height: 74rpx;
    padding-right: 12px;
    border-bottom: 1px solid #ddd;
}

.t1 {
    padding-left: 10%;
}

.clear {
    background: url(data:image/png;base64,);
    background-repeat: no-repeat;
    width: 20px;
    height: 20px;
}

.clear::after {
    content: none;
}

.err_tip {
    font-size: 14px;
    color: #ef5b00;
    text-align: left;
    width: 606rpx;
    margin-left: 64rpx;
    padding-left: 20px;
    background: url(data:image/png;base64,) no-repeat 0 4px;
}

.flex-wrp button.getSMS[disabled] {
    color: #d6d6d6;
    border-color: #d6d6d6;
}

button.block {
    margin: 60rpx 0 0 64rpx;
}

.login-line {
    height: 15rpx;
    width: 606rpx;
    margin: 69rpx 64rpx 76rpx 64rpx;
    border-bottom: 1px solid #e5e5e5;
    color: #a4a4a4;
    position: relative;
}

.login-or {
    position: absolute;
    top: 0;
    left: 50%;
    margin-left: -30rpx;
    width: 60rpx;
    height: 34rpx;
    line-height: 30rpx;
    font-size: 28rpx;
    background: #ffffff;
    text-align: center;
}

.login-end {
    width: 606rpx;
    margin: 0 64rpx;
}

.login-end button.login-left {
    float: left;
    width: 290rpx;
    height: 88rpx;
    line-height: 84rpx;
    background: #86cc52;
    color: #ffffff;
    border-radius: 4rpx;
    border: none;
    font-size: 24rpx;
    position: relative;
}

.login-end button.login-left:hover {
    background: #86cc52;
    color: #ffffff;
}

button.login-left::after {
    display: none;
}

button.login-left icon {
    vertical-align: middle;
    width: 45rpx;
    height: 38rpx;
    background: url(data:image/png;base64,) no-repeat;
    background-size: 100% 100%;
}

button.login-left text {
    vertical-align: middle;
    margin-left: 16rpx;
}

.login-end button.login-right {
    float: right;
    width: 290rpx;
    height: 88rpx;
    line-height: 82rpx;
    color: #000000;
    border-color: #bdbdbd;
    font-size: 24rpx;
}

.login-end button.login-right:hover {
    color: #000000;
    border-color: #bdbdbd;
}

button.login-right::after {
    display: none;
}

button.login-right .login-text {
    vertical-align: middle;
    width: 33rpx;
    height: 27rpx;
    background: url(data:image/png;base64,) no-repeat;
    background-size: 100% 100%;
}

button.login-right .login-phone {
    vertical-align: middle;
    width: 33rpx;
    height: 37rpx;
    background: url(data:image/jpg;base64,) no-repeat;
    background-size: 100% 100%;
}

button.login-right text {
    vertical-align: middle;
    margin-left: 21rpx;
}

.eye {
    z-index: 100;
    position: absolute;
    top: 0;
    right: 0;
    width: 114rpx;
    height: 104rpx;
    background: #ffffff;
}

.close-eye {
    margin: 35rpx 0 0 30rpx;
    width: 54rpx;
    height: 35rpx;
    background: url(data:image/png;base64,) no-repeat;
    background-size: 100% 100%;
}

.open-eye {
    margin: 35rpx 0 0 30rpx;
    width: 54rpx;
    height: 37rpx;
    background: url(data:image/png;base64,) no-repeat;
    background-size: 100% 100%;
}