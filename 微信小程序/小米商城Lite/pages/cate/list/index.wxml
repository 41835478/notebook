<view class="mx-cate-title">
    <view>
        <text>{{title}}</text>
    </view>
</view>
<view class="msxcx-layout-mod" style="margin:0;">
    <view class="msxcx-layout-mod__bd">
        <view class="msxcx-widget-list">
            <navigator class="msxcx-widget-list__item" url="/pages/product/index?id={{goods.product_id}}&posID=10" wx:for="{{list}}" wx:for-item="goods">
                <view class="mx-p-search-result">
                    <image src="{{goods.img_map&&goods.img_map.img_1_1[0]}}"></image>
                    <text class="a">{{goods.name}}</text>
                    <text class="b">{{goods.product_desc_origin}}</text>
                    <text class="c">{{goods.price}}</text>
                </view>
            </navigator>
        </view>
    </view>
</view>
