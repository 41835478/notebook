.msxcx-sp {
    position: relative;
    width: 100%;
    overflow: auto;
}

.msxcx-sp image,.msxcx-sp video {
    display: block;
    width: 100%;
}

.msxcx-sp text {
    display: block;
    font-size: 28rpx;
}

.msxcx-sp .text-group {
    padding: 16rpx;
    overflow: auto;
}

.msxcx-sp .text-group text {
    font-size: 32rpx;
    white-space: nowrap;
    text-overflow: ellipsis;
    overflow: hidden;
}

.msxcx-sp .text-group text.sub {
    color: #999;
    font-size: 24rpx;
}

.msxcx-sp .text-group text.price {
    color: #ff4a00;
}

.msxcx-sp .text-group text.line-through {
    text-decoration: line-through;
}

.msxcx-sp__clear {
    clear: both;
    height: 0;
    overflow: hidden;
}

.msxcx-sp__h1 {
    margin: 64rpx 32rpx;
    font-size: 40rpx;
    font-weight: bold;
    color: #333;
    text-align: left;
}

.msxcx-sp__h1 text {
    font-size: 40rpx;
}

.msxcx-sp__h2 {
    position: relative;
    margin: 96rpx 32rpx 64rpx;
    font-size: 32rpx;
    color: #ff4a00;
    text-align: center;
}

.msxcx-sp__h2 text {
    font-size: 32rpx;
    font-weight: bold;
}

.msxcx-sp__h2::before {
    content: '';
    display: block;
    position: absolute;
    top: -32rpx;
    left: 50%;
    margin-left: -50rpx;
    width: 100rpx;
    height: 6rpx;
    background: #ff6700;
    border-radius: 3rpx;
}

.msxcx-sp__summary {
    margin: 64rpx 32rpx;
    padding: 32rpx;
    color: #333;
    background: #eee;
    text-align: justify;
}

.msxcx-sp__space {
    clear: both;
    height: 32rpx;
}

.msxcx-sp__line {
    clear: both;
    height: 0;
    border-bottom: 1px solid #e5e5e5;
}

.msxcx-sp__title {
    margin-top: 32rpx;
    padding: 0 32rpx;
    height: 88rpx;
    line-height: 88rpx;
    font-size: 32rpx;
}

.msxcx-sp__title .left {
    float: left;
}

.msxcx-sp__title .right {
