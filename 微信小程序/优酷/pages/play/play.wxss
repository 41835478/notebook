text {
    white-space: pre-line;
}

.container {
    width: 750rpx;
    overflow-x: hidden;
}

.border-bottom {
    border-bottom: 2rpx solid #ebebeb;
}

.module-name {
    height: 36rpx;
    line-height: 36rpx;
    font-size: 34rpx;
    font-weight: 700;
    white-space: nowrap;
    overflow: hidden;
    text-overflow: ellipsis;
    float: left;
}

button {
    border: none;
    padding: 0;
    line-height: 0;
    background-color: #fff;
    border-radius: 0;
}

button::after {
    content: '';
    border: none;
}

.module {
    position: relative;
    padding: 15px 0;
}

.clearfix {
    zoom: 1;
}

.net-cover {
    position: absolute;
    left: 0;
    top: 0;
    z-index: 1;
}

.change-recommend {
    padding: 0;
    font-size: 13px;
    font-weight: bolder;
    color: #2692ff;
    display: inline-block;
    position: relative;
}

.gradientbg {
    background: linear-gradient(-180deg,rgba(0,0,0,0) 5%,rgba(0,0,0,0.7) 99%);
    height: 90rpx;
    width: 100%;
    position: absolute;
    bottom: 0;
    z-index: 2;
}

.vtitle {
    color: #000;
    font-size: 28rpx;
    padding-left: 18rpx;
    overflow: hidden;
    text-overflow: ellipsis;
    white-space: nowrap;
    display: block;
    line-height: 28rpx;
    height: 28rpx;
}

.vdec {
    color: #999;
    font-size: 24rpx;
    overflow: hidden;
    text-overflow: ellipsis;
    white-space: nowrap;
    padding-left: 18rpx;
    display: block;
    line-height: 24rpx;
    height: 24rpx;
    padding-top: 10rpx;
}

.item-score {
    font-size: 36rpx;
    font-weight: 600;
    color: #FF6600;
    position: absolute;
    right: 32rpx;
    bottom: 12rpx;
    height: 36rpx;
    z-index: 3;
    line-height: 36rpx;
}

.score-small {
    font-size: 22rpx;
    right: 12rpx;
    height: 24rpx;
    line-height: 23rpx;
}

.drawlist {
