.searchbar {
    background: #ffc811;
    padding: 0 0 20rpx 20rpx;
    position: relative;
}

.searchbar-focus {
    position: fixed;
    left: 0;
    top: 0;
    right: 0;
    z-index: 99;
}

.searchbar_label {
    border-radius: 8rpx;
    background: #fff;
    color: #b2b2b2;
    font-size: 14px;
    height: 56rpx;
    line-height: 1;
    margin-right: 20rpx;
    text-align: center;
    display: -webkit-box;
    display: -webkit-flex;
    display: flex;
    -webkit-box-align: center;
    align-items: center;
    -webkit-box-pack: center;
    justify-content: center;
}

.searchbar_icon {
    margin: 0 10rpx 0 0;
    width: 14px;
    height: 14px;
}

.searchbar_form {
    color: #111;
    display: -webkit-box;
    display: -webkit-flex;
    display: flex;
    -webkit-box-align: center;
    align-items: center;
    height: 56rpx;
    position: relative;
}

.searchbar_inputgroup {
    background: #fff;
    border-radius: 8rpx;
    box-sizing: border-box;
    display: -webkit-box;
    display: -webkit-flex;
    display: flex;
    height: 56rpx;
    position: relative;
    width: 100%;
}

.searchbar_form_icon {
    position: absolute;
    left: 8px;
    top: 50%;
    margin-top: -7px;
}

.searchbar_input {
    border: none;
    height: 56rpx;
    width: 100%;
    padding: 0 30px;
    color: #111;
    font-size: 28rpx;
    position: relative;
}

.searchbar_clear {
    position: absolute;
    right: 0;
    top: 0;
    bottom: 0;
    width: 30px;
}

.searchbar_clear_icon {
    vertical-align: top;
    position: absolute;
    left: 8px;
    top: 50%;
    margin-top: -7px;
}

.searchbar_cancel {
    color: #111;
    font-size: 32rpx;
    line-height: 56rpx;
    width: 130rpx;
    text-align: center;
}

.searchbar-result {
    background: #fff;
    border-radius: 0 0 20rpx 20rpx;
    font-size: 1.3em;
    line-height: 1.5;
    position: fixed;
    left: 0;
    right: 0;
    bottom: 0;
    top: 72rpx;
    z-index: 9;
}

.searchbar_input_fake {
    text-align: center;
    color: #b2b2b2;
    font-size: 1.3em;
}

.searchbar_input_fake::-webkit-input-placeholder {
    color: #b2b2b2;
}

.searchbar-result_cell {
    position: relative;
    font-size: 28rpx;
    line-height: 40rpx;
    padding: 24rpx 0;
    margin: 0 0 0 30rpx;
}

.searchbar-result_cell::after {
    position: absolute;
    left: 0;
    bottom: 0;
    width: 100%;
    content: "";
    height: 2rpx;
    background: #000;
    opacity: 0.1;
}

.searchbar_cellmdd,.searchbar_cellgl {
    position: relative;
    padding-left: 21px;
}

.searchbar_cellmdd::before,.searchbar_cellgl::before {
    background: url(data:image/png;base64,) 0 0 no-repeat;
    background-size: 16px 32px;
    content: '';
    position: absolute;
    left: 0;
    top: 50%;
    margin-top: -8px;
    height: 16px;
    width: 16px;
}

.searchbar_cellgl::before {
    background-position: 0 -16px;
}