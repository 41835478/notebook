page {
    background-color: #f8f8f8;
}

.cat-gap {
    height: 20rpx;
    background-color: #f8f8f8;
    border-top: 1rpx solid #e0e0e0;
}

.cat-item {
    display: flex;
    flex-direction: row;
    justify-content: space-between;
    align-items: center;
    height: 88rpx;
    border-top: 1rpx solid #e0e0e0;
    background-color: #ffffff;
    padding-left: 20rpx;
    padding-right: 20rpx;
}

.cat-sub-itemBody {
    display: flex;
    flex-wrap: wrap;
    flex-direction: row;
    align-items: center;
    justify-content: flex-start;
    background-color: #ffffff;
}

.cat-sub-item {
    display: flex;
    align-items: center;
    justify-content: center;
    width: 360rpx;
    height: 80rpx;
    background-color: #f0f2f5;
    margin-left: 10rpx;
    margin-bottom: 10rpx;
}

.cat-font {
    color: #232326;
    font-size: 30rpx;
}

.cat-img {
    width: 38rpx;
    height: 38rpx;
}

.cat-seeall {
    display: flex;
    flex-direction: row;
    justify-content: flex-end;
    align-items: center;
    height: 88rpx;
    background-color: #ffffff;
    padding-left: 20rpx;
    padding-right: 20rpx;
}

.cat-seeal-font {
    font-size: 26rpx;
    color: #686868;
}