.smartbox {
    background-color: #101419;
    box-sizing: border-box;
    padding: 22rpx 20rpx 0 20rpx;
    position: relative;
}

.smartbox ::-webkit-input-placeholder {
    color: #5b616b;
}

.smartbox__panel {
    height: 60rpx;
    font-size: 26rpx;
    border-radius: 10rpx;
    border: 1rpx solid #36485d;
    width: 100%;
    box-sizing: border-box;
    color: #fff;
    background: url(data:image/png;base64,) no-repeat 10px center;
    background-size: 13px 13px;
}

.smartbox__panel input {
    width: 90%;
    padding-left: 55rpx;
    height: 60rpx;
    line-height: 60rpx;
}

.clear-btn {
    position: absolute;
    top: 40rpx;
    right: 40rpx;
    opacity: 0.5;
    background: url(data:image/png;base64,);
    width: 24rpx;
    height: 24rpx;
    background-size: contain;
}

.smartboxList {
    color: #f7fbff;
}

.smartboxList__code {
    color: #656b6e;
    margin-left: 10rpx;
    font-size: 24rpx;
}

.smartboxList__tr {
    padding: 0 32rpx;
    height: 84rpx;
    display: -webkit-box;
    display: -ms-flexbox;
    display: -webkit-flex;
    display: flex;
    -ms-flex-pack: justify;
    justify-content: space-between;
    box-pack: justify;
}

.smartboxList__tr__item,.smartboxList__tr__item--small {
    font-size: 34rpx;
    line-height: 34rpx;
    display: -webkit-box;
    display: -ms-flexbox;
    display: -webkit-flex;
    display: flex;
    -webkit-box-align: center;
    -ms-flex-align: center;
    align-items: center;
    position: relative;
}

.smartboxList__tr__item__market--us,.smartboxList__tr__item__market--hk,.smartboxList__tr__item--small__market--us,.smartboxList__tr__item--small__market--hk {
    width: 24rpx;
    height: 24rpx;
    text-align: center;
    line-height: 26rpx;
    color: #18191a;
    font-size: 14rpx;
    font-weight: bold;
    position: absolute;
    left: -30rpx;
    top: 28rpx;
}

.smartboxList__tr__item__market--us,.smartboxList__tr__item--small__market--us {
    background-color: #0d5f8d;
}

.smartboxList__tr__item__market--hk,.smartboxList__tr__item--small__market--hk {
    background-color: #75310c;
}

.smartboxList__tr__item--small {
    font-size: 30rpx;
    line-height: 30rpx;
}

.smartboxList__tr__item2 {
    color: #656b6e;
    height: 84rpx;
    line-height: 84rpx;
}

.smartboxList__tr__item3 {
    background: url(data:image/png;base64,) no-repeat center;
    background-size: 23px 23px;
    width: 23px;
    height: 84rpx;
}

.smartboxList__tr__item3--added {
    background: none;
    width: 88rpx;
    font-size: 28rpx;
    color: #4c6c90;
    display: -webkit-box;
    display: -ms-flexbox;
    display: -webkit-flex;
    display: flex;
    -webkit-box-align: center;
    -ms-flex-align: center;
    align-items: center;
}

.history-tip {
    color: #5b616b;
    font-size: 30rpx;
    margin: 26rpx 0 10rpx 34rpx;
}

.history-clear {
    color: #5b616b;
    margin-top: 20rpx;
    font-size: 28rpx;
    text-align: center;
}

.egg {
    display: none;
}

.eggShow {
    display: block;
}

.bannerstip {
    background: #222e3c;
    line-height: 85rpx;
    width: 750rpx;
    width: 100%;
    text-align: center;
    color: #f7fbff;
    font-size: 30rpx;
    position: fixed;
    top: -85rpx;
    height: 85rpx;
    overflow: hidden;
    transition: top 0.6s;
    z-index: 20;
}

.bannerstip .exclamation {
    display: inline-block;
    vertical-align: middle;
    width: 40rpx;
    height: 40rpx;
    margin-right: 10rpx;
    margin-bottom: 3rpx;
    background: url(data:image/png;base64,) no-repeat;
    background-size: contain;
}

.bannerstip.show {
    top: 0;
    transition: top 0.6s;
}