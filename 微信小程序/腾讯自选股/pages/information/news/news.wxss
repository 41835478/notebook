.news.nodata {
    width: 100%;
    margin-top: 50%;
    text-align: center;
    padding: 0;
}

.news.nodata .nodata-icon {
    display: inline-block;
    background: url(data:image/png;base64,) no-repeat;
    background-size: contain;
    width: 53rpx;
    height: 61rpx;
}

.news.nodata .nodata-text {
    color: #bebebe;
    font-size: 20rpx;
}

.news {
}

.news_inline {
    display: inline-block;
}

.news_banner {
    height: 320rpx;
    width: 100%;
    position: relative;
}

.news_banner image {
    width: 100%;
    height: 100%;
    background-size: contain;
}

.news_banner_title {
    position: absolute;
    bottom: 0;
    width: 690rpx;
    padding: 26rpx 30rpx;
    font-size: 32rpx;
    line-height: 1.38;
    letter-spacing: normal;
    text-align: justify;
    color: #ffffff;
    background-image: -webkit-gradient(linear,left top,left bottom,from(rgba(0,0,0,0)),color-stop(52%,rgba(0,0,0,0.54)),color-stop(52%,#000000));
    background-image: -webkit-linear-gradient(top,rgba(0,0,0,0),rgba(0,0,0,0.54) 52%,#000000);
    background-image: linear-gradient(to bottom,rgba(0,0,0,0),rgba(0,0,0,0.54) 52%,#000000);
}

.news_list {
}

.news_list_item {
    padding: 24rpx 30rpx;
    border-bottom: solid 2rpx #1d2228;
    display: -webkit-box;
    display: -ms-flexbox;
    display: -webkit-flex;
    display: flex;
    -webkit-box-pack: justify;
    -ms-flex-pack: justify;
    justify-content: space-between;
    -webkit-box-align: center;
    -ms-flex-align: center;
    align-items: center;
}

.news_list_item_con {
    width: 450rpx;
    height: 140rpx;
    font-size: 32rpx;
    line-height: 1.38;
    color: #f0f1f5;
    display: -webkit-box;
    display: -ms-flexbox;
    display: -webkit-flex;
    display: flex;
    -webkit-box-orient: vertical;
    -webkit-box-direction: normal;
    flex-direction: column;
    -webkit-box-pack: justify;
    -ms-flex-pack: justify;
    justify-content: space-between;
    -webkit-box-align: start;
    -ms-flex-align: start;
    align-items: flex-start;
}

.news_list_item_con .topic {
    font-size: 24rpx;
    width: 60rpx;
    height: 32rpx;
    border-radius: 2rpx;
    background-color: #477dc1;
    padding: 4rpx 6rpx;
    margin-right: 10rpx;
}

.news_list_item_con_content {
    display: -webkit-box;
    overflow: hidden;
    text-overflow: ellipsis;
    -webkit-line-clamp: 2;
    -webkit-box-orient: vertical;
}

.news_list_item_con_resource {
    font-size: 24rpx;
    color: #676d79;
}

.news_list_item_con_resource span {
    margin-right: 10rpx;
}

.news_list_item_img {
    width: 210rpx;
    height: 140rpx;
}

.news_list_item_img image {
    width: 100%;
    height: 100%;
    background-size: contain;
}