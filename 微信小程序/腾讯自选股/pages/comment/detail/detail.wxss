.commentDetail_nav {
    position: absolute;
    display: -webkit-box;
    display: -ms-flexbox;
    display: -webkit-flex;
    display: flex;
    -ms-flex-pack: distribute;
    justify-content: space-around;
    -webkit-box-align: center;
    -ms-flex-align: center;
    align-items: center;
    background-color: #3d3d3d;
    width: 600rpx;
    height: 100rpx;
    top: 300rpx;
    left: 75rpx;
    z-index: 2;
}

.commentDetail_nav::after,.commentDetail_nav::before {
    position: absolute;
    content: '';
    top: 0;
    height: 100rpx;
    width: 100rpx;
    border-radius: 50%;
    background-color: #3d3d3d;
    z-index: -1;
}

.commentDetail_nav::before {
    left: -50rpx;
}

.commentDetail_nav::after {
    right: -50rpx;
}

.commentDetail_nav_slice {
