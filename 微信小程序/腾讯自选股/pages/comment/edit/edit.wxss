page {
    background-color: #fff;
}

.edit {
    display: relative;
    background-color: #fff;
    font-size: 30rpx;
    color: black;
}

.edit .text-area-container {
    padding: 30rpx;
}

.edit textarea {
    height: 250rpx;
    width: 690rpx;
}

.edit_operateArea {
    padding: 0 30rpx;
}

.edit_media {
    position: relative;
    left: 0;
    width: 100%;
    height: auto;
    display: -webkit-box;
    display: -ms-flexbox;
    display: -webkit-flex;
    display: flex;
    -webkit-box-orient: vertical;
    -webkit-box-direction: normal;
    flex-direction: column;
    -webkit-box-pack: end;
    -ms-flex-pack: end;
    justify-content: end;
}

.edit_preview {
    display: -webkit-box;
    display: -ms-flexbox;
    display: -webkit-flex;
    display: flex;
    -webkit-box-pack: start;
    -ms-flex-pack: start;
    justify-content: start;
    -webkit-box-align: center;
    -ms-flex-align: center;
    align-items: center;
    flex-wrap: wrap;
    padding: 20rpx 0 40rpx;
}

.edit_preview image {
    width: 118rpx;
    height: 118rpx;
    background-color: #eeeeee;
}

.edit_preview progress {
    margin-top: -17rpx;
}

.edit_preview_item {
    margin: 0 10rpx 20rpx 10rpx;
    position: relative;
}

.edit_preview_item_del {
    position: absolute;
    top: -12rpx;
    right: -8rpx;
    width: 40rpx;
    height: 40rpx;
    background: url(data:image/png;base64,) no-repeat;
    background-size: contain;
}

.edit_preview_add {
    height: 118rpx;
    width: 118rpx;
    background: url(data:image/png;base64,) no-repeat;
    background-size: 118rpx 118rpx;
}

.edit_operate {
}

.edit_operate_view {
    height: 100rpx;
    -webkit-box-pack: justify;
    -ms-flex-pack: justify;
    justify-content: space-between;
    display: -webkit-box;
    display: -ms-flexbox;
    display: -webkit-flex;
    display: flex;
    -webkit-box-align: center;
    -ms-flex-align: center;
    align-items: center;
    border-top: 1rpx solid #f3f3f3;
    padding: 0 20rpx;
}

.edit_operate_inline {
    display: inline-block;
    vertical-align: middle;
}

.edit_operate_send {
    padding: 0;
    -webkit-box-pack: center;
    -ms-flex-pack: center;
    justify-content: center;
}

.edit_operate_send_sendBtn {
    width: 100%;
    height: 90rpx;
    line-height: 90rpx;
    text-align: center;
    border-radius: 2rpx;
    background-color: #1987ff;
    color: #fff;
    margin-top: 80rpx;
    font-size: 36rpx;
}

.edit_operate_send_sendBtn.grey {
    background: #9599A1;
}

.edit_operate_text {
    padding-left: 28rpx;
    font-size: 30rpx;
}

.edit_operate_icon {
}

.edit_operate_icon_emoji {
    width: 32rpx;
    height: 32rpx;
    background: url(data:image/png;base64,) no-repeat;
    background-size: 32rpx 32rpx;
}

.edit_operate_icon_image {
    width: 32rpx;
    height: 24rpx;
    background: url(data:image/png;base64,) no-repeat;
    background-size: 32rpx 24rpx;
}

.edit_operate_icon_arrow {
    float: right;
    height: 32rpx;
    width: 21rpx;
    background: url(data:image/png;base64,) no-repeat;
    background-size: 21rpx 32rpx;
}

.swiper_container {
    width: 750rpx;
    height: 352rpx;
    background: #f7f7f7;
    position: fixed;
    bottom: 0;
    left: 0;
}

.swiper_container swiper {
    height: 352rpx;
}

.swiper_container .emoji_btn {
    width: 107rpx;
