@-webkit-keyframes rotate {
    from {
        -webkit-transform: rotate(0deg);
    }

    to {
        -webkit-transform: rotate(360deg);
    }
}

.loadEffect.rotate {
    -webkit-animation-name: rotate;
    -webkit-animation-duration: 1.45s;
    -webkit-animation-iteration-count: infinite;
    -webkit-animation-timing-function: linear;
}

.loadEffect {
    width: 16px;
    height: 16px;
    position: relative;
    margin: 0 auto;
    margin-top: 10rpx;
}

.loadEffect .v,.loadEffect .v_dark {
    display: inline-block;
    width: 4px;
    height: 4px;
    border-radius: 50%;
    background: #f7fbff;
    position: absolute;
    opacity: 0.3;
}

.loadEffect .v_dark {
    background: #000000;
}

.loadEffect .v1 {
    opacity: 0.3;
}

.loadEffect .v2 {
    opacity: 0.4;
    transform: rotate(45deg);
    transform-origin: 200% 200%;
}

.loadEffect .v3 {
    opacity: 0.5;
    transform: rotate(90deg);
    transform-origin: 200% 200%;
}

.loadEffect .v4 {
    opacity: 0.6;
    transform: rotate(135deg);
    transform-origin: 200% 200%;
}

.loadEffect .v5 {
    opacity: 0.7;
    transform: rotate(180deg);
    transform-origin: 200% 200%;
}

.loadEffect .v6 {
    opacity: 0.8;
    transform: rotate(225deg);
    transform-origin: 200% 200%;
}

.loadEffect .v7 {
    opacity: 0.9;
    transform: rotate(270deg);
    transform-origin: 200% 200%;
}

.loadEffect .v8 {
    opacity: 1;
    transform: rotate(315deg);
    transform-origin: 200% 200%;
}

.remindPanel {
    padding: 0rpx 14rpx 0 24rpx;
    color: #d7dadf;
    font-size: 26rpx;
    height: 60rpx;
    line-height: 60rpx;
    width: 690rpx;
    margin-left: 15rpx;
    background: #2e4258;
}

.remindPanel .remindtip,.remindPanel .remindtip--smaller {
    display: inline-block;
}

.remindPanel .remindtip--smaller {
    font-size: 23.5rpx;
}

.remindPanel .closeRemind {
    display: inline-block;
    vertical-align: middle;
    margin-top: 10rpx;
    background: url(data:image/png;base64,) no-repeat center;
    background-size: 20rpx 20rpx;
    width: 40rpx;
    height: 40rpx;
    float: right;
}

.remindPanel .remindset {
    float: right;
    margin-right: 10rpx;
    line-height: 60rpx;
    display: inline-block;
    font-size: 25rpx;
}

.bannerstip {
    background: #222e3c;
    height: 80rpx;
    line-height: 80rpx;
    width: 750rpx;
    width: 100%;
    text-align: center;
    color: #f7fbff;
    font-size: 30rpx;
    position: fixed;
    top: -80rpx;
    z-index: 9999;
    transition: top 0.6s;
}

.bannerstip .exclamation {
    display: inline-block;
    vertical-align: middle;
    width: 40rpx;
    height: 40rpx;
    margin-right: 10rpx;
    margin-bottom: 3rpx;
    background: url(data:image/png;base64,) no-repeat;
    background-size: contain;
}

.bannerstip.show {
    top: 0;
    transition: top 0.6s;
}