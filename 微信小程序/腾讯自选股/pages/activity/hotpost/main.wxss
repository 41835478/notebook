#hotpost-swiper .expand {
    z-index: 999;
    animation: inAnim 0.25s ease-out forwards;
}

#hotpost-swiper .shrink {
    animation: outAnim 0.25s ease-out forwards;
}

#hotpost-swiper .pageNum {
    color: #9e7e55;
    font-size: 30rpx;
    text-align: center;
    margin-top: -16rpx;
}

#hotpost-swiper .pageNum .slash {
    margin: 0 2rpx;
}

#hotpost-swiper .like-info {
    margin-top: 30rpx;
    color: #a78148;
    display: flex;
    justify-content: center;
    align-items: center;
}

#hotpost-swiper .like-info .support {
    font-size: 22rpx;
}

@-webkit-keyframes inAnim {
    0% {
        transform-oringin: center center;
        transform: scale(1,1);
    }

    100% {
        transform-oringin: center center;
        transform: scale(1.15,1.15);
    }
}

@keyframes inAnim {
    0% {
        transform-oringin: center center;
        transform: scale(1,1);
    }

    100% {
        transform-oringin: center center;
        transform: scale(1.15,1.15);
    }
}

@-webkit-keyframes outAnim {
    0% {
        transform-oringin: center center;
        transform: scale(1.15,1.15);
    }

    100% {
        transform-oringin: center center;
        transform: scale(1,1);
    }
}

@keyframes outAnim {
    0% {
        transform-oringin: center center;
        transform: scale(1.15,1.15);
    }

    100% {
        transform-oringin: center center;
        transform: scale(1,1);
    }
}

.hotpost-buttonWrapper .likeBtn {
    width: 36rpx;
    height: 30rpx;
    padding: 20rpx;
    padding-left: 0;
    background: url(data:image/png;base64,) center center no-repeat;
    background-size: 26rpx 24rpx;
}

.hotpost-buttonWrapper .unlikeBtn {
    width: 36rpx;
    height: 30rpx;
    padding: 20rpx;
    padding-left: 0;
    background: url(data:image/png;base64,) center center no-repeat;
    background-size: 24rpx 22rpx;
}

.hotpost-buttonWrapper .plus {
    position: absolute;
    margin-top: 10rpx;
    margin-left: 40rpx;
    font-size: 18rpx;
    color: #a78148;
    opacity: 0;
    width: 40rpx;
    height: 40rpx;
}

.hotpost-buttonWrapper .plus.unlikeAnim {
    animation: none;
}

.hotpost-buttonWrapper .plus.likeAnim {
    animation: moveUp 1s linear;
}

.hotpost-buttonWrapper .likeAnim {
    animation: likeAnim 0.35s linear;
}

.hotpost-buttonWrapper .unlikeAnim {
    animation: unlikeAnim 0.35s linear;
}

@-webkit-keyframes moveUp {
    0% {
        opacity: 1;
        transform: translateY(0) translateX(0) scale(1,1);
    }

    100% {
        opacity: 0;
        transform: translateY(-40rpx) translateX(10rpx) scale(1.15,1.15);
    }
}

@keyframes moveUp {
    0% {
        opacity: 1;
        transform: translateY(0) translateX(0) scale(1,1);
    }

    100% {
        opacity: 0;
        transform: translateY(-40rpx) translateX(10rpx) scale(1.15,1.15);
    }
}

@-webkit-keyframes likeAnim {
    0% {
        transform: scale(1,1);
    }

    50% {
        transform: scale(1.5,1.5);
    }

    100% {
        transform: scale(1,1);
    }
}

@keyframes likeAnim {
    0% {
        transform: scale(1,1);
    }

    50% {
        transform: scale(1.5,1.5);
    }

    100% {
        transform: scale(1,1);
    }
}

@-webkit-keyframes unlikeAnim {
    0% {
        transform: scale(1,1);
    }

    50% {
        transform: scale(1.5,1.5);
    }

    100% {
        transform: scale(1,1);
    }
}

@keyframes unlikeAnim {
    0% {
        transform: scale(1,1);
    }

    50% {
        transform: scale(1.5,1.5);
    }

    100% {
        transform: scale(1,1);
    }
}

