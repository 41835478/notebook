page {
    background: #efeff4;
    -webkit-overflow-scrolling: touch;
}

.container {
    height: auto;
    overflow: hidden;
}

.scroll-content {
    overflow: auto;
    overflow-x: hidden;
    -webkit-overflow-scrolling: touch;
}

.ico-triangle-drop,.ico-triangle-rise {
    border-right: solid 10rpx transparent;
    border-left: solid 10rpx transparent;
}

.current-day .li-cont:before,.information-subject .subject-title:before {
    content: '';
    left: 0;
}

.focus-news .hd-banner-title,.information-detail .related-stock-list .row-a {
    overflow: hidden;
    text-overflow: ellipsis;
    white-space: nowrap;
}

.current-day .li-date,.focus-news .news-title,.information-detail .item-title label,.subject-abstract label {
    vertical-align: middle;
}

.shrink-wrap-nav {
    display: -webkit-box;
    display: -ms-flexbox;
    display: -webkit-flex;
    display: flex;
    -webkit-box-pack: center;
    -ms-flex-pack: center;
    justify-content: center;
}

.wrap-nav {
    background-color: #fff;
}

.wrap-nav .lay-nav {
    width: 100%;
}

.wrap-nav .lay-nav .nav {
    line-height: 80rpx;
    font-size: 30rpx;
    display: inline-block;
    padding: 0 36rpx;
    margin: 0 30rpx;
    color: #353535;
}

.wrap-nav .lay-nav .nav-cur {
    color: #007aff;
    border-bottom: 2px solid #007aff;
}

.fixed-nav {
    position: fixed;
    top: 0;
    z-index: 2;
    width: 100%;
}

.information-index {
    background-color: #efeff4;
}

.current-day,.focus-news,.my-choose {
    background-color: #fff;
}

.container {
    min-height: 100%!important;
}

.information-index .scroll-content {
    height: calc(100% - 96rpx);
}

.information-index .icon-drop,.information-index .icon-rise {
    font-size: 20rpx;
}

.li-wrap {
    position: relative;
    padding: 0 30rpx;
}

.li-cont-tit {
    font-size: 30rpx;
    color: #000;
}

.fixed-bar-date {
    top: 0;
    z-index: 2;
    width: 100%;
    height: 64rpx;
    line-height: 64rpx;
    padding: 0 30rpx;
    font-size: 28rpx;
    color: #888;
    background-color: #efeff4;
}

.read .li-cont-tit,.read .news-title {
    color: #888;
}

.pull-bottom-wrapper {
    text-align: center;
    margin-top: 40rpx;
}

.inner-tips {
    padding: 16rpx 0;
    font-size: 24rpx;
    color: #b2b2b2;
}

.related-stock {
    height: 40rpx;
    line-height: 40rpx;
    padding: 2rpx 12rpx;
    font-size: 24rpx;
    border-radius: 8rpx;
}

.related-stock .related-stock-name {
    padding-right: 14rpx;
}

.red .related-stock {
    background-color: rgba(230,53,53,.05);
}

.green .related-stock {
    background-color: rgba(45,185,85,.05);
    color: #2db955;
}

.gray .related-stock,.stop .related-stock {
    background-color: rgba(136,136,136,.05);
    color: #888;
}

.ico-triangle-drop,.ico-triangle-rise {
    display: inline-block;
    width: 0;
    height: 0;
    border-top: transparent;
    margin-left: 10rpx;
}

.ico-triangle-rise {
    border-bottom: solid 16rpx #e63535;
}

.ico-triangle-drop {
    border-top: solid 16rpx #2db955;
}

.subject-abstract {
    margin: 32rpx 30rpx 0;
    padding: 30rpx 15rpx;
    background-color: #f9f9f9;
    font-size: 28rpx;
    color: #888;
    line-height: 1.64;
    border-radius: 5rpx;
}

.blue-span,.focus-news .news-type {
    margin-right: 10rpx;
    border-radius: 4rpx;
}

.blue-span {
    padding: 2rpx 8rpx;
    color: #fff;
    font-size: 20rpx;
    background-color: #007aff;
}

.focus-news .hd-banner,.focus-news .li-img-box,.information-subject .li-img-box {
    background-color: #efeff4;
    background-image: url('data:image/png;base64,');
    background-repeat: no-repeat;
}

.news-related {
    font-size: 24rpx;
}

.news-resource,.news-time {
    position: absolute;
    bottom: 0;
    color: #b2b2b2;
}

.news-resource {
    right: 0;
}

.news-time {
    left: 0;
}

.focus-news .li-img-box,.information-subject .li-img-box {
    margin-left: 20rpx;
    background-position: 50% 50%;
    background-size: 134rpx auto;
}

.focus-news .hd-banner {
    position: relative;
    background-position: 50% 40%;
    background-size: 206rpx auto;
}

.focus-news .hd-banner-img {
    display: block;
    width: 100%;
    height: 330rpx;
    background-size: cover;
}

.focus-news .hd-banner-title {
    position: absolute;
    bottom: 0;
    width: 100%;
    height: 80rpx;
    line-height: 80rpx;
    padding: 0 30rpx;
    box-sizing: border-box;
    font-size: 30rpx;
    color: #fff;
    background: rgba(0,0,0,.5);
}

.focus-news .news-list {
    color: #000;
}

.focus-news .li-cont {
    position: relative;
}

.focus-news .li-wrap {
    position: relative;
    padding: 20rpx 30rpx;
    border-bottom: 1rpx solid #e5e5e5;
}

.focus-news .li-wrap:first-child {
    padding-top: 30rpx;
}

.focus-news .li-img {
    display: block;
    width: 190rpx;
    height: 130rpx;
}

.focus-news .news-type {
    padding: 4rpx 10rpx;
    line-height: 1.3;
    font-size: 20rpx;
    color: #fff;
    background: #007aff;
}

.focus-news .news-title {
    height: 108rpx;
    font-size: 32rpx;
    line-height: 1.3;
}

.focus-news .related-stock-name {
    padding-right: 10rpx;
}

.current-day ul {
    padding-top: 30rpx;
}

.current-day .li-wrap {
    line-height: 1.5;
    padding: 0;
}

.current-day .li-wrap:after {
    border-bottom: none;
}

.current-day .li-cont-boldtit {
    padding-bottom: 6rpx;
    font-size: 30rpx;
    font-weight: 500;
    line-height: 1.4;
    color: #000;
}

.current-day .li-cont-tit {
    font-size: 28rpx;
    color: #888;
    line-height: 1.64;
}

.current-day .li-date {
    padding: 0 38rpx 6rpx 0;
    font-size: 28rpx;
    color: #888;
}

.current-day .li-date-circle {
    display: inline-block;
    width: 14rpx;
    height: 14rpx;
    margin: 0 30rpx;
    background-color: #b2b2b2;
    border-radius: 7rpx;
}

.current-day .li-cont:before {
    position: absolute;
    top: 0;
    width: 1px;
    height: 200%;
    transform: scale(.5);
    transform-origin: 0 0;
    background-color: #b2b2b2;
}

.current-day .li-cont {
    position: relative;
    margin-left: 36rpx;
    padding: 0 36rpx 36rpx 46rpx;
}

.current-day .related-stock-item .stock-item-box {
    padding: 10rpx 0;
}

.current-day .related-stock-item:last-child {
    padding-bottom: 0;
}

.current-day .related-stock {
    margin-top: 16rpx;
}

.my-choose .choose-list {
    margin-top: 12rpx;
}

.my-choose .li-cont {
    position: relative;
}

.my-choose .li-wrap {
    padding: 20rpx 30rpx;
    line-height: 1.4;
    background-color: #fff;
    border-bottom: 1rpx solid #e5e5e5;
}

.my-choose .related-stock-item .stock-item-box {
    padding: 20rpx 0;
}

.my-choose .related-stock-item {
    padding-right: 40rpx;
    font-size: 24rpx;
}

.my-choose .choose-li-date {
    padding: 20rpx 0;
    font-size: 24rpx;
    color: #b2b2b2;
    text-align: right;
}

.my-choose .li-img-box {
    width: 160rpx;
    height: 120rpx;
    line-height: 1.6;
    margin-right: 38rpx;
    color: #fff;
    text-align: center;
}

.my-choose .long.li-img-box {
    line-height: 1.2;
}

.my-choose .raise-red.li-img-box {
    background-image: linear-gradient(127deg,#f14668,#da2d49);
}

.my-choose .decline-green.li-img-box {
    background-image: linear-gradient(307deg,#209b62,#2db955);
}

.my-choose .stop.li-img-box {
    background-color: #b2b2b2;
}

.my-choose .li-img-box p {
    color: #fff;
    font-size: 26rpx;
    text-align: center;
}

.my-choose .li-img-box .img-box-name {
    padding-top: 24rpx;
    font-size: 26rpx;
}

.my-choose .li-img-box .img-box-value {
    font-size: 24rpx;
}

.my-choose .li-cont-tit {
    width: 100%;
    display: -webkit-box;
    -webkit-line-clamp: 2;
    -webkit-box-orient: vertical;
    overflow: hidden;
}

.current-list {
    padding-top: 30rpx;
}