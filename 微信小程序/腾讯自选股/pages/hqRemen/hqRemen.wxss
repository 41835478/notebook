}

.bannerstip {
    background: #222e3c;
    line-height: 85rpx;
    width: 750rpx;
    width: 100%;
    text-align: center;
    color: #f7fbff;
    font-size: 30rpx;
    position: fixed;
    top: -85rpx;
    height: 85rpx;
    overflow: hidden;
    transition: top 0.6s;
    z-index: 20;
}

.bannerstip .exclamation {
    display: inline-block;
    vertical-align: middle;
    width: 40rpx;
    height: 40rpx;
    margin-right: 10rpx;
    margin-bottom: 3rpx;
    background: url(data:image/png;base64,) no-repeat;
    background-size: contain;
}

.bannerstip.show {
    top: 0;
    transition: top 0.6s;
}

