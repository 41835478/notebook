.newsPanel .newsPanel__tr:last-of-type {
    border-bottom: none;
}

.newsPanel .newsPanel__tr.withborder {
    border-bottom: 1rpx solid #1e2128;
}

.newsPanel .newsPanel__tr.other-navigator-hover {
    background-color: #1f2630;
}

.newsPanel {
    padding-bottom: 70rpx;
    background-color: #101419;
    margin-top: 10rpx;
}

.newsPanel__iphonex {
    padding-bottom: 118rpx;
}

.newsPanel__title {
    font-size: 26rpx;
    color: #899198;
    margin-left: 30rpx;
    text-indent: 12rpx;
    display: -webkit-box;
    display: -ms-flexbox;
    display: -webkit-flex;
    display: flex;
    -webkit-box-align: center;
    -ms-flex-align: center;
    align-items: center;
    margin-top: 22rpx;
}

.newsPanel__title__line {
    width: 1px;
    height: 25rpx;
    border-left: 1rpx solid #3d76b8;
}

.newsPanel__tr {
    border-bottom: 1rpx solid #1e2128;
    padding: 24rpx 30rpx;
}

.newsPanel__tr__item {
    font-size: 32rpx;
    color: #f7fbff;
    margin-bottom: 10rpx;
    max-height: 92rpx;
    line-height: 46rpx;
    overflow: hidden;
}

.newsPanel__tr__item__source {
    font-size: 24rpx;
    color: #878a8f;
}

.newsPanel__tr__item__date {
    font-size: 24rpx;
    color: #878a8f;
}

.newsPanel__tr__item2 {
    display: -webkit-box;
    display: -ms-flexbox;
    display: -webkit-flex;
    display: flex;
    -ms-flex-pack: justify;
    justify-content: space-between;
    box-pack: justify;
}

page {
    font-family: -apple-system-font,'Helvetica Neue',Helvetica,Roboto,sans-serif;
    background-color: #101419;
    user-select: none;
    color: #f7fbff;
    height: 100%;
}

.other-navigator-hover {
    background-color: #1f2630;
}

.hktipPanel {
    color: #656b6e;
    font-size: 28rpx;
    text-align: center;
    height: 100rpx;
    line-height: 100rpx;
}

.hktip {
    display: inline-block;
    margin-left: 10rpx;
    background: url(data:image/png;base64,) no-repeat;
    background-size: contain;
    width: 30rpx;
    height: 30rpx;
    vertical-align: text-bottom;
}

.debugButton {
    width: 100rpx;
    height: 100rpx;
    background-image: url(data:image/png;base64,);
    background-color: #FFF;
    background-size: cover;
    border-radius: 20rpx;
    border: 1rpx solid red;
    text-align: center;
    position: fixed;
    bottom: 90rpx;
    right: 50rpx;
}

.debugButtonHover {
    background-color: red;
}

.flex {
    display: -webkit-box;
    display: -ms-flexbox;
    display: -webkit-flex;
    display: flex;
}

.flex-1 {
    -webkit-box-flex: 1;
    flex: 1;
}

.align-c {
    text-align: center!important;
}

.red,.rise {
    color: #e73146!important;
}

.white-bg {
    background: #fff;
}

.hide {
    display: none!important;
}

.flex-pack-justify {
    -webkit-box-pack: justify;
    -ms-flex-pack: justify;
    justify-content: space-between;
}

.flex-align-center {
    -webkit-box-align: center;
    -ms-flex-align: center;
    align-items: center;
}

.flex-pack-center {
    -webkit-box-pack: center;
    -ms-flex-pack: center;
    justify-content: center;
}