.m-play {
    position: relative;
    width: 100%;
    height: 422rpx;
}

.video {
    width: 100%;
    height: 422rpx;
}

.m-videopic {
    position: absolute;
    top: 0;
    left: 0;
    width: 750rpx;
    height: 422rpx;
    background-color: rgb(153,153,153);
}

.m-videopic_pic {
    width: 100%;
    height: 100%;
}

.m-videopic_time {
    font-size: 20rpx;
    position: absolute;
    bottom: 0;
    right: 0;
    z-index: 5;
    padding: 10rpx;
    background-color: rgba(0,0,0,0.7);
    color: #fff;
}

.m-advertisement {
    width: 100%;
    font-size: 28rpx;
    padding: 46rpx 30rpx 46rpx 30rpx;
    box-sizing: border-box;
    border: 1rpx solid rgb(230,230,230);
}

.c-nummber {
    color: rgb(11,190,6);
}