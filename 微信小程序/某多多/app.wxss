page,view,text,input,image,picker,icon,label,textarea,video,scroll-view {
    box-sizing: border-box;
    font-family: -apple-system-font,"Helvetica Neue",Helvetica,sans-serif;
}

image {
    border: 0 none;
    vertical-align: top;
}

::-webkit-scrollbar {
    width: 0;
    height: 0;
    color: transparent;
}

.view-show {
    opacity: 1.0!important;
}

page {
    height: 100vh;
    width: 100vw;
    overflow-x: hidden;
    color: #666666;
    background-color: #f4f4f4;
}

.page-top-line {
    width: 200%;
    height: 1px;
    position: fixed;
    z-index: 999;
    left: 0;
    top: 0;
    transform: scale(0.5,0.5);
    transform-origin: 0 0;
    background-color: #d2d2d2;
}

.container {
    box-sizing: border-box;
    width: 100%;
    overflow-x: hidden;
}

.pdd-loading {
    position: fixed;
    z-index: 99999;
    width: 66px;
    height: 66px;
    top: 35%;
    left: 50%;
    margin-left: -33px;
}

.pdd-loading button {
    width: 66px;
    height: 66px;
    background-color: rgba(0,0,0,0.7);
}

.pdd-loading button::before {
    position: absolute;
    top: 20px;
    left: 20px;
    width: 26px!important;
    height: 26px!important;
}

.navigator-hover {
    background-color: #fff!important;
    opacity: 1.0;
}

.wifi-error {
    position: fixed;
    top: 0;
    bottom: 0;
    padding-top: 300rpx;
    overflow: hidden;
    width: 100%;
    text-align: center;
    z-index: 11;
    background-color: #fff;
}

.wifi-error-img {
    margin: auto;
    width: 30%;
    margin-top: 20%;
    padding-top: 30%;
    background: url("http://pinduoduoimg.yangkeduo.com/wxapp/net_error.png") no-repeat;
    background-size: 100% auto;
}

.wifi-error-text {
    color: #9d9d9d;
    font-size: 14px;
}

.wifi-error-btn {
    background-color: #ff001f;
    color: #fff;
    font-size: 16px;
    width: 40%;
    height: 40px;
    line-height: 40px;
    margin: auto;
    margin-top: 25px;
    border-radius: 4px;
}

.pdd-default-image {
    width: 100%;
    background-position: center;
    background-image: url("data:image/png;base64,");
}

.pdd-modal-mask {
    width: 100%;
    height: 100%;
    position: fixed;
    top: 0;
    left: 0;
    background: rgba(0,0,0,0.8);
    overflow: hidden;
    z-index: 10003;
    color: #fff;
    animation: fadeIn 0.3s ease normal forwards;
}

.pdd-modal-mask .modal-ctn {
    position: relative;
    width: 100%;
    height: 100%;
    overflow: hidden;
    color: #fff;
    animation: superfadeIn 0.3s ease normal forwards;
}

@-webkit-keyframes superfadeIn {
    from {
        opacity: 0;
        transform: scale(0);
    }

    to {
        opacity: 1;
        transform: scale(1);
    }
}

@keyframes superfadeIn {
    from {
        opacity: 0;
        transform: scale(0);
    }

    to {
        opacity: 1;
        transform: scale(1);
    }
}