}

.detail .deal_info .content_info {
    float: right;
}

.detail .deal_info {
    border-top: none;
    margin: 0 20rpx 0 16rpx;
}

.detail .deal_info:after {
    border-top: none;
}

.detail .deal_info .item {
    display: inline-block;
    margin: 0 0 0 20rpx;
}

.detail .deal_info .dot {
    display: inline-block;
    width: 6rpx;
