.detail view {
    color: #333;
    height: 90rpx;
    line-height: 90rpx;
    font-size: 26rpx;
    margin: 0 20rpx;
    position: relative;
}

.detail view::after {
    content: ' ';
    display: block;
    position: absolute;
    top: 0;
    right: -100%;
    bottom: -100%;
    left: 0;
    transform-origin: 0 0;
    pointer-events: none;
    transform: scale(0.5);
    width: 200%;
    box-sizing: border-box;
    border-top: 1px solid #e0e0e0;
}

.detail .deal_info view::after {
    border: none;
}

.detail .title {
    float: left;
    width: 190rpx;
}

.detail .content {
    position: relative;
    float: right;
    width: 520rpx;
