}

.detail .deal_info .title,.detail .deal_info .content_info {
    color: #333;
    height: 90rpx;
    line-height: 90rpx;
    font-size: 26rpx;
    display: inline-block;
    margin: 0;
}

.detail .deal_info .title {
    width: 150rpx;
