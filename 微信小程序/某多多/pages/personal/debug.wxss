.navigation-url-window {
    position: fixed;
    top: 20%;
    left: 5%;
    width: 90%;
    background-color: #fff;
    border-radius: 10rpx;
    padding: 10rpx;
    z-index: 1000;
    font-size: 28rpx;
}

.navigation-url-window input {
    padding: 10rpx;
    margin: 10rpx 0;
    color: #000;
    font-size: 30rpx;
    border-bottom: 1px solid #09bb07;
}

.navigation-url-window button,.debug-confirm button {
    width: 40%;
    margin: 10rpx;
    float: left;
}

.debug-mask {
    position: fixed;
    width: 100%;
    height: 100%;
    top: 0;
    left: 0;
    z-index: 800;
    background-color: #000;
    opacity: 0.7;
}

.debug-confirm {
    position: fixed;
    top: 40%;
    left: 5%;
    width: 90%;
    background-color: #fff;
    border-radius: 10rpx;
    padding: 10rpx;
    z-index: 1000;
    font-size: 28rpx;
    line-height: 30rpx;
}

.debug-confirm .title {
    text-align: center;
    margin-bottom: 20rpx;
    width: 100%;
    font-size: 30rpx;
    font-weight: bold;
}

.debug-confirm .content {
    padding: 10rpx;
}