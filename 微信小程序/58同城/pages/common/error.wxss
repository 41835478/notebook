.wrap-body {
    background-color: #f8f7f7;
    width: 100%;
    height: 100%;
    text-align: center;
    display: -webkit-box;
    display: -webkit-flex;
    display: flex;
    -webkit-box-orient: vertical;
    -webkit-box-direction: normal;
    flex-direction: column;
    -webkit-box-align: center;
    align-items: center;
    -webkit-box-pack: center;
    justify-content: center;
    overflow: hidden;
}

.no_result_icon {
    width: 300rpx;
    height: 300rpx;
    margin: 0 auto 40rpx;
    background: url(https://img.58cdn.com.cn/weixin/img/wechat-app/no-result.png) no-repeat 0 0;
    background-size: 100% 100%;
}

.tips {
    font-size: 28rpx;
    color: #999999;
}

.btn_to_service_seek {
    margin-top: 60rpx;
}