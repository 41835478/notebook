.search-inp {
    display: -webkit-box;
    display: -webkit-flex;
    display: flex;
    -webkit-box-orient: vertical;
    -webkit-box-direction: normal;
    flex-direction: column;
    background: #fff;
    padding: 15rpx 30rpx 10rpx;
    color: #AAAAAA;
    font-size: 24rpx;
    box-sizing: border-box;
}

.search-cont {
    -webkit-box-flex: 1;
    flex: 1;
    display: -webkit-box;
    display: -webkit-flex;
    display: flex;
    height: 60rpx;
    background-color: #F5F5F5;
    -webkit-box-align: center;
    align-items: center;
    -webkit-box-pack: center;
    justify-content: center;
    border-radius: 4rpx;
}

.search-logo {
    margin-right: 12rpx;
}