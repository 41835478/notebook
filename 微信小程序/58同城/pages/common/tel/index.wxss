.tel-alert {
    position: fixed;
    top: 0;
    bottom: 0;
    left: 0;
    right: 0;
    background: rgba(0,0,0,0.5);
    z-index: 999;
}

.tel-alert-content {
    position: absolute;
    top: 33%;
    left: 0;
    right: 0;
    width: 540rpx;
    margin: auto;
    background: #fff;
    font-size: 28rpx;
    z-index: 9999;
    padding: 60rpx 20rpx 40rpx 20rpx;
    color: #666666;
    text-align: center;
    border-radius: 4rpx;
}

.tel-alert-close {
    width: 68rpx;
    height: 68rpx;
    position: absolute;
    left: 0;
    right: 0;
    margin: auto;
    bottom: -100rpx;
    background-image: url(data:image/png;base64,);
    background-size: 100% auto;
}

.alert-time-tip {
    margin: 0 6rpx;
}

.tel-alert-number {
    font-size: 36rpx;
    color: #FF552E;
    padding: 20rpx 0 6rpx 0;
}

.alert-tel-btn-wrap {
    font-size: 30rpx;
    color: #FFFFFF;
    height: 80rpx;
    line-height: 80rpx;
    width: 320rpx;
    background: #FF552E;
    margin: 30rpx auto auto auto;
}

.alert-tel-btn {
    position: relative;
    padding-left: 40rpx;
}

.alert-tel-btn:before {
    position: absolute;
    left: 0;
    top: 0;
    bottom: 0;
    width: 34rpx;
    height: 34rpx;
    content: '';
    margin: auto;
    background-image: url(data:image/png;base64,);
    background-size: 100% 100%;
}