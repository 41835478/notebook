@import "../../__wuBaseWxss__/5.wxss";

page {
    overflow: hidden;
}

.container-main {
    height: 100vh;
    background-color: #f6f6f6;
}

.container-main-logined {
    background-color: #fff;
}

.avatar-container {
    border-bottom: 20rpx solid #f6f6f6;
    background-color: #fff;
}

.avatar-info {
    display: -webkit-box;
    display: -webkit-flex;
    display: flex;
    -webkit-box-pack: center;
    justify-content: center;
    -webkit-box-align: center;
    align-items: center;
    height: 120rpx;
    text-align: center;
    font-size: 32rpx;
    color: #333;
}

.avatar-wrapper {
    height: 220rpx;
    display: -webkit-box;
    display: -webkit-flex;
    display: flex;
    -webkit-box-pack: center;
    justify-content: center;
    -webkit-box-orient: horizontal;
    -webkit-box-direction: normal;
    flex-direction: row;
    box-sizing: border-box;
    margin-bottom: 40rpx;
}

.avatar-bg {
    position: absolute;
    top: 0;
    left: 0;
    height: 220rpx;
    width: 100%;
    background-size: cover;
    background-position: center;
    background-repeat: no-repeat;
    overflow: hidden;
}

.avatar-bg-default {
    background-image: url("https://img.58cdn.com.cn/weixin/img/wechat-app/avatar-bg@2x.png");
}

.blur {
    filter: blur(10px);
}

.avatar-border {
    position: relative;
    top: 60rpx;
    width: 200rpx;
    height: 200rpx;
    border-radius: 50%;
    -webkit-box-flex: 0;
    flex: 0 0 200rpx;
    box-shadow: 0 10rpx 5rpx #c3c6d2;
    border: 2rpx solid #fff;
    display: -webkit-box;
    display: -webkit-flex;
    display: flex;
    -webkit-box-pack: center;
    justify-content: center;
    -webkit-box-align: center;
    align-items: center;
}

.avatar {
    border-radius: 50% 50%;
    width: 100%;
    height: 100%;
    background-image: url("https://img.58cdn.com.cn/weixin/img/wechat-app/avatar-default.png");
    background-size: cover;
}

.title-container {
    height: 100rpx;
    border-bottom: 1rpx solid #f6f6f6;
}

.title {
    margin-left: 31rpx;
    font-size: 38rpx;
    line-height: 100rpx;
}

.listWrapper {
    background-color: #f6f6f6;
}

.list {
    background-color: #fff;
}

.list-item {
    display: -webkit-box;
    display: -webkit-flex;
    display: flex;
    -webkit-box-align: center;
    align-items: center;
    padding: 34rpx 0;
    border-bottom: 1rpx solid #EEEEEE;
    margin-left: 40rpx;
    padding-right: 40rpx;
}

.list-item:last-child {
    border-bottom: 0;
}

.list-item-icon {
    width: 35rpx;
    height: 32rpx;
}

.list-item-more {
    width: 16rpx;
    height: 27rpx;
}

.icon-my_resume {
    background: url(data:image/png;base64,) no-repeat;
    background-size: 100% 100%;
}

.icon-my_apply {
    background: url(data:image/png;base64,) no-repeat;
    background-size: 100% 100%;
}

.icon-my_subscription {
    background: url(data:image/png;base64,) no-repeat;
    background-size: 100% 100%;
}

.icon-my_store {
    background: url(data:image/png;base64,) no-repeat;
    background-size: 100% 100%;
}

.icon-my_im {
    background: url(data:image/png;base64,) no-repeat;
    background-size: 100% 100%;
}

.icon-more {
    background: url(data:image/png;base64,) no-repeat;
    background-size: 100% 100%;
}

.list-item-text {
    -webkit-box-flex: 1;
    flex: 1;
    margin-left: 39rpx;
    font-size: 32rpx;
    line-height: 40rpx;
    color: #333;
}

.btn-logout {
    text-align: center;
    color: #333;
    position: fixed;
    bottom: 0;
    left: 0;
    right: 0;
    height: 100rpx;
    line-height: 100rpx;
    background-color: #fff;
    font-size: 32rpx;
    border-top: 20rpx solid #f6f6f6;
    border-bottom: 28rpx solid #f6f6f6;
}