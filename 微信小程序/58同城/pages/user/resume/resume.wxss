@import "../../../__wuBaseWxss__/5.wxss";

.container-tab_top {
    height: 80rpx;
    display: -webkit-box;
    display: -webkit-flex;
    display: flex;
    -webkit-box-orient: horizontal;
    -webkit-box-direction: normal;
    flex-direction: row;
    -webkit-box-align: center;
    align-items: center;
    font-size: 30rpx;
    border-bottom: 1rpx solid #d6d6db;
}

.container-empty-icon {
    background-image: url(data:image/png;base64,);
}

.tab_top-item {
    display: -webkit-box;
    display: -webkit-flex;
    display: flex;
    -webkit-box-align: center;
    align-items: center;
    -webkit-box-pack: center;
    justify-content: center;
    height: 100%;
    -webkit-box-flex: 0;
    flex: 0 0 33%;
    text-align: center;
}

.tab_top-item_on {
    border-bottom: 5rpx solid #ff552e;
}

.container-list {
    -webkit-box-flex: 1;
    flex: 1;
    overflow: hidden;
    height: 100%;
}

.container-list-item {
    padding: 30rpx;
    margin-bottom: 10rpx;
    border-bottom: 15rpx solid #eeeeee;
}

.list-item-title {
    display: -webkit-box;
    display: -webkit-flex;
    display: flex;
    -webkit-box-align: center;
    align-items: center;
}

.list-item-content {
    height: 50rpx;
    font-size: 26rpx;
    display: -webkit-box;
    display: -webkit-flex;
    display: flex;
    -webkit-box-align: center;
    align-items: center;
    color: #888888;
}

.item-content-left {
    -webkit-box-align: center;
    align-items: center;
    display: -webkit-box;
    display: -webkit-flex;
    display: flex;
    -webkit-box-flex: 1;
    flex: 1;
}

.label-times {
    margin-left: 10rpx;
}

.list-item-title-left {
    text-align: left;
    font-size: 34rpx;
    line-height: 60rpx;
}

.list-item-title-right {
    text-align: right;
    -webkit-box-flex: 1;
    flex: 1;
    font-size: 26rpx;
    color: #888888;
}

.list-item-foot {
    color: #888888;
    height: 40rpx;
    font-size: 26rpx;
}

.resume-type {
    line-height: 40rpx;
    text-align: right;
}