@import "../../../__wuBaseWxss__/5.wxss";

.li-info {
    overflow: hidden;
    padding: 36rpx 30rpx 34rpx 0;
    margin-left: 30rpx;
    border-bottom: 1rpx solid #F3F3F3;
}

.li-body {
    display: -webkit-box;
    display: -webkit-flex;
    display: flex;
    -webkit-box-orient: vertical;
    -webkit-box-direction: normal;
    flex-direction: column;
}

.li-head {
    margin-bottom: 12rpx;
}

.piclogo {
    font-family: 'PingFangSC-Regular,sans-serif';
    display: inline-block;
    position: relative;
    top: -2rpx;
    width: 38rpx;
    height: 38rpx;
    line-height: 38rpx;
    font-size: 28rpx;
    padding: 0;
    border-radius: 6rpx;
    margin-right: 10rpx;
    color: #09bb07;
    border: 1rpx solid #09bb07;
    box-sizing: border-box;
    text-align: center;
}

.title {
    font-size: 34rpx;
}

.intro {
    display: -webkit-box;
    display: -webkit-flex;
    display: flex;
    padding-left: 20rpx;
}

.desc {
    -webkit-box-flex: 1;
    flex: 1;
    display: -webkit-box;
    display: -webkit-flex;
    display: flex;
    line-height: 37rpx;
    -webkit-box-orient: vertical;
    -webkit-box-direction: normal;
    flex-direction: column;
    -webkit-box-pack: justify;
    justify-content: space-between;
    overflow: hidden;
}

.quanzhan-desc {
    -webkit-box-flex: 1;
    flex: 1;
    display: -webkit-box;
    display: -webkit-flex;
    display: flex;
    color: #a9a9a9;
    font-size: 26rpx;
    line-height: 37rpx;
    -webkit-box-align: center;
    align-items: center;
    overflow: hidden;
}

.quanzhan-info {
    margin-right: 10rpx;
}

.btn {
    margin-top: 5rpx;
    width: 136rpx;
    -webkit-box-flex: 0;
    flex: 0 0 136rpx;
    align-self: center;
}

.addr {
    margin-bottom: 5rpx;
}

.addr text {
    font-size: 26rpx;
    margin-right: 26rpx;
    color: #888888;
}

.price {
    font-size: 30rpx;
    color: #f76260;
}

.list-third-line {
    font-size: 24rpx;
    color: #B2B2B2;
    overflow: hidden;
    text-overflow: ellipsis;
    white-space: nowrap;
}

.area {
    margin-right: 26rpx;
}

.price-unit {
    font-size: 20rpx;
    color: #f76260;
}

.no-unit {
    margin-right: 26rpx;
}

.date-km {
    color: #B2B2B2;
    font-size: 24rpx;
    margin-right: 10rpx;
}

.price-date-km {
    overflow: hidden;
    white-space: nowrap;
    text-overflow: ellipsis;
}

.delete-btn {
    position: relative;
    font-size: 26rpx;
    height: 60rpx;
    padding-left: 20rpx;
    padding-right: 20rpx;
    text-align: right;
    line-height: 60rpx;
    color: #ff552e;
    border-radius: 6rpx;
    background-color: #fff;
}

.delete-btn::before {
    content: '';
    position: absolute;
    top: 12rpx;
    left: 50rpx;
    width: 32rpx;
    height: 32rpx;
    background: url(data:image/png;base64,) no-repeat 0 0;
    background-size: 100% 100%;
    opacity: 0.3;
}

.fang_leixing {
    margin-left: 20rpx;
}

.loading-more {
    position: relative;
    text-align: center;
    font-size: 26rpx;
    height: 70rpx;
    line-height: 70rpx;
}

.loading-icon {
    position: relative;
    top: 10rpx;
    width: 40rpx;
    height: 40rpx;
    margin-right: 20rpx;
    border-radius: 100%;
    background-image: url("https://img.58cdn.com.cn/weixin/img/wechat-app/loading.gif");
    background-position: 0 0;
    background-size: 100% 100%;
}