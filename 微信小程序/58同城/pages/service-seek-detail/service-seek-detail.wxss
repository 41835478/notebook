@import "../../__wuBaseWxss__/5.wxss";

@import "template/add-label.wxss";

@import "template/vertify-info.wxss";

.register-wrap.hide-scroll {
    height: 100vh;
    overflow: hidden;
}

.register-from {
    margin: 24rpx;
    font-size: 14px;
}

.sub-post {
    position: relative;
    height: 98rpx;
    padding: 0 32rpx 0 24rpx;
    margin-bottom: 20rpx;
    display: -webkit-box;
    display: -webkit-flex;
    display: flex;
    -webkit-box-align: center;
    align-items: center;
    border: 1rpx solid #D2D2D2;
}

.select-price {
    background-color: #fff;
}

.sub-post.no-border-bottom {
    margin-bottom: 0;
    border-bottom: none;
    box-sizing: border-box;
}

.label {
    color: #727472;
    width: 63px;
    text-align: justify;
    display: inline-block;
    padding-right: 20rpx;
}

.price-unit {
    display: inline-block;
    width: 70rpx;
    color: #727472;
    margin-left: 10rpx;
}

.space1 {
    border-left: 54rpx solid transparent;
}

.space2 {
    border-left: 16rpx solid transparent;
}

.select-post {
    position: relative;
    font-size: 14px;
    color: #333;
    margin-left: 20rpx;
    cursor: pointer;
    background-color: #fff;
}

.select-icon {
    margin: 0;
    padding: 0;
    position: absolute;
    display: block;
    top: 50%;
    right: 26rpx;
    width: 20rpx;
    height: 26rpx;
    margin-top: -12rpx;
    background: url(data:image/png;base64,) no-repeat;
    background-size: 100% auto;
}

.title {
    font-size: 12px;
    color: #333;
    padding: 20rpx 30rpx;
    line-height: 12px;
    background-color: #f3f3f3;
}

.register-input {
    width: 100%;
    -webkit-box-flex: 1;
    flex: 1;
    height: 68rpx;
    line-height: 68rpx;
    background-color: #fff;
    font-size: 14px;
    color: #333;
    margin-left: 20rpx;
    box-sizing: border-box;
    border: none;
}

.register-input:focus {
    border: 1rpx solid #e0aa40;
}

.submit-wrap {
    text-align: center;
    padding: 74rpx 0;
}

.register-submit {
    display: block;
    font-size: 16px;
    width: 280rpx;
    color: #fff;
    height: 86rpx;
    background-color: #53BCDE;
    line-height: 88rpx;
    text-align: center;
}

.sub-select {
    background-color: #6f747a;
}

.data-city-list,.data-list {
    position: absolute;
    display: none;
    left: 0;
    right: 0;
    top: 0;
    bottom: 0;
    margin: auto;
    background-color: #fff;
    list-style: none;
    overflow: scroll;
}

.head-back {
    width: 100%;
    height: 88rpx;
    line-height: 88rpx;
    text-align: center;
    font-size: 14px;
    color: #555;
    border-bottom: 1rpx solid #f3f3f3;
    background-color: #f6f6f6;
}

.head-title {
    position: fixed;
    height: 88rpx;
    line-height: 88rpx;
    text-align: center;
    width: 100%;
    top: 0;
}

.back {
    position: absolute;
    top: 0;
    left: 16rpx;
    width: 100rpx;
    height: 88rpx;
    display: block;
    background: url(data:image/png;base64,) left center no-repeat;
    background-size: 34rpx auto;
}

.data-item {
    display: block;
    line-height: 88rpx;
    height: 88rpx;
    font-size: 14px;
    color: #555;
    border-bottom: 1rpx solid #f3f3f3;
    padding-left: 24rpx;
}

.data-city-list {
    display: none;
}

.list-block {
    height: 100vh;
}

.list-title {
    position: relative;
    height: 88rpx;
    background-color: #f6f6f6;
    color: #555;
    line-height: 88rpx;
    text-align: center;
    font-size: 14px;
    border-bottom: 1rpx solid #f3f3f3;
}

.list-title-icon {
    position: absolute;
    top: 0;
    left: 16rpx;
    width: 100rpx;
    height: 88rpx;
    display: block;
    background: url(data:image/png;base64,) left center no-repeat;
    background-size: 17px auto;
}

.item {
    line-height: 88rpx;
    height: 88rpx;
    font-size: 14px;
    color: #555;
    border-bottom: 1rpx solid #f3f3f3;
    padding-left: 12px;
}

.sub-area {
    display: -webkit-box;
    display: -webkit-flex;
    display: flex;
    -webkit-box-orient: vertical;
    -webkit-box-direction: normal;
    flex-direction: column;
    -webkit-box-align: start;
    align-items: flex-start;
    height: auto;
    padding-bottom: 20rpx;
}

.area-label {
    color: #727472;
    line-height: 98rpx;
}

.area-item {
    margin-right: 20rpx;
    background-color: #f3f3f3;
    position: relative;
    padding: 15rpx;
}

.area-item-add {
    display: -webkit-box;
    display: -webkit-flex;
    display: flex;
    width: 200rpx;
    border: 2rpx dotted black;
    text-align: center;
    -webkit-box-align: center;
    align-items: center;
    padding: 13rpx 0rpx;
}

.area-item-add-text {
    -webkit-box-flex: 1;
    flex: 1;
    font-weight: bold;
}

.area-item-wrapper {
    display: -webkit-box;
    display: -webkit-flex;
    display: flex;
}

.area-item-close {
    background-color: #777777;
    color: #fff;
    width: 40rpx;
    height: 40rpx;
    border-radius: 20rpx;
    font-size: 28rpx;
    line-height: 40rpx;
    text-align: center;
    position: absolute;
    right: -17rpx;
    top: -20rpx;
}

.require-wrap {
    padding: 24rpx 32rpx 24rpx 24rpx;
    margin-bottom: 20rpx;
    border: 1rpx solid #D2D2D2;
}

.label-title {
    font-size: 28rpx;
    color: #727472;
    height: 30rpx;
}

.list-label {
    padding: 24rpx 24rpx 0px 24rpx;
    font-size: 26rpx;
}

.list-item-label {
    display: inline-block;
    -webkit-appearance: none;
    min-width: 40rpx;
    text-align: center;
    line-height: 50rpx;
    height: 50rpx;
    padding: 0 20rpx;
    margin-right: 12rpx;
    background-color: #f6f6f6;
    border-radius: 100rpx;
    color: #555;
    margin-bottom: 20rpx;
    -webkit-tap-highlight-color: transparent;
}

.list-item-label:last-child {
    margin-right: 0;
}

.list-item-label.disabled {
    color: #fff;
    background-color: #b3b3b3;
}

.list-item-label.on {
    color: #fff;
    background-color: #53bcde;
}

.list-item-label.add {
    background-color: #fff;
    border: 2rpx solid #53bcde;
    color: #53bcde;
    font-size: 26rpx;
    padding: 0 20rpx;
    line-height: 46rpx;
    height: 46rpx;
    padding-left: 44rpx;
    position: relative;
}

.add:before,.add:after {
    height: 20rpx;
    width: 4rpx;
    content: "";
    position: absolute;
    background-color: #53bcde;
    top: 0;
    bottom: 0;
    left: 26rpx;
    margin: auto;
}

.add:after {
    transform: rotate(-90deg);
}

.require-textarea-block {
    background-color: #fff;
    width: 100%;
    height: 240rpx;
}

.require-detail,.require-detail-text {
    padding: 24rpx 0;
    width: 100%;
    height: 100%;
    color: #333;
    border: none;
    resize: none;
    box-sizing: border-box;
}

.require-detail-text.default-text-color {
    color: #727472;
}

.require-detail:focus {
    border: 1rpx solid #e0aa40;
}

.operator-banner {
    color: #ffbf5b;
    font-size: 28rpx;
    padding: 18rpx 22rpx;
    background-color: #fff7eb;
}

.operator-modal {
    top: 0;
    left: 0;
    width: 100%;
    height: 100%;
    position: fixed;
    display: -webkit-box;
    display: -webkit-flex;
    display: flex;
    -webkit-box-orient: vertical;
    -webkit-box-direction: normal;
    flex-direction: column;
    -webkit-box-pack: center;
    justify-content: center;
    -webkit-box-align: center;
    align-items: center;
    background-color: rgba(0,0,0,0.5);
    z-index: 100;
}

.operator-modal-wrapper {
    position: relative;
}

.modal-content {
    width: 480rpx;
    -webkit-box-flex: 0;
    flex: 0 0 400rpx;
    height: 400rpx;
    padding-top: 20rpx;
    position: relative;
    text-align: center;
    border-radius: 8rpx;
}

.modal-success {
    padding-top: 55rpx;
    background-image: url("//img.58cdn.com.cn/weixin/img/wechat-app/operator/hongbao.png");
    background-size: cover;
    background-repeat: no-repeat;
}

.modal-close {
    width: 50rpx;
    height: 50rpx;
    background-image: url(data:image/png;base64,);
    background-size: contain;
    background-repeat: no-repeat;
    position: absolute;
    bottom: -75rpx;
    left: 0rpx;
    right: 0rpx;
    margin: auto;
}

.modal-success-title {
    font-size: 24rpx;
    color: #ffcf57;
}

.modal-success-money {
    margin-top: 40rpx;
    color: #ff6047;
    font-size: 55rpx;
    font-weight: bold;
}

.modal-success-unit {
    padding-left: 5rpx;
    font-size: 22rpx;
}

.modal-success-desc {
    margin-top: 5rpx;
    font-size: 20rpx;
    color: #a9a9a9;
}

.modal-success-ok {
    color: #ff6047;
    background-color: #ffe152;
    width: 80%;
    margin: 0 auto;
    border-radius: 10rpx;
    position: absolute;
    left: 0rpx;
    right: 0rpx;
    margin: auto;
    bottom: 50rpx;
    font-size: 24rpx;
    line-height: 60rpx;
}

.modal-fail {
    padding-top: 30rpx;
    background-color: #fff;
    -webkit-box-flex: 0;
    flex: 0 0 430rpx;
    height: 430rpx;
}

.modal-fail-icon {
    width: 80%;
    -webkit-box-flex: 0;
    flex: 0 0 220rpx;
    height: 220rpx;
    margin: auto;
    background-image: url("//img.58cdn.com.cn/weixin/img/wechat-app/operator/submit-success.png");
    background-size: contain;
    background-repeat: no-repeat;
}

.modal-fail-title {
    margin-top: 15rpx;
    font-weight: bold;
    font-size: 35rpx;
}

.modal-fail-desc {
    margin-top: 5rpx;
    font-size: 20rpx;
    color: #a9a9a9;
}

.modal-fail-ok {
    color: #ff6047;
    background-color: #ffe152;
    width: 80%;
    margin: 0 auto;
    border-radius: 10rpx;
    position: absolute;
    left: 0rpx;
    right: 0rpx;
    margin: auto;
    bottom: 30rpx;
    font-size: 24rpx;
    line-height: 60rpx;
}