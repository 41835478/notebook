.container-main {
    background-color: #fff;
}

.rommate-list {
    height: 100vh;
}

.rommate-item {
    padding: 25rpx 30rpx;
    border-bottom: 1px solid #eee;
    display: -webkit-box;
    display: -webkit-flex;
    display: flex;
    -webkit-box-pack: justify;
    justify-content: space-between;
    -webkit-box-align: center;
    align-items: center;
}

.rommate-item .info {
    display: -webkit-box;
    display: -webkit-flex;
    display: flex;
    -webkit-box-align: center;
    align-items: center;
}

.rommate-item .info image {
    width: 90rpx;
    height: 90rpx;
    border-radius: 50%;
    margin-right: 30rpx;
    background-image: url("https://img.58cdn.com.cn/weixin/img/wechat-app/avatar-default.png");
    background-size: cover;
}

.rommate-item .info text {
    font-family: PingFangSC-Regular;
    font-size: 32rpx;
    color: #333333;
    letter-spacing: -0.77rpx;
}

.rommate-item .toChat {
    font-family: PingFangSC-Regular;
    font-size: 24rpx;
    color: #FF552E;
    letter-spacing: 0;
    line-height: 25rpx;
    height: 29rpx;
    padding-left: 47rpx;
    background-image: url(data:image/png;base64,);
    background-repeat: no-repeat;
    background-size: 37rpx 29rpx;
    background-position: left center;
}