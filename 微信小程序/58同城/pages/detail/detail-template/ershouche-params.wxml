<template name="ershouche-params">
    <view class="fang-block ersc-cw-block marg-top">
        <view class="fang-tag ersc-cw" wx:if="{{info.paramMap.price}}">
            <text class="tab-label">价格：</text>
            <text class="tab-title">{{info.paramMap.price}}{{info.priceUnit}}</text>
        </view>
        <view class="fang-tag ersc-cw" wx:if="{{info.paramMap.certificate}}">
            <text class="tab-label">上牌时间：</text>
            <text class="tab-title">{{info.paramMap.certificate}}</text>
        </view>
        <view class="fang-tag ersc-cw" wx:if="{{info.paramMap.miles}}">
            <text class="tab-label">行驶里程：</text>
            <text class="tab-title">{{info.paramMap.miles}}</text>
        </view>
        <view class="fang-tag ersc-cw" wx:if="{{info.paramMap.expired}}">
            <text class="tab-label">年检到期：</text>
            <text class="tab-title">{{info.paramMap.expired}}</text>
        </view>
    </view>
</template>
