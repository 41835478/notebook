@import "../../__wuBaseWxss__/5.wxss";

@import "list/index.wxss";

@import "../common/verify-pic.wxss";

@import "../detail/detail-template/apply-resume-list.wxss";

.main-container {
    overflow: hidden;
    height: 100vh;
    display: -webkit-box;
    display: -webkit-flex;
    display: flex;
    -webkit-box-orient: vertical;
    -webkit-box-direction: normal;
    flex-direction: column;
}

.youxuan-container {
    -webkit-box-flex: 1;
    flex: 1;
    height: calc(100vh - 88rpx - 80rpx);
    overflow: hidden;
}

.nav_bottom-wrapper {
    width: 100%;
    height: 88rpx;
    bottom: 0rpx;
    background-color: #ff552e;
    display: -webkit-box;
    display: -webkit-flex;
    display: flex;
    -webkit-box-orient: horizontal;
    -webkit-box-direction: normal;
    flex-direction: row;
    -webkit-box-flex: 0;
    flex: 0 0 80rpx;
}

.nav_bottom {
    font-size: 32rpx;
    color: #fff;
    -webkit-box-flex: 1;
    flex: 1;
    text-align: center;
    height: 80rpx;
    line-height: 80rpx;
    box-sizing: border-box;
}

.toTop {
    font-size: 20rpx;
    color: #ddd;
    width: 80rpx;
    height: 80rpx;
    line-height: 60rpx;
    text-align: center;
    border-radius: 100%;
    position: fixed;
    bottom: 110rpx;
    right: 30rpx;
    background-color: rgba(0,0,0,0.3);
}

.toTop::after {
    content: '';
    position: absolute;
    bottom: 20rpx;
    left: 27rpx;
    border-left: 15rpx solid transparent;
    border-right: 15rpx solid transparent;
    border-bottom: 15rpx solid #ccc;
}

.nav-btn {
    font-size: 30rpx;
    color: #333;
    -webkit-box-flex: 1;
    flex: 1;
    line-height: 80rpx;
    box-sizing: border-box;
    text-overflow: ellipsis;
    white-space: nowrap;
    overflow: hidden;
    letter-spacing: -0.72rpx;
    display: -webkit-box;
    display: -webkit-flex;
    display: flex;
    -webkit-box-align: center;
    align-items: center;
    -webkit-box-pack: center;
    justify-content: center;
}

.nav-btn .nav-name {
    height: 82rpx;
    display: inline-block;
}

.nav-symbol {
    color: #C0BCBC;
    font-size: 20rpx;
}

.nav-on {
    color: #ff552e;
}

.nav-on .nav-name {
    border-bottom: 4rpx solid #ff552e;
}

.nav-wrapper {
    display: -webkit-box;
    display: -webkit-flex;
    display: flex;
    height: 88rpx;
    -webkit-box-flex: 0;
    flex: 0 0 88rpx;
    border-bottom: 1rpx solid #F3F3F3;
}

.tips {
    margin-top: 165rpx;
    display: -webkit-box;
    display: -webkit-flex;
    display: flex;
    -webkit-box-orient: vertical;
    -webkit-box-direction: normal;
    flex-direction: column;
    -webkit-box-pack: center;
    justify-content: center;
    -webkit-box-align: center;
    align-items: center;
}

.tips-icon {
    width: 70rpx;
    height: 86rpx;
    margin-bottom: 40rpx;
    background-image: url(data:image/png;base64,);
    background-repeat: no-repeat;
    background-size: contain;
}

.tips-desc {
    font-size: 26rpx;
    line-height: 26rpx;
    color: #A9A9A9;
}

.no-more-info {
    width: 100%;
    text-align: center;
    font-size: 22rpx;
    color: #ff552e;
    padding: 15rpx 0rpx;
    display: none;
}

.show {
    display: block;
}

.gotoCateList {
    height: 72rpx;
    line-height: 72rpx;
    background: #FEFCED;
    text-align: center;
    font-family: PingFangSC-Regular;
    font-size: 28rpx;
    color: #E6743D;
    letter-spacing: 0;
}

.gotoCateList::after {
    content: "";
    position: absolute;
    width: 0;
    height: 0;
    border-top: 9rpx solid transparent;
    border-bottom: 9rpx solid transparent;
    border-left: 9rpx solid #E6743D;
    color: #E6743D;
    top: 30rpx;
    right: 240rpx;
}