@import "timer_spike_container.wxss";

.blank {
    width: 100%;
    height: 16rpx;
    background-color: #EDEDED;
}

.ts-goods-item-view {
    height: 262rpx;
    width: 100%;
    position: relative;
    padding: 0;
    display: flex;
    background-color: #ffffff;
    margin-left: 0;
    margin-right: 0;
    margin-top: 18rpx;
}

.ts-goods-item-view::after {
    position: relative;
    height: 0;
    width: 0;
}

.ts-goods-bottom-line {
    position: absolute;
    bottom: 0;
    right: 0;
    width: 481rpx;
    height: 2rpx;
    background-color: #EDEDED;
}

.ts-goods-img-view {
    height: 240rpx;
    width: 240rpx;
    display: inline-flex;
    margin-right: 28rpx;
    position: relative;
    z-index: 10;
}

.ts-goods-img {
    position: relative;
    top: 0;
    width: 240rpx;
    height: 240rpx;
    z-index: 11;
}

.ts-sales-out-sign {
    position: absolute;
    top: 0;
    left: 0;
    bottom: 20rpx;
    width: 240rpx;
    height: 240rpx;
    z-index: 12;
    background-color: rgba(255,255,255,0.5);
}

.ts-sales-out-img {
    width: 100%;
    height: 100%;
    z-index: 13;
}

.ts-goods-info-view {
    display: inline-flex;
    height: 100%;
    flex-direction: column;
    background-color: white;
}

.ts-goods-name {
    width: 454rpx;
    height: 80rpx;
    font-size: 28rpx;
    line-height: 40rpx;
    color: #000000;
    overflow: hidden;
    text-overflow: ellipsis;
    word-break: break-all;
    word-wrap: break-word;
    -webkit-line-clamp: 2;
    -webkit-box-orient: vertical;
    margin-top: 5rpx;
    text-align: left;
}

.ts-goods-name-sold-out {
    color: #9C9C9C;
}

.ts-goods-sales-info {
    width: 100%;
    display: flex;
    align-items: center;
    margin-top: 14rpx;
    height: 33rpx;
}

.ts-sales-process-bg {
    height: 32rpx;
    width: 258rpx;
    flex-grow: 0;
    flex-shrink: 0;
    border: 1rpx solid #F3ABA9;
    background-color: #F3ABA9;
    border-radius: 16rpx;
    position: relative;
    display: flex;
    align-items: center;
    justify-content: center;
}

.ts-solding-out:after {
    content: '\e656';
    position: absolute;
    font-size: 31rpx;
    bottom: 0;
    right: -1rpx;
    width: 32rpx;
    height: 32rpx;
    font-family: "icomoon";
    line-height: 32rpx;
    color: #FDBD1B;
    font-weight: normal;
    -webkit-font-smoothing: antialiased;
}

.ts-solding-out .solding-out {
    width: 88rpx;
    height: 21rpx;
    background: url(http://minipinduoduo.oss-cn-shanghai.aliyuncs.com/spike/icon_solding_out_3.png) no-repeat;
    background-size: 100% 100%;
    z-index: 20;
}

.ts-sales-process {
    height: 100%;
    background-color: #E02E24;
    border-radius: 16rpx;
    position: absolute;
    top: 0;
    left: 0;
}

.ts-sales-out-process {
    width: 100%;
}

.ts-progress-five-percent {
    border-radius: 16rpx!important;
}

.ts-progress-ten-percent {
    border-radius: 22rpx!important;
}

.ts-sales-present {
    position: relative;
    line-height: 20rpx;
    height: 30rpx;
    width: 100%;
    line-height: 30rpx;
    font-size: 22rpx;
    color: #FFFFFF;
    display: flex;
    align-items: center;
    justify-content: center;
    font-weight: 700;
}

.ts-goods-sales-num {
    height: 100%;
    line-height: 100%;
    font-size: 24rpx;
    color: #E02E24;
    margin-left: 22rpx;
    display: flex;
    justify-content: center;
    align-items: center;
}

.ts-goods-price-info {
    width: 454rpx;
    height: 60rpx;
    display: flex;
    justify-content: space-between;
    align-items: center;
    margin-top: 44rpx;
}

.ts-goods-price-view {
    vertical-align: text-bottom;
    height: 100%;
    line-height: 100%;
    margin-top: 20rpx;
}

.ts-goods-price {
    display: inline;
    font-size: 40rpx;
    height: 56rpx;
    line-height: 56rpx;
    color: #E02E24;
    letter-spacing: 0;
    font-weight: 600;
}

.ts-goods-market-price {
    display: inline;
    font-size: 24rpx;
    color: #9C9C9C;
    text-decoration: line-through;
    height: 33rpx;
    line-height: 33rpx;
    margin-left: 16rpx;
    letter-spacing: 0;
}

.ts-goods-price::before {
    content: 'Â¥';
    font-size: 24rpx;
    display: inline-block;
    width: 24rpx;
    height: 33rpx;
    line-height: 33rpx;
}

.ts-goods-btn {
    height: 60rpx;
    width: 142rpx;
    display: inline-block;
    margin-right: -2rpx;
    margin-top: 2rpx;
    position: relative;
}

.ts-goods-btn::after {
    border: 0;
    position: relative;
}

.ts-on-btn {
    background: url(http://minipinduoduo.oss-cn-shanghai.aliyuncs.com/spike/icon_on_3.png) no-repeat;
    background-size: 100% 100%;
}

.ts-coming-btn {
    background: url(http://minipinduoduo.oss-cn-shanghai.aliyuncs.com/spike/icon_remind_3.png) no-repeat;
    background-size: 100% 100%;
    margin-top: -2rpx;
}

.ts-off-btn {
    background: url(http://minipinduoduo.oss-cn-shanghai.aliyuncs.com/spike/icon_off_3.png) no-repeat;
    background-size: 100% 100%;
    margin-top: -2rpx;
}

.ts-remind-btn {
    background: url(http://minipinduoduo.oss-cn-shanghai.aliyuncs.com/spike/icon_remind_off_3.png) no-repeat;
    background-size: 100% 100%;
    margin-top: -2rpx;
}

.ts-goods-btn-view {
    display: block;
    height: 60rpx;
    width: 142rpx;
}

.ts-goods-off-price {
    color: #9C9C9C!important;
}

.ts-goods-sales-out-info {
    margin-top: 7rpx;
    margin-bottom: 7rpx;
}

.ts-goods-sales-out-info .ts-goods-sales-out-num {
    color: #9C9C9C;
    margin-left: 0;
}

.ts-goods-sales-comming-info {
    width: 100%;
    display: flex;
    align-items: center;
    margin-top: 5rpx;
    height: 37rpx;
}

.ts-goods-sales-comming-info .ts-sales-comming-process-bg {
    border: 1px solid #E5DFE1;
    background: #F4F4F4;
    width: 100%;
}

.ts-goods-sales-comming-info .ts-goods-sales-comming-num {
    font-size: 26rpx;
    color: #F2B61D;
    display: flex;
    justify-content: flex-start;
    margin-left: 0;
}

.ts-goods-sales-comming-info .space {
    opacity: 0.4;
    position: relative;
    margin-left: 10rpx;
    margin-right: 10rpx;
    font-size: 24rpx;
}

.ts-goods-comming-price-info {
    margin-top: 51rpx;
}

.brand-spike-list {
    width: 100%;
    height: 428rpx;
    position: relative;
    display: flex;
    justify-content: center;
}

.brand-img {
    width: 750rpx;
    height: 428rpx;
    position: absolute;
    top: 0;
    left: 0;
}

.brand-item-info {
    display: inline-block;
    width: 214rpx;
    height: 294rpx;
    background-color: #fff;
    margin: 110rpx 14rpx 0;
    position: relative;
}

.brand-item-info image {
    display: block;
    width: 214rpx;
    height: 214rpx;
}

.brand-spike-list .ts-goods-info-view {
    position: relative;
    height: 80rpx;
    width: 100%;
}

.brand-spike-list .ts-goods-name {
    width: 200rpx;
    font-size: 24rpx;
    height: 36rpx;
    margin-left: 5rpx;
    position: absolute;
    color: #151516;
    white-space: nowrap;
}

.brand-spike-list .ts-goods-price {
    font-size: 26rpx;
    font-weight: normal;
    height: 30rpx;
    left: 10rpx;
    line-height: 30rpx;
    position: absolute;
    bottom: 6rpx;
    display: flex;
    justify-content: center;
    align-items: center;
}

.brand-spike-list .ts-goods-price::before {
    font-size: 20rpx;
    display: inline-block;
    width: 18rpx;
    height: 30rpx;
    line-height: 30rpx;
}

.brand-spike-list .ts-goods-market-price {
    font-size: 20rpx;
    height: 30rpx;
    line-height: 30rpx;
    right: 10rpx;
    letter-spacing: 0;
    color: #9c9c9c;
    position: absolute;
    bottom: 6rpx;
}

.sale-out {
    width: 80rpx;
    height: 26rpx;
    margin: 0 auto;
    position: absolute;
    right: 0;
    bottom: 4rpx;
    background-color: red;
    border-radius: 13rpx 0 0 13rpx;
    color: #fff;
    font-size: 16rpx;
    text-align: center;
    line-height: 30rpx;
}

.ts-time-line {
    margin-top: 24rpx;
}

.ts-now-time-line-icon {
    margin-right: 16rpx;
    font-size: 32rpx;
}

.icon-daily-berserk::before {
    content: "\e655";
}

.ts-time-line-num {
    font-size: 30rpx;
}

.ts-down-count {
    margin-top: 12rpx;
}

.ts-down-count-num-view .num {
    width: 40rpx;
    font-size: 28rpx;
}

