button,button::after {
    border: none;
    border-radius: 0;
}

.icon-address-delete {
    font-size: 30rpx;
    color: #666666;
    line-height: 1;
    background-color: transparent;
    padding: 14rpx 0 0 1rpx;
    font-family: "icomoon";
    vertical-align: top;
    text-align: center;
    font-weight: normal;
    -webkit-font-smoothing: antialiased;
    width: 100%;
    height: 100%;
}

.navigation-modal-icon .txt {
    padding: 0 0 10rpx 2rpx;
    margin-top: 10rpx;
    width: 86rpx;
    color: #5F5F5F;
    font-size: 20rpx;
    line-height: 1;
    text-align: center;
    opacity: 1;
    color: #333;
    z-index: 1009;
}