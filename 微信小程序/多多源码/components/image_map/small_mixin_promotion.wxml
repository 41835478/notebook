<template name="SmallMixinPromotion">
    <form bindsubmit="$uploadFormId" reportSubmit="true">
        <button catchtap="gotoSubject" class="double-grid-one" data-subject-id="{{subject_id}}" formType="submit" id="impr-id">
            <view>
                <view class="double-grid-item {{(index+1)%2===0?'gap-left':'gap-right'}}">
                    <image class="small-mixin-pro-img" src="{{banner}}"></image>
                </view>
            </view>
        </button>
    </form>
</template>
