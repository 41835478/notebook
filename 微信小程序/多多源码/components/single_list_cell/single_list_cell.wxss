.single-list-cell {
    background-color: white;
    height: 90rpx;
    line-height: 90rpx;
    display: block;
    position: relative;
    font-size: 32rpx;
}

.single-list-cell-icon {
    display: inline-block;
    position: relative;
    width: 40rpx;
    height: 40rpx;
    margin-left: 16rpx;
    top: 25rpx;
}

.single-list-cell-title {
    display: inline-block;
    margin-left: 16rpx;
}

.single-list-cell::before {
    content: "\e617";
    font-size: 24rpx;
    color: #9c9c9c;
    font-family: "icomoon";
    line-height: 1;
    font-weight: normal;
    font-smoothing: antialiased;
    position: absolute;
    top: 34rpx;
    right: 20rpx;
}

.single-list-cell-bottomline {
    position: absolute;
    height: 1px;
    background-color: #ededed;
    bottom: 0;
    left: 8px;
    right: 0px;
}