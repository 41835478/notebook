.subject-item {
    width: 50%;
    height: 584rpx;
    box-sizing: border-box;
    display: inline-flex;
    vertical-align: top;
    margin-bottom: 18rpx;
    overflow: hidden;
}

.subject-grid-item {
    width: 348rpx;
    height: 584rpx;
    background-color: #ffffff;
    position: relative;
}

.subject-gap-right {
    padding-left: 18rpx;
    padding-right: 9rpx;
}

.subject-gap-left {
    padding-right: 18rpx;
    padding-left: 9rpx;
}

.draw-common-img {
    background: url('http://minipinduoduo.oss-cn-shanghai.aliyuncs.com/lottery/lottery_result_common.png') no-repeat;
    background-size: 476rpx;
}

.time-left-icon {
    line-height: 35rpx;
    min-width: 121rpx;
    height: 35rpx;
    line-height: 35rpx;
    background-color: #DFBD88;
    position: absolute;
    left: -1rpx;
    font-size: 22rpx;
    padding-left: 12rpx;
    padding-right: 24rpx;
    letter-spacing: 0;
    color: #FFFFFF;
    border-top-right-radius: 35rpx;
    border-bottom-right-radius: 35rpx;
    top: 0;
    left: 0;
    background-image: none;
}

.subject-goods-img {
    width: 100%;
    height: 348rpx;
}

.subject-goods-name {
    padding: 14rpx 18rpx 0 18rpx;
    width: 100%;
    overflow: hidden;
    word-break: break-all;
    word-wrap: break-word;
    text-align: justify;
    font-size: 24rpx;
    color: #151516;
    height: 82rpx;
    line-height: 1.5;
}

.subject-goods-detail {
    width: 100%;
    padding: 14rpx 20rpx 0 20rpx;
    display: flex;
    align-items: flex-end;
    justify-content: space-between;
}

.subject-goods-sign-img {
    width: 74rpx;
    height: 30rpx;
    margin-bottom: 4rpx;
}

.subject-goods-sign {
    width: 75rpx;
    height: 35rpx;
    position: relative;
    display: flex;
    justify-content: center;
    align-items: center;
    font-size: 22rpx;
    padding-left: 1px;
    padding-top: 1px;
    line-height: 1;
    color: rgb(186,152,67);
    margin-right: 10rpx;
}

.subject-goods-sign::after {
    content: '';
    position: absolute;
    left: 0;
    top: 0;
    width: 200%;
    height: 200%;
    border: 1px solid rgb(186,152,67);
    transform-origin: 0 0;
    transform: scale(0.5,0.5);
    box-sizing: border-box;
}

.subject-goods-price {
    line-height: 48rpx;
    height: 48rpx;
    font-size: 34rpx;
    color: #DE2F24;
    position: relative;
    padding-left: 10rpx;
}

.subject-goods-price::before {
    content: '¥';
    display: inline-block;
    width: 21rpx;
    line-height: 33rpx;
    height: 33rpx;
    font-size: 24rpx;
}

.subject-goods-price-view {
    height: 100%;
    display: flex;
    align-items: flex-end;
}

.subject-goods-market {
    font-size: 22rpx;
    text-decoration: line-through;
    color: #9c9c9c;
}

.subject-goods-btn-view {
    margin-top: 18rpx;
    width: 100%;
    height: 60rpx;
    padding: 0 16rpx;
    display: flex;
}

.subject-goods-btn {
    width: 100%;
    height: 100%;
    display: flex;
    justify-content: center;
    align-items: center;
    font-size: 26rpx;
    color: #ffffff;
    background-color: #e02e24;
    border-radius: 4rpx;
}

.subject-group-num {
    position: absolute;
    top: 0;
    left: 0;
    font-size: 22rpx;
    color: #ffffff;
    height: 38rpx;
    line-height: 38rpx;
    padding: 0 10rpx;
    background: -webkit-gradient(linear,0 0,right 0,from(rgb(173,135,39)),to(rgb(226,188,95)));
    border-radius: 0 17rpx 17rpx 0;
}

.coupon-subject-goods-style {
    display: flex;
    border: 1px solid #de2116;
    height: 34rpx;
    width: 154rpx;
    border-radius: 2px;
}

.coupon-subject-goods-hint {
    height: 100%;
    width: 70rpx;
    border-radius: 1px 0 0 1px;
    flex-shrink: 0;
    flex-grow: 0;
    background-color: #de2116;
    color: #ffffff;
    font-size: 18rpx;
    display: flex;
    justify-content: center;
    align-items: center;
}

.coupon-subject-goods-price {
    height: 100%;
    width: 80rpx;
    display: flex;
    justify-content: center;
    align-items: center;
    font-size: 20rpx;
    color: #de2116;
}

.small-mixin-pro-img {
    width: 100%!important;
    height: 540rpx!important;
}

.subject-goods-sign-style2 {
    margin-right: 0;
    color: #ee2e3a;
    font-size: 24rpx;
}

.subject-goods-sign-style2::after,.subject-goods-sign-style3::after,.subject-goods-sign-style4::after {
    border: none;
}

.subject-goods-sign-style3,.subject-goods-market-style3,.subject-goods-price-style3,.subject-goods-sign-style4 {
    color: #ffffff;
}

.style3-sub-wrap {
    background-color: #e02e24;
    border-radius: 8rpx;
}

.subject-goods-sign-style4 {
    background-color: #e02e24;
    border-radius: 4rpx;
}

.style3-btn-wrap {
    width: 100%;
    padding: 16rpx 20rpx 0 20rpx;
}

.style3-sub-wrap {
    height: 96rpx;
    display: flex;
    align-items: center;
    justify-content: space-around;
}

.subject-goods-btn-style3 {
    width: 80rpx;
    height: 80rpx;
    display: flex;
    flex-direction: column;
    align-items: center;
    border-radius: 8rpx;
    background: #ffffff;
    color: #e02e24;
    font-weight: bolder;
    font-size: 28rpx;
    line-height: 1.2;
    text-align: center;
}

.subject-goods-btn-style3>view {
    width: 56rpx;
    padding-top: 4rpx;
}

.subject-goods-sign-style3,.subject-goods-market-style3 {
    font-size: 26rpx;
    width: 100%;
}

.goods-sign-market-style3 {
    width: 86rpx;
}

.subject-goods-market-style3 {
    padding-left: 2rpx;
}

.padd-left {
    padding-left: 16rpx;
}

.subject-num-img-wrap {
    position: relative;
}

.subject-group-num-style4 {
    position: absolute;
    bottom: 0;
    left: 0;
    width: 182rpx;
    height: 62rpx;
    line-height: 62rpx;
    text-align: center;
    background: rgba(0,0,0,0.6);
    color: #D2D2D2;
    font-size: 26rpx;
    border-radius: 0 17rpx 0 0;
}

.style2-style4-fontsize {
    font-size: 26rpx;
}

.subject-item-style2 {
    height: 502rpx;
}

.subject-goods-name-style2 {
    position: absolute;
    bottom: 0;
    left: 0;
    width: 100%;
    height: 116rpx;
    line-height: 40rpx;
    text-align: center;
    color: #ffffff;
    font-size: 26rpx;
    padding: 35rpx 20rpx 13rpx 19rpx;
    text-align: justify;
    background: linear-gradient(to bottom,rgba(0,0,0,0) 0%,rgba(0,0,0,0.2) 25%,rgba(0,0,0,0.3) 40%,rgba(0,0,0,0.5) 60%,rgba(0,0,0,0.75) 75%);
}

.subject-item-style3 {
    height: 566rpx;
}

.relative-recommend-item {
    height: auto;
    padding: 0;
    margin-bottom: 8rpx;
    overflow: hidden;
}

.relative-recommend-v0-detail {
    width: 100%;
    padding: 4rpx 20rpx 0 20rpx;
    height: 62rpx;
    font-size: 0;
    overflow: hidden;
}

.relative-recommend-price::before {
    content: '¥  ';
    font-size: 20rpx;
}

.relative-recommend-price {
    height: 62rpx;
    line-height: 62rpx;
    font-size: 32rpx;
    color: #ee2e3a;
    float: left;
}

.relative-recommend-info {
    float: right;
    height: 62rpx;
    line-height: 62rpx;
    font-size: 26rpx;
    color: #9c9c9c;
}

.relative-recommend-subject-float-left {
    float: left;
    margin-right: 3rpx;
}

.relative-recommend-subject-float-right {
    float: right;
    margin-left: 3rpx;
}

.relative-recommend-grid-item {
    width: 372rpx;
    height: auto;
    overflow: hidden;
    background-color: #fff;
}

.inline-block {
    display: inline-flex;
    width: 50%;
    overflow: hidden;
    line-height: normal;
    background: none;
    padding: 0!important;
}

.formid-subject-item {
    width: 100%;
}