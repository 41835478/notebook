    height: 100%;
}

.hide {
    display: none;
}

.complaint-container {
    width: 100%;
    height: 100%;
    overflow: auto;
    -webkit-overflow-scrolling: touch;
    text-align: center;
    background-color: #f0f0f0;
}

.complaint-container .contpart {
    height: 100%;
}

.complaint-container .outer {
    min-height: 100%;
    background: #fff;
}

.complaint-container .ncomplaint {
    box-sizing: border-box;
    padding: 0 10px 20px;
    width: 100%;
}

.complaint-container .ncomplaint .extra {
    position: relative;
    background-color: #fff;
    padding-bottom: 10px;
}

.complaint-container .expire {
    padding-top: 120px;
}

.complaint-container .expire .exicon {
    width: 90px;
    height: 90px;
    display: block;
    background: url("data:image/png;base64,") center center no-repeat;
    background-size: 100% auto;
    text-align: center;
    margin: 0 auto;
}

.complaint-container .expire .extext {
    width: 100%;
    font-size: 14px;
    color: #adadad;
}

.complaint-container .ncomplaint .complt-title {
    font-size: 14px;
    padding: 14px 0 12px 6px;
    color: #999;
    text-align: left;
}

.complaint-container .ncomplaint .complt-title:first-child {
    padding-top: 20px;
}

.complaint-container .ncomplaint .complt-box {
    background: #fff;
    border-radius: 4px;
}

.complaint-container .footer.kefu-cont {
    margin-top: -58px;
    height: 58px;
    line-height: 58px;
}

.complaint-container .footer .kefu-online {
    margin-right: 90px;
}

.complaint-container .kefu-cont {
    height: 50px;
    line-height: 50px;
    background: #fff;
    font-size: 14px;
    border-radius: 2px;
}

.complaint-container .kefu-cont .btn-kefu {
    width: 50%;
    text-align: center;
    float: left;
    position: relative;
}

.complaint-container .kefu-cont .btn-tel:before {
    display: block;
    content: '';
    position: absolute;
    left: 0;
    bottom: 0;
    height: 100%;
    border-left: 1px solid #eee;
    transform: scaleX(0.5);
    -webkit-transform: scaleX(0.5);
}

.complaint-container .footer.kefu-cont .btn-tel:before {
    border: 0;
}

.complaint-container .kefu-cont text {
    display: inline-block;
    text-decoration: none;
    padding-left: 18px;
    color: #666;
}

.complaint-container .kefu-cont .kefu-online {
    background: url("data:image/png;base64,");
    background-repeat: no-repeat;
    background-position: left center;
    background-size: 14px auto;
}

.complaint-container .kefu-cont .kefu-tel {
    background: url("data:image/png;base64,");
    background-repeat: no-repeat;
    background-position: left center;
    background-size: 14px auto;
}

.complaint-container .complt-luxury {
    display: none;
    text-align: center;
    line-height: 1;
    margin-top: 100px;
}

.complaint-container .complt-luxury img {
    max-width: 110px;
    display: block;
    margin: 0 auto;
}

.complaint-container .complt-luxury .complt-luxury-tips1 {
    padding: 16px 0 8px;
    color: #666;
    font-size: 14px;
}

.complaint-container .complt-luxury .complt-luxury-tips2 {
    padding: 0 0 16px;
    color: #999;
    font-size: 12px;
}

.complaint-container .complt-luxury .kefu-tel {
    display: block;
    margin: 0 auto;
    background: #4a4c5b;
    width: 90px;
    height: 36px;
    line-height: 36px;
    color: #fff;
    font-size: 12px;
    border-radius: 4px;
    text-decoration: none;
}

.complaint-container .complt-luxury .kefu-tel:active {
    background: #444753;
}

.complaint-container .ncomplaint .ul {
    position: relative;
    box-sizing: border-box;
}

.complaint-container .ncomplaint .other {
    color: #333;
    font-size: 16px;
}

.complaint-container .ncomplaint .ul:after {
    display: block;
    content: '';
    position: absolute;
    left: 0;
    top: 0;
    box-sizing: border-box;
    width: 200.5%;
    height: 200%;
    border: 1px solid #e5e5e5;
    border-bottom: 0;
    border-radius: 8px;
    transform-origin: 0 0;
    -webkit-transform-origin: 0 0;
    transform: scale(0.5);
    -webkit-transform: scale(0.5);
}

.complaint-container .ncomplaint ul.open:after,.complaint-container .ncomplaint ul.open li:after {
    border-bottom-left-radius: 0;
    border-bottom-right-radius: 0;
}

.complaint-container .ncomplaint .ul.ful.on .li {
    color: #fa8919;
}

.complaint-container .ncomplaint .ul.ful.on.open .li {
    color: #333;
}

.complaint-container .ncomplaint .li {
    position: relative;
    padding-left: 15px;
    font-size: 14px;
    height: 48px;
    font-size: 14px;
    line-height: 48px;
    text-align: left;
    text-overflow: ellipsis;
    white-space: nowrap;
    color: #333;
}

.complaint-container .ncomplaint .ul .li .arrow {
    display: block;
    position: absolute;
    right: 16px;
    top: 22px;
    width: 10px;
    height: 6px;
    background: url("data:image/NGf;base64,") no-repeat;
    background-size: 100% 100%;
}

.complaint-container .ncomplaint .ul.open .li .arrow {
    transform: rotate(180deg);
    -webkit-transform: rotate(180deg);
    -moz-transform: rotate(180deg);
}

.complaint-container .ncomplaint .subul {
    text-align: left;
    position: relative;
    width: 100%;
    font-size: 13px;
    border-bottom-left-radius: 4px;
    border-bottom-right-radius: 4px;
    z-index: 1;
}

.complaint-container .ncomplaint .subul:before {
    content: '';
    position: absolute;
    left: 15px;
    right: 0;
    top: 0;
    display: block;
    box-sizing: border-box;
    height: 1px;
    background-color: #ebebeb;
    transform-origin: 0 0;
    -webkit-transform-origin: 0 0;
    transform: scaleY(0.5);
    -webkit-transform: scaleY(0.5);
}

.complaint-container .ncomplaint .subul text {
    display: block;
    font-size: 14px;
    color: #666;
    padding: 12px 20px 12px 40px;
    line-height: 37px;
    box-sizing: border-box;
    overflow: hidden;
    height: 56.5px;
    width: 100%;
    background-image: url("data:image/NGf;base64,");
    background-repeat: no-repeat;
    background-position: 15px center;
    background-size: 16px 16px;
    overflow: hidden;
}

.complaint-container .ncomplaint .subul text.on {
    background-image: url("data:image/NGf;base64,");
    background-repeat: no-repeat;
    background-position: 15px center;
    background-size: 16px 16px;
}

.complaint-container .ncomplaint .extra {
    position: relative;
    background-color: #fff;
}

.complaint-container .ncomplaint .extra-textarea {
    padding: 10px 0;
    background-color: #fff;
}

.complaint-container .ncomplaint .extra:before {
    display: block;
    content: '';
    position: absolute;
    left: 0;
    top: 0;
    box-sizing: border-box;
    width: 200%;
    height: 200%;
    border: 1px solid #e5e5e5;
    border-bottom: 0;
    border-radius: 8px;
    transform-origin: 0 0;
    -webkit-transform-origin: 0 0;
    transform: scale(0.5);
    -webkit-transform: scale(0.5);
    border-bottom: 1px solid #e5e5e5;
}

.complaint-container .ncomplaint .extra .span {
    display: block;
    padding-left: 15px;
    height: 50px;
    line-height: 50px;
    font-size: 12px;
    text-align: left;
    color: #fa8919;
    position: relative;
}

.complaint-container .ncomplaint .extra .span:before {
    display: block;
    content: '';
    position: absolute;
    left: 0;
    bottom: 0;
    width: 100%;
    border-bottom: 1px solid #e5e5e5;
    transform: scaleY(0.5);
    -webkit-transform: scaleY(0.5);
}

.complaint-container .ncomplaint .extra .span:after {
    display: block;
    content: '';
    position: absolute;
    left: 24px;
    bottom: 0;
    width: 34px;
    height: 7px;
    background: #fff url("data:image/NGf;base64,") no-repeat;
    background-size: 100% 100%;
    background-repeat: no-repeat;
    background-position: left bottom;
}

.complaint-container .ncomplaint .extra input {
    position: static;
    color: #fff;
    padding: 15px 15px 10px 15px;
    box-sizing: border-box;
    width: 100%;
    height: 60px;
    font-size: 13px;
    line-height: 30px;
    text-align: justify;
    word-wrap: break-word;
    word-break: break-all;
    white-space: normal;
    color: #666;
    border: none;
    outline: none;
    resize: none;
    background-color: rgba(0,0,0,0);
}

.complaint-container .ncomplaint .extra .em {
    position: absolute;
    right: 16px;
    bottom: 10px;
    font-style: normal;
    font-size: 12px;
    color: #ccc;
    float: right;
}

.complaint-container .ncomplaint .extra .em.org {
    color: #fa8919;
}

.complaint-container .ncomplaint .psection {
    direction: ltr;
    text-align: left;
    color: #ccc;
}

.complaint-container .btnwrap {
    position: relative;
    left: 0;
    top: 10px;
    height: 44px;
    line-height: 44px;
    text-align: center;
}

.complaint-container .index-submit {
    display: block;
    font-size: 15px;
    color: #fff;
    width: 100%;
    height: 100%;
    line-height: 41.25px;
    border-radius: 4px;
    border-radius: 4px;
    border: none;
    outline: none;
    background-color: #dedede;
    transition: all 0.6s ease;
    -webkit-transition: all 0.6s ease;
}

.complaint-container .index-submit.submitable {
    background-color: #fa8919;
}

.p-loading {
    position: absolute;
    left: 0;
    top: 0;
    z-index: 99;
    width: 100%;
    height: 100%;
    background: #fff no-repeat url("http://static.udache.com/passenger/apps/price/estimate/images/loading.1d8e67c.gif") center center;
    background-size: 42px auto;
}