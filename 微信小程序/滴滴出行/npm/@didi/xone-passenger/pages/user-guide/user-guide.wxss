page {
    font-size: 20px;
    height: 100%;
}

.mod-p-price-rule {
    height: 100%;
    width: 100%;
    background: #eee;
    padding-top: 4px;
}

.mod-p-price-rule .mod-lists-inner {
    -webkit-overflow-scrolling: touch;
}

.mod-p-price-rule .mod-lists-inner .car-item {
    margin-top: 6px;
}

.mod-p-price-rule .mod-lists-inner .scene-name {
    color: #666;
    border-top: 1rpx solid rgba(0,0,0,0.08);
}

.mod-p-price-rule .mod-lists-inner .car-type-name,.mod-p-price-rule .mod-lists-inner .scene-name {
    background-color: #fff;
    font-family: PingFangSC-Regular;
    position: relative;
    padding: 0 10px;
    height: 60px;
    line-height: 60px;
    font-size: 14px;
    display: flex;
    align-items: center;
    justify-content: space-between;
}

.mod-p-price-rule .mod-lists-inner .car-type-name:after,.mod-p-price-rule .mod-lists-inner .scene-name:after {
    content: '';
    position: absolute;
    bottom: 0;
    left: 0;
    width: 100%;
    border-bottom: 1rpx solid #e5e5e5;
    transform: scaleY(0.5);
}

.mod-p-price-rule .mod-lists-inner .car-type-name .car-title,.mod-p-price-rule .mod-lists-inner .scene-name .car-title {
    color: #333;
    font-size: 16px;
}

.mod-p-price-rule .mod-lists-inner .car-type-name .car-sub-title,.mod-p-price-rule .mod-lists-inner .scene-name .car-sub-title {
    color: #666;
}

.mod-p-price-rule .mod-lists-inner .car-type-name .arrow,.mod-p-price-rule .mod-lists-inner .scene-name .arrow {
    transition: all 0.3s;
    margin-right: 12px;
}

.mod-p-price-rule .mod-lists-inner .car-type-name .arrow-top,.mod-p-price-rule .mod-lists-inner .scene-name .arrow-top {
    width: 0;
    height: 0;
    box-sizing: border-box;
    border-width: 0 4px 5px;
    border-style: solid;
    border-color: transparent transparent #666;
}

.mod-p-price-rule .mod-lists-inner .car-type-name .arrow-right,.mod-p-price-rule .mod-lists-inner .scene-name .arrow-right {
    width: 6px;
    height: 6px;
    margin-right: 15px;
    border-top: solid 1rpx #666;
    border-right: solid 1rpx #666;
    box-sizing: border-box;
    transform: rotate(45deg);
}

.mod-p-price-rule .mod-lists-inner .car-type-name .arrow-bottom,.mod-p-price-rule .mod-lists-inner .scene-name .arrow-bottom {
    width: 0;
    height: 0;
    border-width: 5px 4px 0;
    border-style: solid;
    border-color: #666 transparent transparent;
    box-sizing: border-box;
}