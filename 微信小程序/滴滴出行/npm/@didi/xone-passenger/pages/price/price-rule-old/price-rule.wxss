.mod-p-price-rule .container {
    position: absolute;
    left: 0;
    top: 0;
    z-index: 0;
    width: 100%;
    height: 100%;
    text-align: left;
    overflow: auto;
    -webkit-overflow-scrolling: touch;
}

.mod-p-price-rule .p-error {
    position: absolute;
    left: 0;
    top: 0;
    z-index: 99;
    display: flex;
    align-items: center;
    justify-content: center;
    width: 100%;
    height: 100%;
    font-size: 14px;
    line-height: 20px;
    color: #ff8a01;
    background-color: #fff;
}

.mod-p-price-rule .p-loading {
    position: absolute;
    left: 0;
    top: 0;
    z-index: 99;
    width: 100%;
    height: 100%;
    background: #fff no-repeat url("http://static.udache.com/passenger/apps/price/estimate/images/loading.1d8e67c.gif") center center;
    background-size: 2.1rem auto;
}

.mod-p-price-rule .p-main {
    position: relative;
    box-sizing: border-box;
    height: 100%;
    background-color: #fdfdfc;
}

.mod-p-price-rule .p-main-content-wrap {
    position: absolute;
    left: 0;
    top: 0;
    bottom: 0;
    z-index: 1;
    width: 100%;
    overflow: auto;
    -webkit-overflow-scrolling: touch;
}

.mod-p-price-rule .content {
    position: relative;
    height: 100%;
}

.mod-p-price-rule .header-bg {
    position: absolute;
    left: 0;
    top: 0;
    z-index: -1;
    width: 100%;
    height: 5.7rem;
    background-size: 100% 100%!important;
    background-color: #f2f2f3;
    transition: 300ms;
}

.mod-p-price-rule .car-type-info {
    width: 100%;
    height: 100%;
    overflow: hidden;
}

.mod-p-price-rule .swiper-container {
    position: relative;
    z-index: 1;
}

.mod-p-price-rule .swiper-wrapper {
    height: 100%;
}

.mod-p-price-rule .swiper-slide {
    position: relative;
    float: left;
    display: flex;
    flex-direction: column;
    height: 100%;
}

.mod-p-price-rule .price-rule-scroller {
    opacity: 1;
    transition: opacity 0.1s ease-in;
    overflow-y: auto;
    -webkit-overflow-scrolling: touch;
    flex: 1;
}

.mod-p-price-rule .price-rule-scroller.show-scroller {
    opacity: 1;
}

.mod-p-price-rule .car-type-combo-info-wrap {
    position: relative;
    margin-top: 2.5rem;
    padding-bottom: 2rem;
}

.mod-p-price-rule .car-type-combo-info-wrap:after {
    content: '';
    position: absolute;
    left: 0;
    bottom: 0;
    z-index: 1;
    display: block;
    width: 100%;
    height: 1px;
    background-color: rgba(0,0,0,0.1);
    transform-origin: 0 100%;
    transform: scaleY(0.5);
}

.mod-p-price-rule .car-type-combo-info-wrap:first-child {
    margin-top: 0rem;
}

.mod-p-price-rule .car-type-combo-info-wrap:last-child:after {
    display: none;
}

.mod-p-price-rule .car-level-name-wrap {
    height: 0.7rem;
    text-align: center;
    margin-bottom: 1.35rem;
}

.mod-p-price-rule .car-level-name-wrap .car-level-name {
    display: block;
    font-size: 10px;
    line-height: 14px;
    color: #999;
}

.mod-p-price-rule .tab-date-wrap {
    margin: 0 3rem 1rem;
}

.mod-p-price-rule .tab-date-wrap .tab-date {
    display: flex;
    border: solid 1px #4a4c5b;
    font-size: 12px;
    border-radius: 0.1rem;
}

.mod-p-price-rule .tab-date-wrap .tab-date .tab-li.active {
    background: #4a4c5b;
    color: #fff;
}

.mod-p-price-rule .tab-date-wrap .tab-date .tab-li {
    flex: 1;
    text-align: center;
    height: 25px;
    line-height: 25px;
    color: #4a4c5b;
    border-right: solid 1px #4a4c5b;
}

.mod-p-price-rule .tab-date-wrap .tab-date .tab-li:last-child {
    border-right: none;
}

.mod-p-price-rule .tab-date-wrap .custom-des {
    font-size: 11px;
    color: #b0b0b0;
    text-align: center;
    padding-top: 0.8rem;
}

.mod-p-price-rule .price-rule-detail .price-rule-detail-wrap>view {
    position: relative;
    margin-top: 2.5rem;
    padding-bottom: 2rem;
}

.mod-p-price-rule .price-rule-detail .price-rule-detail-wrap>view:first-child {
    margin-top: 0rem;
}

.mod-p-price-rule .price-rule-detail .price-rule-detail-wrap>view:after {
    content: '';
    position: absolute;
    left: 0;
    bottom: 0;
    z-index: 1;
    display: block;
    width: 100%;
    height: 1px;
    background-color: rgba(0,0,0,0.1);
    transform-origin: 0 100%;
    transform: scaleY(0.5);
}

.mod-p-price-rule .price-rule-detail .price-rule-detail-wrap>view:last-child:after {
    display: none;
}

.mod-p-price-rule .price-rule-detail .items {
    margin: 0 3rem;
}

.mod-p-price-rule .price-rule-detail .item {
    margin-top: 0.8rem;
}

.mod-p-price-rule .price-rule-detail .item .t {
    display: flex;
}

.mod-p-price-rule .price-rule-detail .item .t>view {
    display: block;
    font-size: 14px;
    line-height: 20px;
    color: #999;
}

.mod-p-price-rule .price-rule-detail .item .t.b>view {
    color: #666;
}

.mod-p-price-rule .price-rule-detail .item .t>view:last-child {
    flex: 1;
    text-align: right;
}

.mod-p-price-rule .price-rule-detail .item .t>view:first-child {
    text-align: left;
}

.mod-p-price-rule .price-rule-detail .item .d {
    position: relative;
    padding: 0.2rem 0 0.8rem;
    font-size: 12px;
    line-height: 16px;
    color: #999;
}

.mod-p-price-rule .price-rule-detail .item .d .date-part-wrap {
    display: flex;
    height: 0.7rem;
    align-items: center;
}

.mod-p-price-rule .price-rule-detail .item .d .date-part-wrap {
    padding-top: 0.25rem;
}

.mod-p-price-rule .price-rule-detail .item .d .date-part-wrap .date-part {
    font-style: normal;
    display: block;
    font-size: 11px;
}

.mod-p-price-rule .price-rule-detail .item .d .date-part-wrap .date-part:last-child {
    flex: 1;
    text-align: right;
}

.mod-p-price-rule .price-rule-detail .item .d:after {
    content: '';
    position: absolute;
    left: 0;
    bottom: 0;
    z-index: 1;
    display: block;
    width: 100%;
    height: 1px;
    background-color: rgba(0,0,0,0.1);
    transform-origin: 0 100%;
    transform: scaleY(0.5);
}

.mod-p-price-rule .price-rule-detail .item .d-null {
    padding-bottom: 0;
}

.mod-p-price-rule .price-rule-detail .item .d-null:after {
    display: none;
}

.mod-p-price-rule .price-rule-detail .notice {
    position: relative;
    padding: 0.8rem 3rem 0;
    font-size: 12px;
    line-height: 16px;
    color: #999;
}

.mod-p-price-rule .car-type-img {
    height: 8.2rem;
}

.mod-p-price-rule .car-type-img .car-img {
    display: block;
    margin: auto;
    height: 100%;
    width: 20rem;
}

.mod-p-price-rule .car-type-info-title {
    position: relative;
    margin: 0 3rem;
    line-height: 20px;
    height: 20px;
}

.mod-p-price-rule .car-type-info-title:before {
    content: '';
    position: absolute;
    left: 0;
    top: 50%;
    z-index: 1;
    display: block;
    width: 100%;
    height: 1px;
    background: rgba(0,0,0,0.1);
    transform-origin: 0 0;
    transform: scaleY(0.5);
    transform: translateY(-50%);
    -webkit-transform: translateY(-50%);
    -moz-transform: translateY(-50%);
}

.mod-p-price-rule .car-type-info-title text {
    position: absolute;
    left: 50%;
    top: 0;
    z-index: 2;
    display: inline-block;
    padding: 0 0.5rem;
    font-size: 14px;
    line-height: 20px;
    color: #666;
    letter-spacing: 2px;
    white-space: nowrap;
    background-color: #fdfdfc;
    transform: translateX(-50%);
}

.mod-p-price-rule .wx-swiper-dots {
    position: absolute;
    top: 9.2rem;
    bottom: 28rem;
    z-index: 1;
}

.mod-p-price-rule .wx-swiper-dots .wx-swiper-dot {
    width: 4px;
    height: 4px;
    margin-right: 4px;
}

.mod-p-price-rule .sub-fee-items {
    display: flex;
}

.mod-p-price-rule .sub-fee-items .left {
    flex: 1;
}