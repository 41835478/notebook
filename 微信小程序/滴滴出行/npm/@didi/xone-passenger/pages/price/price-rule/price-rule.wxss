page {
    font-size: 20px;
}

.mod-p-price-rule .container {
    position: absolute;
    left: 0;
    top: 0;
    bottom: 0;
    width: 100%;
    height: 100%;
    overflow: hidden;
}

.mod-p-price-rule .mod-detail {
    background: #fff;
    position: absolute;
    top: 0;
    left: 0;
    z-index: 100;
    width: 100%;
    height: 100%;
    overflow-y: scroll;
    -webkit-overflow-scrolling: touch;
}

.mod-p-price-rule .mod-detail .mod-header {
    transform: translate3d(0,0,0);
    min-height: 184px;
}

.mod-p-price-rule .mod-detail .mod-header .header-img {
    width: 100%;
}

.mod-p-price-rule .mod-detail .mod-header .header-img .bg {
    position: absolute;
    left: 0;
    top: 0;
    z-index: -1;
    width: 100%;
    height: 114px;
    background-size: 100% 100%!important;
    background-color: #f2f2f3;
}

.mod-p-price-rule .mod-detail .mod-header .header-img image {
    width: 100%;
    display: block;
    height: 164px;
}

.mod-p-price-rule .mod-detail .mod-header .header-title {
    font-size: 14px;
    color: #666;
    line-height: 20px;
    text-align: center;
    padding: 0px 5 px;
}

.mod-p-price-rule .mod-detail .mod-date-tab {
    padding: 16px 0 20px;
}

.mod-p-price-rule .mod-detail .mod-date-tab .day-lists {
    display: flex;
    justify-content: center;
}

.mod-p-price-rule .mod-detail .mod-date-tab .day-lists .day-item {
    width: 80px;
    height: 40px;
    display: flex;
    align-items: center;
    background: rgba(0,0,0,0.04);
}

.mod-p-price-rule .mod-detail .mod-date-tab .day-lists .day-item .itemspan {
    position: relative;
    display: block;
    color: #ccc;
    font-size: 12px;
    text-align: center;
    width: 80px;
    height: 20px;
    line-height: 20px;
}

.mod-p-price-rule .mod-detail .mod-date-tab .day-lists .day-item .itemspan.active {
    color: #666;
}

.mod-p-price-rule .mod-detail .mod-date-tab .day-lists .day-list-item:first-child .day-item {
    border-top-left-radius: 27.5px;
    border-bottom-left-radius: 27.5px;
}

.mod-p-price-rule .mod-detail .mod-date-tab .day-lists .day-list-item:first-child .day-item .itemspan:after {
    content: '';
    position: absolute;
    right: 0;
    top: 0;
    width: 1px;
    height: 20px;
    background: rgba(0,0,0,0.1);
    transform: scaleX(0.5);
}

.mod-p-price-rule .mod-detail .mod-date-tab .day-lists .day-list-item:last-child .day-item {
    border-top-right-radius: 27.5px;
    border-bottom-right-radius: 27.5px;
}

.mod-p-price-rule .mod-detail .mod-date-tab .day-lists .day-list-item:last-child .day-item span:after {
    display: none;
}

.mod-p-price-rule .mod-detail .mod-date-tab .day-lists .period-item {
    font-size: 11px;
    color: #b0b0b0;
    text-align: center;
    padding-top: 16px;
}

.mod-p-price-rule .mod-detail .mod-content .vehicle-lists {
    position: relative;
    padding: 40px 60px;
}

.mod-p-price-rule .mod-detail .mod-content .vehicle-lists:after {
    content: "";
    position: absolute;
    left: 0;
    bottom: 0;
    z-index: 1;
    display: block;
    width: 100%;
    height: 1px;
    background-color: rgba(0,0,0,0.1);
    transform-origin: 0 100%;
    transform: scaleY(0.5);
}

.mod-p-price-rule .mod-detail .mod-content .vehicle-lists:first-child {
    padding-top: 0;
}

.mod-p-price-rule .mod-detail .mod-content .vehicle-lists .fee-lists .fee-item .fee-name-item {
    font-size: 14px;
    color: #666;
    margin-top: 16px;
    line-height: 20px;
}

.mod-p-price-rule .mod-detail .mod-content .vehicle-lists .fee-lists .fee-item .date-lists .date-item {
    color: #b0b0b0;
    font-size: 11px;
    margin-top: 5.5px;
    line-height: 17px;
}

.mod-p-price-rule .mod-detail .mod-content .vehicle-lists .fee-lists .fee-item .fee-des-lists {
    position: relative;
    padding-bottom: 16px;
}

.mod-p-price-rule .mod-detail .mod-content .vehicle-lists .fee-lists .fee-item .fee-des-lists .fee-des-item {
    font-size: 11px;
    color: #999;
    line-height: 15px;
    padding-top: 4px;
}

.mod-p-price-rule .mod-detail .mod-content .vehicle-lists .fee-lists .fee-item .fee-des-lists:after {
    content: "";
    position: absolute;
    left: 0;
    bottom: 0;
    z-index: 1;
    display: block;
    width: 100%;
    background-color: rgba(0,0,0,0.1);
    transform-origin: 0 100%;
    transform: scaleY(0.5);
}

.mod-p-price-rule .mod-detail .mod-content .vehicle-lists .fee-lists .fee-item:last-of-type .fee-des-lists {
    padding-bottom: 0;
}

.mod-p-price-rule .mod-detail .mod-content .vehicle-lists .fee-lists .fee-item:last-of-type .fee-des-lists:after {
    display: none;
}

.mod-p-price-rule .mod-detail .mod-content .vehicle-lists .notice-lists {
    position: relative;
    margin-top: 16px;
    padding-top: 16px;
}

.mod-p-price-rule .mod-detail .mod-content .vehicle-lists .notice-lists .notice-item {
    color: #999;
    font-size: 11px;
    line-height: 15px;
}

.mod-p-price-rule .mod-detail .mod-content .vehicle-lists .notice-lists:before {
    content: "";
    position: absolute;
    left: 0;
    top: 0;
    z-index: 1;
    display: block;
    width: 100%;
    height: 1px;
    background-color: rgba(0,0,0,0.1);
    transform-origin: 0 100%;
    transform: scaleY(0.5);
}

.mod-p-price-rule .mod-detail .mod-content .vehicle-lists:last-child:after {
    display: none;
}

.mod-p-price-rule .mod-detail .mod-content .mod-sub-fee-items {
    display: flex;
}

.mod-p-price-rule .mod-detail .mod-content .mod-sub-fee-items>.left {
    flex: 1;
}

.mod-p-price-rule .mod-detail .mod-content .mod-content-title {
    position: relative;
    line-height: 20px;
    height: 20px;
}

.mod-p-price-rule .mod-detail .mod-content .mod-content-title:before {
    content: '';
    position: absolute;
    left: 0;
    top: 50%;
    z-index: 1;
    display: block;
    width: 100%;
    height: 1px;
    background: rgba(0,0,0,0.1);
    transform-origin: 0 0;
    transform: scaleY(0.5);
    transform: translateY(-50%);
    -webkit-transform: translateY(-50%);
    -moz-transform: translateY(-50%);
}

.mod-p-price-rule .mod-detail .mod-content .mod-content-title .titlespan {
    position: absolute;
    left: 50%;
    top: 0;
    z-index: 2;
    display: inline-block;
    padding: 0 10 px;
    font-size: 14px;
    line-height: 20px;
    color: #666;
    letter-spacing: 2px;
    white-space: nowrap;
    background-color: #fdfdfc;
    transform: translateX(-50%);
}