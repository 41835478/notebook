a:link,a:visited,a:hover,a:active {
    font-size: 14px;
    color: #999;
    text-decoration: none;
    margin: 0;
}

.hide {
    display: none;
}

.container {
    height: 100%;
}

#main {
    position: absolute;
    top: 0;
    bottom: 0;
    width: 100%;
    border-radius: 5px;
    font-family: "Heiti SC Light","Heiti SC","Helvetica Neue Thin","Helvetica Neue Light","Helvetica Neue","Microsoft YaHei",arial,sans-serif,'Droid Sans Fallback';
}

#main .swipe {
    width: 100%;
    height: 100%;
    overflow: hidden;
    position: absolute;
    top: 0;
    bottom: 30px;
    display: -webkit-box;
    display: -webkit-flex;
    display: flex;
    -webkit-box-align: center;
    align-items: center;
    position: relative;
    overflow-y: auto;
}

#main .swipe .content-wrap {
    min-height: 180px;
    position: relative;
    overflow: hidden;
    box-sizing: border-box;
    padding: 0;
    margin: auto;
    width: 267px;
    text-align: center;
}

#main .swipe .content-wrap .small-size {
    position: relative;
    padding: 20px 0 10px;
    height: 13px;
    font-size: 14px;
    line-height: 1.1;
    color: #666;
    text-align: center;
}

#main .swipe .content-wrap .small-size:after {
    position: absolute;
    bottom: 16px;
    width: 30%;
    content: '';
    display: block;
    height: 1px;
    right: 0;
    background-color: #ebebeb;
    transform-origin: right 0;
    -webkit-transform-origin: right 0;
    transform: scaleY(0.5);
    -webkit-transform: scaleY(0.5);
}

#main .swipe .content-wrap .small-size:before {
    position: absolute;
    bottom: 16px;
    width: 30%;
    content: '';
    display: block;
    height: 1px;
    background-color: #ebebeb;
    transform-origin: 0 0;
    -webkit-transform-origin: 0 0;
    transform: scaleY(0.5);
    -webkit-transform: scaleY(0.5);
}

#main .swipe .content-wrap .style-fix .detail-list {
    margin-bottom: 0!important;
}

#main .swipe .content-wrap .style-fix .detail-list:after {
    display: none!important;
}

#main .swipe .content-wrap .detail-wrap .total-amount {
    margin-top: 20px;
    color: #333;
}

#main .swipe .content-wrap .detail-wrap .total-amount .n-font-size {
    margin-left: 1px;
    font-size: 14px;
}

#main .swipe .content-wrap .detail-wrap .total-amount .big-size {
    display: inline-block;
    padding-left: 14px;
    font-family: HelveticaNeue-Thin,'Helvetica Neue Thin','Helvetica Neue Light','Helvetica Neue',Helvetica;
    font-weight: lighter;
    font-size: 49px;
    line-height: 49px;
    height: 49px;
}

#main .swipe .content-wrap .detail-wrap .total-amount .n-font-size+.big-size {
    padding-left: 6px;
}

#main .swipe .content-wrap .detail-wrap .detail-list {
    margin-top: 19.5px;
    width: 100%;
    list-style: none;
    position: relative;
    padding-bottom: 15px;
}

#main .swipe .content-wrap .detail-wrap .detail-list:after {
    position: absolute;
    width: 100%;
    content: '';
    display: block;
    bottom: 0;
    height: 1px;
    background-color: #ebebeb;
    transform-origin: right 0;
    -webkit-transform-origin: right 0;
    transform: scaleY(0.5);
    -webkit-transform: scaleY(0.5);
}

#main .swipe .content-wrap .detail-wrap .detail-list .li .li-whole-item {
    font-size: 14px;
    line-height: 29px;
    overflow: hidden;
    display: flex;
    justify-content: space-between;
}

#main .swipe .content-wrap .detail-wrap .detail-list .li .li-detail-items {
    background: #f8f8f8;
    position: relative;
    margin-bottom: 5px;
    margin-top: 5px;
    padding: 0 14px;
}

#main .swipe .content-wrap .detail-wrap .detail-list .li .li-detail-item {
    line-height: 29px;
    font-size: 12px;
    color: #999;
    overflow: hidden;
    display: flex;
    justify-content: space-between;
}

#main .swipe .content-wrap .detail-wrap .detail-list .li .li-detail-item:before {
    content: '';
    width: 0;
    height: 0;
    font-size: 0;
    border-width: 5px;
    border-style: solid;
    border-color: transparent transparent #f8f8f8;
    overflow: hidden;
    position: absolute;
    left: 20px;
    top: -10px;
}

#main .swipe .content-wrap .detail-wrap .detail-list .li:first-child {
    margin-bottom: 0;
}

#main .swipe .content-wrap .detail-wrap .detail-list .li:last-child {
    margin-bottom: 0;
}

#main .swipe .content-wrap .detail-wrap .detail-list .li .left {
    padding-right: 10px;
    text-align: left;
}

#main .swipe .content-wrap .detail-wrap .detail-list .li .right {
    text-align: right;
}

#main .swipe .content-wrap .detail-wrap .oneprice {
    font-size: 12px!important;
    line-height: 22px!important;
    color: #999!important;
    padding-left: 2px;
    padding-right: 2px;
}

#main .swipe .content-wrap .detail-wrap .onprice:first-child {
    margin-top: 2px;
}

#main .swipe .content-wrap .detail-wrap .oneprice:last-child {
    margin-bottom: 2px;
}

#main .swipe .content-wrap .estimate-fee-tips {
    margin-top: 10px;
    margin-bottom: 15px;
    display: block;
    line-height: 18px;
    font-size: 13px;
    text-align: justify;
    box-sizing: border-box;
    color: #9b9b9b;
}

#main .swipe .content-wrap .act-info {
    margin-top: 20px;
}

#main .swipe .content-wrap .act-info .act-info-inner {
    display: inline-block;
    margin: auto;
    padding-left: 29.4px;
    transform-origin: 50% 0;
    -webkit-transform-origin: 50% 0;
    transform: scale(0.83333333);
    -webkit-transform: scale(0.83333333);
    background: url("data:image/gif;base64,") left center no-repeat;
    color: #878787;
    background-size: 24px auto;
}

#main .swipe .content-wrap .act-info .act-info-inner:before {
    content: "";
    background-size: contain;
}

#main .swipe .content-wrap .act-info .act-info-inner .act-info-title,#main .swipe .content-wrap .act-info .act-info-inner .act-info-msg {
    display: block;
    font-size: 12px!important;
    line-height: 1.5;
    text-align: left;
}

#main .swipe .content-wrap .act-info .act-info-inner .act-info-msg {
    padding-right: 9.6px;
    background: url("data:image/png;base64,") right center no-repeat;
    background-size: 6px auto;
}

#main .swipe .content-wrap #cartype-price-rule-detail {
    font-size: 12px;
    line-height: 1;
    color: #666;
    padding-right: 9px;
}

#main .swipe .content-wrap #cartype-price-rule-detail:after {
    background: url("data:image/png;base64,iVBORw0KGgoAAAANSUhEUgAAAAoAAAASCAQAAADIvofAAAAAP0lEQVQYV2PYzrydmQEdbL+0/dZ2OXTBg9v/b7+LJrydf/tx7MInqCV8D7vw7e0s+AWxaKemEK4AwRp0WAIZAE1XYnLUE4uHAAAAAElFTkSuQmCC") right center no-repeat;
    background-size: contain;
    display: inline-block;
    width: 9px;
    height: 9px;
    content: "";
}

#main .swipe .content-wrap #cartype-price-rule-detail:active {
    color: #333;
}

#main .swipe .price-tips {
    width: 317px;
    margin-top: 10px;
    margin-left: -25px;
}

#main .swipe .price-remark {
    color: #999;
    font-size: 14px;
    line-height: 1.5em;
    transform-origin: 50% 50% 50%;
    text-align: left;
    width: 317px;
    margin: 0 auto;
    transform: scale(0.83);
    -webkit-transform: scale(0.83);
}

#main .swipe .price-remark .text {
    width: 100%;
}

#main .swipe .price-rule {
    width: 100%;
    font-size: 12px;
    color: #666;
    text-align: center;
    margin-top: 10px;
    vertical-align: middle;
}

#main .swipe .price-rule .text {
    vertical-align: middle;
    display: inline-block;
    height: 18px;
    line-height: 18px;
}

#main .swipe .price-rule .mfic-click {
    vertical-align: middle;
    display: inline-block;
    height: 18px;
    line-height: 18px;
}

#main .swipe .price-rule .mfic-click:before {
    font-size: 18px;
}

.p-loading {
    position: absolute;
    left: 0;
    top: 0;
    z-index: 99;
    width: 100%;
    height: 100%;
    background: #fff no-repeat url("http://static.udache.com/passenger/apps/price/estimate/images/loading.1d8e67c.gif") center center;
    background-size: 42px auto;
}

.p-error {
    position: absolute;
    left: 0;
    top: 0;
    z-index: 99;
    display: flex;
    align-items: center;
    justify-content: center;
    width: 100%;
    height: 100%;
    font-size: 14px;
    line-height: 20px;
    color: #ff8a01;
    background-color: #fff;
}

@media screen and (-webkit-min-device-pixel-ratio:3) {
    .detail-list:after {
        height: 1px;
        transform: scaleY(0.333333333333);
        -webkit-transform: scaleY(0.333333333333);
    }
}