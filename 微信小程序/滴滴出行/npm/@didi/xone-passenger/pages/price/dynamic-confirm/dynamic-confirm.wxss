.mod-p-dynamic-confirm .main {
    font-family: "PingFangSC-Regular","STHeitiSC-Medium","HelveticaNeue-Medium","Microsoft YaHei",arial,sans-serif;
    position: absolute;
    width: 100%;
    height: 100%;
    overflow: auto;
    -webkit-overflow-scrolling: touch;
}

.mod-p-dynamic-confirm .main em {
    color: #ff8903;
    font-style: normal;
}

.mod-p-dynamic-confirm .main .em {
    color: #ff8903;
    font-style: normal;
}

.mod-p-dynamic-confirm .main #container {
    box-sizing: border-box;
    display: flex;
    flex-direction: column;
    width: 100%;
    min-height: 100%;
    background-color: #fff;
}

.mod-p-dynamic-confirm .main #container .header {
    text-align: center;
}

.mod-p-dynamic-confirm .main #container .h1 {
    padding-top: 53px;
    font-size: 25px;
    color: #333;
}

.mod-p-dynamic-confirm .main #content {
    padding-top: 6px;
    text-align: left;
}

.mod-p-dynamic-confirm .main #content .desc {
    font-size: 14px;
    line-height: 17px;
    text-align: center;
    padding: 0 25px;
    color: #666;
}

.mod-p-dynamic-confirm .main #content .org {
    color: #ff8903;
}

.mod-p-dynamic-confirm .main #content .big {
    font-size: 22px;
}

.mod-p-dynamic-confirm .main #content .price-info {
    margin: 22px auto 0;
    width: 97%;
    min-height: 258px;
    line-height: 1;
    text-align: center;
    color: #333;
    display: block;
    background: url("https://static.udache.com/passenger/apps/price/dynamic-confirm/images/icon-center@2x.d47eb92.png") no-repeat;
    background-size: 100% 100%;
}

.mod-p-dynamic-confirm .main #content .price-info .ratio-text {
    padding-top: 58px;
    font-size: 17px;
    line-height: 22px;
    color: #666;
}

.mod-p-dynamic-confirm .main #content .price-info .ratio-detail {
    padding-top: 24px;
    font-size: 0;
}

.mod-p-dynamic-confirm .main #content .price-info .ratio-detail text {
    display: inline-block;
}

.mod-p-dynamic-confirm .main #content .price-info .ratio-detail .des {
    font-size: 12px;
    color: #666;
    padding-right: 4px;
}

.mod-p-dynamic-confirm .main #content .price-info .ratio-detail .bg {
    text-indent: 1000px;
    overflow: hidden;
    background: no-repeat url("data:image/png;base64,") left bottom;
    background-size: 14px auto;
}

.mod-p-dynamic-confirm .main #content .price-info .ratio-detail .signal {
    position: relative;
    font-size: 22px;
    line-height: 18px;
    width: 18px;
    color: #fa8f54;
}

.mod-p-dynamic-confirm .main #content .price-info .ratio-detail .value {
    font-family: "PingFangSC-Medium","STHeitiSC-Medium","HelveticaNeue-Medium","Microsoft YaHei",arial,sans-serif;
    font-size: 50px;
    color: #fc9153;
    line-height: 13px;
}

.mod-p-dynamic-confirm .main #content .price-info .ratio-detail .unit {
    padding-left: 3px;
    font-size: 16px;
    color: #fa8f54;
}

.mod-p-dynamic-confirm .main .price {
    margin: 0 auto;
    padding-top: 25px;
    font-size: 17px;
    line-height: 19px;
    width: 72%;
    text-align: center;
    color: #666;
}

.mod-p-dynamic-confirm .main .member-message {
    margin: 0 auto;
    font-size: 14px;
    color: #393939;
    line-height: 20px;
    width: 260px;
    text-align: center;
}

.mod-p-dynamic-confirm .main .price .text {
    display: block;
}

.mod-p-dynamic-confirm .main .ratio-detail .span2 {
    font-family: "PingFangSC-Medium","STHeitiSC-Medium","HelveticaNeue-Medium","Microsoft YaHei",arial,sans-serif;
    font-size: 50px;
    color: #fa8f54;
    line-height: 40px;
}

.mod-p-dynamic-confirm .main .ratio-detail .span3 {
    padding-left: 4px;
    font-size: 16px;
    line-height: 1;
    color: #fa8f54;
}

.mod-p-dynamic-confirm .main .tips {
    font-size: 13px;
    text-align: center;
    color: #999;
    padding-bottom: 10px;
}

.mod-p-dynamic-confirm .main .tip-icon {
    width: 0.7rem;
    height: 0.7rem;
    display: inline-block;
    padding: 0.1rem 0.3rem 0 0;
    background: url("data:image/png;base64,") no-repeat;
    background-size: 0.7rem 0.7rem;
    line-height: 0.7rem;
    vertical-align: middle;
}

.mod-p-dynamic-confirm .main .footer {
    padding: 10px;
}

.mod-p-dynamic-confirm .main #agree,.mod-p-dynamic-confirm .main #wait,.mod-p-dynamic-confirm .main #guide {
    position: relative;
    display: block;
    width: 91%;
    height: 48px;
    line-height: 48px;
    font-size: 18px;
    border: none;
}

.mod-p-dynamic-confirm .main #agree {
    margin: auto;
    color: #fff;
    background-color: #4a4c5b;
    border-radius: 2px;
}

.mod-p-dynamic-confirm .main #wait,.mod-p-dynamic-confirm .main #guide {
    margin-top: 10px;
    color: #333;
    border-radius: 2px;
    background: #fff;
}

.mod-p-dynamic-confirm .main #wait:after {
    border-radius: 2px;
}