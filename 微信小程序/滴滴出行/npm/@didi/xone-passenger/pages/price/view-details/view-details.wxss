.mod-p-view-details {
    font-size: 20px;
}

.mod-p-view-details .container {
    position: absolute;
    left: 0;
    top: 0;
    z-index: 0;
    width: 100%;
    height: 100%;
    overflow: auto;
    background: #fff;
    -webkit-overflow-scrolling: touch;
    font-family: 'PingFangSC-Light',HelveticaNeue-Thin,'Helvetica Neue Thin','Helvetica Neue Light','Helvetica Neue',Helvetica;
}

.mod-p-view-details a:link,.mod-p-view-details a:visited,.mod-p-view-details a:hover,.mod-p-view-details a:active {
    font-size: 14px;
    color: #999;
    text-decoration: none;
    margin: 0;
}

.pay-fee-description {
    font-size: 11px;
    color: #999;
    padding: 1rem 0 0 0;
    line-height: 18px;
    text-align: center;
}

.hide {
    display: none;
}

.main {
    box-sizing: border-box;
    background: #fff;
    padding: 5rem 0;
}

.main .content-wrap {
    position: relative;
    text-align: center;
}

.main .content-wrap .small-size {
    position: relative;
    font-size: 0.7rem;
    color: #999;
    text-align: center;
    margin: auto;
}

.main .content-wrap .small-size .small-size-line:before {
    position: absolute;
    left: 0;
    top: 1rem;
    content: '';
    height: 1px;
    border-bottom: solid 1px #ccc;
    width: 100%;
    transform: scaleY(0.5);
    -webkit-transform: scaleY(0.5);
}

.main .content-wrap .small-size .small-size-txt {
    background: #fff;
    padding: 0 0.5rem;
    position: relative;
}

.main .content-wrap .small-size-common {
    margin-top: 2rem;
}

.main .content-wrap .detail-wrap {
    box-sizing: border-box;
    padding: 0;
    margin: auto;
    width: 267px;
}

.main .content-wrap .detail-wrap .total-amount {
    margin-top: 2.5rem;
    color: #333;
}

.main .content-wrap .detail-wrap .total-amount .n-font-size {
    margin-left: 1px;
    font-size: 14px;
}

.main .content-wrap .detail-wrap .total-amount .big-size {
    display: inline-block;
    font-size: 40px;
    line-height: 40px;
    height: 40px;
    color: #333;
}

.main .content-wrap .detail-wrap .detail-list {
    padding-top: 0.8rem;
    width: 100%;
    list-style: none;
    position: relative;
}

.main .content-wrap .detail-wrap .detail-list .li .li-whole-item {
    line-height: 20px;
    font-size: 14px;
    color: #333;
    width: 100%;
    padding-top: 0.5rem;
    height: 20px;
}

.main .content-wrap .detail-wrap .detail-list .li .li-detail-item-wrap {
    background: #f8f8f8;
    position: relative;
    margin-top: 10px;
}

.main .content-wrap .detail-wrap .detail-list .li .org_bot_cor {
    width: 0;
    height: 0;
    font-size: 0;
    border-width: 5px;
    border-style: solid;
    border-color: transparent transparent #f8f8f8;
    overflow: hidden;
    position: absolute;
    left: 10px;
    top: -10px;
}

.main .content-wrap .detail-wrap .detail-list .li .li-content-wrap {
    box-sizing: border-box;
    padding: 0.7rem;
}

.main .content-wrap .detail-wrap .detail-list .li .li-detail-item {
    line-height: 14px;
    height: 12px;
    font-size: 12px;
    color: #999;
    width: 100%;
    padding-bottom: 0.3rem;
}

.main .content-wrap .detail-wrap .detail-list .li .li-detail-item:last-child {
    padding-bottom: 0;
}

.main .content-wrap .detail-wrap .detail-list .li .left {
    float: left;
    width: 190px;
    text-align: left;
    overflow: hidden;
    text-overflow: ellipsis;
    white-space: nowrap;
}

.main .content-wrap .detail-wrap .detail-list .li .right {
    float: right;
}

.main .to-rule {
    position: relative;
    font-size: 11px;
    color: #999;
    text-align: center;
    margin: 0 auto;
    height: 15px;
    line-heignt: 15px;
    margin: 0.8rem 0;
}

.main .to-rule .border_corl {
    width: 0.2rem;
    height: 0.2rem;
    position: absolute;
    top: 50%;
    margin-top: -0.15rem;
    margin-left: 0.1rem;
    border-left: 1px solid #fff;
    border-top: 1px solid #999;
    border-bottom: 1px solid #fff;
    border-right: 1px solid #999;
    transform: rotate(45deg);
    -webkit-transform: rotate(45deg);
}

@media screen and (-webkit-min-device-pixel-ratio:2) {
    .small-size-line:before {
        transform: scaleY(0.5);
        -webkit-transform: scaleY(0.5);
    }
}

@media screen and (-webkit-min-device-pixel-ratio:3) {
    .small-size-line:before {
        transform: scaleY(0.333333333333);
        -webkit-transform: scaleY(0.333333333333);
    }
}