.mod-p-cancel-reason {
    width: 100%;
    height: 100%;
}

.mod-p-cancel-reason .container {
    position: absolute;
    left: 0;
    top: 0;
    z-index: 0;
    width: 100%;
    height: 100%;
    text-align: center;
    overflow: auto;
    -webkit-overflow-scrolling: touch;
    box-sizing: border-box;
    padding: 0 20px;
}

.mod-p-cancel-reason .hide {
    display: none!important;
}

.mod-p-cancel-reason .header {
    position: relative;
    padding: 1.55rem 3.05rem 0.95rem;
}

.mod-p-cancel-reason .header .txt {
    font-size: 17px;
    color: #000;
    text-align: center;
    padding-bottom: 6px;
}

.mod-p-cancel-reason .header .txt-sec {
    font-size: 14px;
    color: #999;
    line-height: 20px;
}

.mod-p-cancel-reason .content {
    position: relative;
    -webkit-box-flex: 1;
    flex: 1;
}

.mod-p-cancel-reason .ul {
    position: relative;
}

.mod-p-cancel-reason .li {
    position: relative;
    padding: 0;
    text-align: left;
}

.mod-p-cancel-reason .footer:before {
    content: '';
    display: block;
    position: absolute;
    left: 0;
    top: 0;
    width: 100%;
    height: 1px;
    background-color: #ebebeb;
    transform-origin: 0 0;
    -webkit-transform-origin: 0 0;
    transform: scaleY(1);
    -webkit-transform: scaleY(1);
}

.mod-p-cancel-reason .li:after {
    content: '';
    display: block;
    position: absolute;
    left: 0;
    bottom: 0;
    width: 100%;
    height: 1px;
    background-color: #ebebeb;
    transform-origin: 0 100%;
    -webkit-transform-origin: 0 100%;
    transform: scaleY(1);
    -webkit-transform: scaleY(1);
}

.mod-p-cancel-reason .li-txt {
    display: inline-block;
    font-size: 14px;
    padding: 13px 0;
    color: #666;
    vertical-align: middle;
    width: 100%;
    box-sizing: border-box;
}

.mod-p-cancel-reason .circle {
    display: inline-block;
    position: absolute;
    right: 0;
    width: 16px;
    height: 42px;
    vertical-align: middle;
    background: no-repeat url("data:image/png;base64,") left center;
    background-size: 16px auto;
}

.mod-p-cancel-reason .selected>.circle {
    background: no-repeat url("data:image/png;base64,") left center;
    background-size: 16px auto;
}

.mod-p-cancel-reason .footer {
    position: relative;
    z-index: 10;
    padding: 8.2rem 0 0.5rem 0;
}

.mod-p-cancel-reason .more {
    position: relative;
    height: 2.5rem;
    font-size: 0;
}

.mod-p-cancel-reason .more-txt {
    position: absolute;
    left: 50%;
    top: 50%;
    display: block;
    padding-right: 20px;
    font-size: 14px;
    line-height: 0.9rem;
    color: #999;
    transform: translate(-50%,-50%);
}

.mod-p-cancel-reason .more-sign {
    width: 5px;
    height: 5px;
    position: absolute;
    top: 50%;
    margin-top: -5px;
    margin-left: 48px;
    border-left: 1px solid #fff;
    border-top: 1px solid #fff;
    border-bottom: 1px solid #999;
    border-right: 1px solid #999;
    transform: rotate(45deg);
    -webkit-transform: rotate(45deg);
}

.mod-p-cancel-reason .btn-submit {
    font-size: 18px;
    line-height: 47px;
    color: #fff;
    background-color: #ccc;
    border-radius: 2px;
    height: 47px;
}

.mod-p-cancel-reason .bs-active {
    background-color: #4a4c5b;
}

.mod-p-cancel-reason .bs-disabled {
    background-color: #ccc!important;
}

@media screen and (-webkit-device-pixel-ratio:1.5),screen and (device-pixel-ratio:1.5) {
    .mod-p-cancel-reason .footer:before,.mod-p-cancel-reason .li:after {
        transform: scaleY(0.66666667);
        -webkit-transform: scaleY(0.66666667);
    }
}

@media screen and (-webkit-device-pixel-ratio:2),screen and (device-pixel-ratio:2) {
    .mod-p-cancel-reason .footer:before,.mod-p-cancel-reason .li:after {
        transform: scaleY(0.5);
        -webkit-transform: scaleY(0.5);
    }
}

@media screen and (-webkit-min-device-pixel-ratio:3),screen and (min-device-pixel-ratio:3) {
    .mod-p-cancel-reason .footer:before,.mod-p-cancel-reason .li:after {
        transform: scaleY(0.333333333);
        -webkit-transform: scaleY(0.333333333);
    }

    .mod-p-cancel-reason .dialog-icon:before {
        background: no-repeat url("data:image/png;base64,") center center;
    }
}