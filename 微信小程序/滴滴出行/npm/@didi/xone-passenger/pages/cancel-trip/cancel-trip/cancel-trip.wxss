.mod-p-cancel-trip .container {
    font-size: 20px;
    position: absolute;
    left: 0;
    top: 0;
    z-index: 0;
    width: 100%;
    height: 100%;
    overflow: auto;
    -webkit-overflow-scrolling: touch;
    text-align: center;
    background-color: #fff;
}

.mod-p-cancel-trip canvas {
    display: block;
    margin: 0 auto;
}

.mod-p-cancel-trip .em {
    color: #ff8903;
    font-style: normal;
}

.mod-p-cancel-trip .content {
    box-sizing: border-box;
    padding: 50px 0 15px 0;
    height: 100%;
    position: relative;
}

.mod-p-cancel-trip .bottom {
    width: 6rem;
    text-align: center;
    margin: 0 auto;
    padding: 4.185rem 0 0 0;
}

.mod-p-cancel-trip .main-cont {
    box-sizing: border-box;
    padding: 0 0;
    width: 100%;
}

.mod-p-cancel-trip .icon {
    display: block;
    margin: auto;
    width: 240px;
    height: 172px;
    background-size: contain!important;
}

.mod-p-cancel-trip .text-1 {
    padding: 0 2.675rem;
    font-size: 20px;
    line-height: 28px;
    color: #333;
}

.mod-p-cancel-trip .text-2 {
    padding: 0 2rem;
    margin: 0.55rem 0 2.475rem 0;
    font-size: 14px;
    line-height: 20px;
    color: #999;
}

.mod-p-cancel-trip .strive {
    position: relative;
    height: 3.8rem;
    line-height: 3.8rem;
    width: 10.45rem;
    font-size: 44px;
    color: #fc9153;
    font-family: "PingFangSC-Medium";
    text-align: center;
    margin: 0 auto;
    display: inline-block;
}

.mod-p-cancel-trip .strive:after {
    content: " ";
    position: absolute;
    left: 0;
    top: 0;
    width: 200%;
    height: 200%;
    border: 2px solid #fc9153;
    border-radius: 20px;
    transform-origin: 0 0;
    -webkit-transform-origin: 0 0;
    transform: scale(0.5,0.5);
    -webkit-transform: scale(0.5,0.5);
}

.mod-p-cancel-trip .distance {
    font-size: 14px;
    color: #999;
    margin: 0 auto;
    padding: 0.75rem 2.025rem 0 2.025rem;
}

.mod-p-cancel-trip .button-group {
    width: 100%;
    box-sizing: border-box;
    padding: 0.8rem 0.5rem 0.5rem 0.5rem;
}

.mod-p-cancel-trip .wait,.mod-p-cancel-trip .confirm {
    height: 47px;
    font-family: "PingFangSC-Regular","PingFangSC-Medium","STHeitiSC-Medium","HelveticaNeue-Medium","Microsoft YaHei","Arial","sans-serif";
    font-size: 18px;
    line-height: 47px;
    color: #000;
}

.mod-p-cancel-trip .wait {
    position: relative;
    color: #4a4c5b;
}

.mod-p-cancel-trip .wait:after {
    content: "  ";
    position: absolute;
    left: 0;
    top: 0;
    width: 200%;
    height: 200%;
    border: 1px solid #ccc;
    border-radius: 4px;
    transform-origin: 0 0;
    -webkit-transform-origin: 0 0;
    transform: scale(0.5,0.5);
    -webkit-transform: scale(0.5,0.5);
}

.mod-p-cancel-trip .confirm {
    margin: 0.5rem 0;
    color: #fff;
    background: #4a4c5b;
    border-radius: 2px;
}

.mod-p-cancel-trip .rule-text {
    display: inline-block;
    font-size: 13px;
    color: #999;
    vertical-align: middle;
    text-align: center;
    padding-left: 0.75rem;
    line-height: 13px;
    height: 13px;
}

.mod-p-cancel-trip .mfic-click {
    display: inline-block;
    font-size: 16px;
    color: #999;
    vertical-align: middle;
    line-height: 22px;
}

@media screen and (-webkit-min-device-pixel-ratio:2) {
    .mod-p-cancel-trip .confirm:after,.mod-p-cancel-trip .wait:after {
        transform-origin: 0 0;
        -webkit-transform-origin: 0 0;
        transform: scale(0.5,0.5);
        -webkit-transform: scale(0.5,0.5);
    }
}