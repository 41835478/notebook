.suggestlist {
    width: 100%;
    height: 100vh;
    background-color: #fff;
}

.suggestlist .split {
    position: relative;
    z-index: 10;
    width: 100%;
    height: 10px;
    background: #f3f4f5;
}

.suggestlist .list-wrapper {
    width: 100%;
    height: 90vh;
    margin: 0 auto;
    border-radius: 2px;
    background: #fff;
    box-shadow: 0 1px 3px rgba(0,0,0,.1);
}

.suggestlist .list-wrapper .list-scroll-wrapper {
    width: 100%;
    height: 90vh;
}

.suggestlist .list-wrapper .border-bottom-1px {
    border-bottom: 1rpx solid #ebebeb;
}

.suggestlist .list-wrapper .sugerror {
    text-align: center;
    height: 60px;
    box-sizing: border-box;
}

.suggestlist .list-wrapper .sugerror .title {
    padding: 11px 0 8px;
    font-size: 17px;
    color: #333;
}

.suggestlist .list-wrapper .sugerror .desc {
    font-size: 12px;
    color: #999;
}

.suggestlist .list-wrapper .list-item {
    padding: 0 16px 0 17px;
    border-bottom: 1rpx solid #ebebeb;
}

.suggestlist .list-wrapper .list-item.active,.suggestlist .list-wrapper .list-item:active {
    background: rgba(0,0,0,.04);
}

.suggestlist .list-wrapper .list-item.special .item-wrapper .icon {
    color: #fc9153;
}

.suggestlist .list-wrapper .list-item.special .item-wrapper .icon .mfic-click {
    font-size: 16px;
    color: #999;
}

.suggestlist .list-wrapper .list-item.special .item-wrapper .address-wrapper {
    height: 54px;
}

.suggestlist .list-wrapper .list-item.special .item-wrapper .address-wrapper .address-info .displayname {
    color: #fc9153;
}

.suggestlist .list-wrapper .list-item .item-wrapper {
    display: flex;
    align-items: center;
    font-size: 0;
}

.suggestlist .list-wrapper .list-item .item-wrapper .icon {
    flex: 0 0 22px;
    width: 22px;
    color: #999;
    font-size: 12px;
}

.suggestlist .list-wrapper .list-item .item-wrapper .address-wrapper {
    flex: 1;
    display: flex;
    flex-direction: column;
    justify-content: center;
    overflow: hidden;
    height: 79px;
}

.suggestlist .list-wrapper .list-item .item-wrapper .address-wrapper .address-info {
    overflow: hidden;
    white-space: nowrap;
    text-overflow: ellipsis;
    height: 20px;
    line-height: 20px;
}

.suggestlist .list-wrapper .list-item .item-wrapper .address-wrapper .address-info.noaddress {
    margin-top: 6px;
}

.suggestlist .list-wrapper .list-item .item-wrapper .address-wrapper .address-info .displayname {
    overflow: hidden;
    white-space: nowrap;
    text-overflow: ellipsis;
    font-size: 17px;
    color: #333;
}

.suggestlist .list-wrapper .list-item .item-wrapper .address-wrapper .address-info .recommend {
    display: inline-block;
    vertical-align: top;
    position: relative;
    margin-left: 6px;
    padding: 0 6px;
    border-radius: 2px;
    font-size: 12px;
    color: #fff;
    transform: scale(.8);
    transform-origin: left center;
}

.suggestlist .list-wrapper .list-item .item-wrapper .address-wrapper .address-info .recommend.recommend {
    background-color: #3cbca3;
}

.suggestlist .list-wrapper .list-item .item-wrapper .address-wrapper .address-info .recommend.current {
    background-color: #3ca0e6;
}

.suggestlist .list-wrapper .list-item .item-wrapper .address-wrapper .address {
    margin-top: 5px;
    overflow: hidden;
    white-space: nowrap;
    text-overflow: ellipsis;
    font-size: 14px;
    color: #999;
}

.suggestlist .list-wrapper .list-item .item-wrapper .distance-info {
    flex: 0 0 48px;
    width: 48px;
    margin-top: -14px;
    text-align: right;
}

.suggestlist .list-wrapper .list-item .item-wrapper .distance-info .distance {
    font-size: 12px;
    color: #999;
}

.suggestlist .list-wrapper .list-item .subpoi-wrapper {
    padding-bottom: 20px;
}

.suggestlist .list-wrapper .list-item .subpoi-wrapper .subpoilist .group {
    display: flex;
    margin-bottom: 10px;
}

.suggestlist .list-wrapper .list-item .subpoi-wrapper .subpoilist .group:last-child {
    margin-bottom: 0;
}

.suggestlist .list-wrapper .list-item .subpoi-wrapper .subpoilist .group .item {
    flex: 1;
    margin-right: 15px;
    height: 30px;
    line-height: 30px;
    text-align: center;
    color: #666;
    font-size: 12px;
}

.suggestlist .list-wrapper .list-item .subpoi-wrapper .subpoilist .group .item:last-child {
    margin: 0;
}

.suggestlist .list-wrapper .list-item .subpoi-wrapper .subpoilist .group .item-border {
    box-sizing: border-box;
    border: solid #ccc;
    border-width: 1rpx;
    border-radius: 2px;
    background-color: #fff;
}

.suggestlist .list-wrapper .list-item .subpoi-wrapper .subpoilist .group .item-border:active {
    background-color: rgba(0,0,0,.04);
}

.noresult .warning {
    padding: 120px 0 16px;
}

.noresult .warning .warning-icon {
    width: 50px;
    height: 50px;
    margin: 0 auto;
    border-radius: 50%;
    background-color: #4a4c5b;
}

.noresult .warning .warning-icon .mfic-important {
    display: inline-block;
    margin: -7px 0 0 -7px;
    font-size: 64px;
    color: #f3f4f5;
}

.noresult .desc {
    text-align: center;
    font-size: 17px;
    color: #666;
}

