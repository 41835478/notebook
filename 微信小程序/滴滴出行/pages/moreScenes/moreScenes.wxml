<view class="more-scenes-container">
    <view class="more-scenes-warp">
        <view bindtap="_dispatch" class="scene-item-container" data-bindtap="tabClick({{item.id}})" data-path="moreScenes" wx:for="{{moreScenes.props.moreSceneProperty.sceneArray}}">
            <view class="item">
                <view class="mfic {{item.icon}} "></view>
                <view class="scene-name">{{item.name}}</view>
            </view>
        </view>
    </view>
</view>
