    transition: opacity .2s;
    z-index: 10;
}

.mf-tips-container .mf-tips-wrap {
    display: -webkit-box;
    display: -ms-flexbox;
    display: -webkit-flex;
    display: flex;
    position: relative;
    background: rgba(74,76,91,.8);
    border-radius: 2px;
}

.mf-tips-container .mf-tips-wrap .mf-triangle-wrap {
    position: absolute;
}

.mf-tips-container .mf-tips-wrap .mf-triangle-wrap .mf-triangle {
    display: block;
    position: absolute;
    width: 0;
    height: 0;
    vertical-align: middle;
    border-width: 0 6px 6px;
    border-style: solid;
    border-color: transparent transparent rgba(74,76,91,.8);
    font-style: normal;
}

.mf-tips-container .mf-tips-wrap .mf-close-btn {
    position: absolute;
    right: 16px;
    top: -4px;
    width: 10px;
    height: 10px;
    font-size: 12px;
    transform: scale(1.3);
}

.mf-tips-container .mf-tips-wrap .mf-msg {
    max-width: 80%;
    margin: 0 36px 0 16px;
    text-overflow: ellipsis;
    white-space: nowrap;
    overflow: hidden;
}

.mf-tips-container.top .mf-tips-wrap .mf-triangle-wrap {
    padding-top: 6px;
    top: 0;
    left: 0;
}

.mf-tips-container.top .mf-tips-wrap .mf-triangle-wrap .mf-triangle {
    position: absolute;
    top: -6px;
    left: 1px;
    transform: rotate(0);
}

.mf-tips-container.bottom .mf-tips-wrap .mf-triangle-wrap {
    padding-bottom: 10px;
    bottom: 0;
    left: 0;
}

.mf-tips-container.bottom .mf-tips-wrap .mf-triangle-wrap .mf-triangle {
    position: absolute;
    bottom: -6px;
    left: 1px;
    transform: rotate(180deg);
}

.mf-tips-container.left .mf-tips-wrap .mf-triangle-wrap {
    padding-left: 10px;
    top: 0;
    left: 0;
}

.mf-tips-container.left .mf-tips-wrap .mf-triangle-wrap .mf-triangle {
    position: absolute;
    top: 5px;
    left: -9px;
    transform: rotate(-90deg);
}

.mf-tips-container.right .mf-tips-wrap .mf-triangle-wrap {
    padding-right: 10px;
    top: 0;
    right: 0;
}

.mf-tips-container.right .mf-tips-wrap .mf-triangle-wrap .mf-triangle {
    position: absolute;
    top: 5px;
    right: -9px;
    transform: rotate(90deg);
}

.mf-tip-fade-enter-active,.mf-tip-fade-leave-active {
    opacity: 0;
}

.mf-tips-zoom-enter-active {
    animation: tips-in .4s;
    transform: translateZ(0);
}

.mf-tips-zoom-leave-active {
    animation: tips-out .2s;
    transform: translateZ(0);
}

@-webkit-keyframes tips-in {
    0% {
        transform: scale(0);
    }

    50% {
        transform: scale(1.1);
    }

    100% {
        transform: scale(1);
    }
}

@-webkit-keyframes tips-out {
    0% {
        transform: scale(1);
        opacity: 1;
    }

    100% {
        transform: scale(0);
        opacity: 0;
    }
}

