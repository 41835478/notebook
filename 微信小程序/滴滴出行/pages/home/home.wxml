                                    <cover-view class="callout-text">{{biz.realtime.realMap.innerRealMap.state.controlTip.text}}</cover-view>
                                </cover-view>
                                <cover-view class="probe-callout-img">
                                    <cover-image class="callout-arrow" src="{{biz.realtime.realMap.innerRealMap.state.triImg}}"></cover-image>
                                </cover-view>
                            </cover-view>
                            <cover-view class="map-top-shadow" wx:if="{{!biz.realtime.realMap.innerRealMap.state.hideShadow}}">
                                <cover-image class="map-top-shadow-img" src="{{biz.realtime.realMap.innerRealMap.state.mapShadowTop}}"></cover-image>
                            </cover-view>
                            <cover-view class="map-btm-shadow" wx:if="{{!biz.realtime.realMap.innerRealMap.state.hideShadow}}">
                                <cover-image class="map-btm-shadow-img" src="{{biz.realtime.realMap.innerRealMap.state.mapShadowBtm}}"></cover-image>
                            </cover-view>
                        </map>
                    </view>
                </view>
                <view class="realtime-bottom-wrap">
                    <view bindtap="_dispatch" class="searchbox" data-bindtap="jumpTosuggest" data-path="biz.realtime.searchBox">
                        <image class="search-icon" src="../../components/bus/realtime/searchBox/imgs/search.png"></image>
                        <text class="search-text">输入目的地或公交线路</text>
                    </view>
                    <view class="recommend">
                        <block wx:if="{{biz.realtime.recommend.props.recommendList.length==0}}">
                            <view class="recommend-not-normal" wx:if="{{biz.realtime.recommend.props.isFetchingRecommend}}">
                                <view>
                                    <view bindtap="_dispatch" class="mf-part-loading" data-bindtap="clickHandler" data-path="biz.realtime.recommend.loading" wx:if="{{biz.realtime.recommend.loading.state.isShow}}">
                                        <view class="mf-part-loading-bar" wx:if="{{biz.realtime.recommend.loading.props.status==='pending'}}">
                                            <view class="mf-part-loading-first"></view>
                                            <view class="mf-part-loading-second"></view>
                                            <view class="mf-part-loading-three"></view>
                                        </view>
                                        <view class="mf-part-loading-text" wx:if="{{biz.realtime.recommend.loading.props.status==='fail'}}">
      {{biz.realtime.recommend.loading.props.errorMsg}}
    </view>
                                    </view>
                                </view>
                            </view>
                            <view bindtap="_dispatch" class="recommend-not-normal" data-bindtap="tryAgain" data-path="biz.realtime.recommend" wx:elif="{{biz.realtime.recommend.props.errCode=='NET_FAILD_ERR'}}">网络错误，请检查后重试</view>
                            <view class="recommend-not-normal" wx:elif="{{biz.realtime.recommend.props.errCode=='NO_RECOMMEND'}}">暂无合适的推荐路线</view>
                        </block>
                        <view class="recommend-content" wx:else>
                            <view class="recommend-tab">
                                <scroll-view bindscroll="_dispatch" class="recommend-list" data-bindscroll="scrollHandler" data-path="biz.realtime.recommend" scrollLeft="{{biz.realtime.recommend.state.scrollLeft}}" scrollWithAnimation="true" scrollX="true">
                                    <view bindtap="_dispatch" class="recommend-item {{biz.realtime.recommend.props.recommendIndex==idx?'selected':''}}" data-bindtap="selectLine" data-index="{{idx}}" data-path="biz.realtime.recommend" wx:for="{{biz.realtime.recommend.props.recommendList}}" wx:for-index="idx" wx:for-item="recommend" wx:key="recommend">{{recommend.name}}</view>
                                </scroll-view>
                            </view>
                            <view class="recommend-card">
                                <view bindtap="_dispatch" class="recommend-detail" data-bindtap="jumpLineDetail" data-path="biz.realtime.recommend">
                                    <view class="line-name-dis">
                                        <span>{{biz.realtime.recommend.props.curRecommend.selected_stop.name}}</span>
                                        <span class="line-stop-distance">(距我{{biz.realtime.recommend.props.curRecommend.selected_stop.distance}})</span>
                                    </view>
                                    <view class="line-dest">开往{{biz.realtime.recommend.props.curRecommend.terminal_station_name}}</view>
                                    <view class="line-eta" wx:if="{{biz.realtime.recommend.props.curRecommend.eta}}">
                                        <view class="real-bus" wx:if="{{biz.realtime.recommend.props.curRecommend.eta.real_bus}}">
                                            <span class="bus-arrival" wx:if="{{biz.realtime.recommend.props.curRecommend.eta.time==0}}">{{biz.realtime.recommend.props.curRecommend.eta.description}}</span>
                                            <block wx:else>
                                                <span class="bus-time">{{biz.realtime.recommend.props.curRecommend.eta.description}}</span>
                                                <span class="bus-unit">分钟</span>
                                            </block>
                                            <image class="bus-signal" src="../../components/bus/realtime/recommend/img/bus_signal.gif" wx:if="{{biz.realtime.recommend.props.curRecommend.eta.show_icon}}"></image>
                                        </view>
                                        <view style="color:{{biz.realtime.recommend.props.curRecommend.eta.color}}" wx:else>
              {{biz.realtime.recommend.props.curRecommend.eta.description}}
            </view>
                                    </view>
                                </view>
                                <view bindtap="_dispatch" class="exchange-line" data-bindtap="exchangeLine" data-path="biz.realtime.recommend" wx:if="{{biz.realtime.recommend.props.curRecommend.pair_id}}">
                                    <view class="exchange-wrap">
                                        <image src="../../components/bus/realtime/recommend/img/home_icon_exchange@2x.png"></image>
                                    </view>
                                </view>
                                <view bindtap="_dispatch" class="exchange-line" data-bindtap="exchangeLineGray" data-path="biz.realtime.recommend" wx:else>
                                    <view class="exchange-wrap">
                                        <image src="../../components/bus/realtime/recommend/img/home_icon_exchange_gray@2x.png"></image>
                                    </view>
                                </view>
                            </view>
                        </view>
                    </view>
                </view>
            </view>
            <view wx:elif="{{biz.state.mode=='transfer'}}">
  transfer
</view>
        </view>
    </view>
</view>
