page {
    font-size: 17px;
    background-color: #f3f3f3;
}

.rule {
    heigth: 54px;
    font-size: 13px;
    line-height: 54px;
    color: #999;
    text-align: center;
}

.setting-ctn {
    display: flex;
    align-items: center;
    height: 56px;
    line-height: 56px;
    color: #666;
    background-color: #fff;
}

.setting-ctn .desc {
    flex: none;
    padding: 0 16px;
}

.setting-ctn input {
    flex: auto;
    color: #333;
}

.setting-ctn .visibility-ctn {
    flex: none;
    padding: 0 16px;
}

.setting-ctn .visibility-icon {
    display: inline-block;
    font-size: 18px;
}

.placeholder {
    color: #ccc;
}

.btn-ctn {
    margin: 20px 10px 0;
}