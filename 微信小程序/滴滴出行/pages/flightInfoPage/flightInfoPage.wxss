.flycode-info-wrapper .flycode-info-item.flycode-info-timer,.flycode-info-wrapper .flycode-info-record-list .flycode-info-item {
    border-top: 1rpx solid rgba(0,0,0,.08);
}

.mf-simple-picker {
    position: fixed;
    top: 0;
    left: 0;
    width: 100%;
    height: 100%;
    z-index: 600;
    display: none;
}

.mf-simple-picker.show {
    display: block;
}

.mf-simple-picker.show .mf-simple-picker-layer {
    animation: mf-simple-picker-fadeIn .3s ease-in-out both;
}

.mf-simple-picker.show .mf-simple-picker-board {
    animation: mf-simple-picker-up .3s ease-in-out both;
}

.mf-simple-picker.hide {
    display: block;
    animation: mf-simple-picker-hide .3s ease-in-out both;
}

.mf-simple-picker.hide .mf-simple-picker-layer {
    animation: mf-simple-picker-fadeOut .3s ease-in-out both;
}

.mf-simple-picker.hide .mf-simple-picker-board {
    animation: mf-simple-picker-down .3s ease-in-out both;
}

.mf-simple-picker .mf-simple-picker-layer {
    position: absolute;
    top: 0;
    left: 0;
    width: 100%;
    height: 100%;
    background-color: rgba(37,38,45,.4);
    opacity: 0;
}

.mf-simple-picker .mf-simple-picker-board {
    position: absolute;
    bottom: 0;
    left: 0;
    height: 273px;
    width: 100%;
    background-color: #fff;
    transform: translateY(100%);
}

.mf-simple-picker .mf-simple-picker-choose {
    position: relative;
    height: 60px;
    color: #999;
    border-bottom: 1rpx solid rgba(0,0,0,.08);
}

.mf-simple-picker .mf-simple-picker-choose .mf-simple-picker-title {
    margin: 0;
    line-height: 60px;
    font-weight: 400;
    text-align: center;
    font-size: 18px;
    color: #333;
}

.mf-simple-picker .mf-simple-picker-choose .cancel,.mf-simple-picker .mf-simple-picker-choose .confirm {
    position: absolute;
    top: 6px;
    padding: 16px;
    font-size: 14px;
}

.mf-simple-picker .mf-simple-picker-choose .confirm {
    right: 0;
    color: #fc9153;
}

.mf-simple-picker .mf-simple-picker-choose .cancel {
    left: 0;
}

.mf-simple-picker .mf-simple-picker-choose .cancel:active {
    color: #c2c2c2;
}

@-webkit-keyframes mf-simple-picker-fadeIn {
    from {
        opacity: 0;
    }

    to {
        opacity: 1;
    }
}

@-webkit-keyframes mf-simple-picker-fadeOut {
    from {
        opacity: 1;
    }

    to {
        opacity: 0;
    }
}

@-webkit-keyframes mf-simple-picker-up {
    from {
        transform: translateY(100%);
    }

    to {
        transform: translateY(0);
    }
}

@-webkit-keyframes mf-simple-picker-down {
    from {
        transform: translateY(0);
    }

    to {
        transform: translateY(100%);
    }
}

@-webkit-keyframes mf-simple-picker-hide {
    0%,99% {
        z-index: 600;
    }

    100% {
        z-index: -10;
    }
}

