<view class="imageplayer imageplayer-{{images.length}}">
    <image-player dataIndex="0" dataUrls="{{images}}" wx:if="{{images.length==1}}"></image-player>
    <block wx:if="{{images.length==2}}">
        <image-player dataIndex="0" dataUrls="{{images}}"></image-player>
        <image-player dataIndex="1" dataUrls="{{images}}"></image-player>
    </block>
    <block wx:if="{{images.length==3}}">
        <view class="mageplayer-3-view-first">
            <image-player dataIndex="0" dataUrls="{{images}}"></image-player>
        </view>
        <view>
            <image-player dataIndex="1" dataUrls="{{images}}"></image-player>
            <image-player dataIndex="2" dataUrls="{{images}}"></image-player>
        </view>
    </block>
    <block wx:if="{{images.length==4}}">
        <view class="mageplayer-4-view-first">
            <image-player dataIndex="0" dataUrls="{{images}}"></image-player>
        </view>
        <view>
            <image-player dataIndex="1" dataUrls="{{images}}"></image-player>
            <image-player dataIndex="2" dataUrls="{{images}}"></image-player>
            <image-player dataIndex="3" dataUrls="{{images}}"></image-player>
        </view>
    </block>
    <block wx:if="{{images.length==5}}">
        <view class="imageplayer-5-view-first">
            <image-player dataIndex="0" dataUrls="{{images}}"></image-player>
            <image-player dataIndex="1" dataUrls="{{images}}"></image-player>
        </view>
        <view class="imageplayer-5-view-flast">
            <image-player dataIndex="2" dataUrls="{{images}}"></image-player>
            <image-player dataIndex="3" dataUrls="{{images}}"></image-player>
            <image-player dataIndex="4" dataUrls="{{images}}"></image-player>
        </view>
    </block>
    <block wx:if="{{images.length==6}}">
        <view class="imageplayer-6-view-first">
            <view class="mageplayer-7-view-first-first">
                <image-player dataIndex="0" dataUrls="{{images}}"></image-player>
            </view>
            <view>
                <image-player dataIndex="1" dataUrls="{{images}}"></image-player>
                <image-player dataIndex="2" dataUrls="{{images}}"></image-player>
            </view>
        </view>
        <view class="imageplayer-6-view-flast">
            <image-player dataIndex="3" dataUrls="{{images}}"></image-player>
            <image-player dataIndex="4" dataUrls="{{images}}"></image-player>
            <image-player dataIndex="5" dataUrls="{{images}}"></image-player>
        </view>
    </block>
    <block wx:if="{{images.length==7}}">
        <view class="mageplayer-7-view-first">
            <image-player dataIndex="0" dataUrls="{{images}}"></image-player>
        </view>
        <view class="imageplayer-7-view">
            <image-player dataIndex="1" dataUrls="{{images}}"></image-player>
            <image-player dataIndex="2" dataUrls="{{images}}"></image-player>
            <image-player dataIndex="3" dataUrls="{{images}}"></image-player>
        </view>
        <view class="imageplayer-7-view">
            <image-player dataIndex="4" dataUrls="{{images}}"></image-player>
            <image-player dataIndex="5" dataUrls="{{images}}"></image-player>
            <image-player dataIndex="6" dataUrls="{{images}}"></image-player>
        </view>
    </block>
    <block wx:if="{{images.length==8}}">
        <view class="imageplayer-8-view-first">
            <image-player dataIndex="0" dataUrls="{{images}}"></image-player>
            <image-player dataIndex="1" dataUrls="{{images}}"></image-player>
        </view>
        <view class="imageplayer-8-view">
            <image-player dataIndex="2" dataUrls="{{images}}"></image-player>
            <image-player dataIndex="3" dataUrls="{{images}}"></image-player>
            <image-player dataIndex="4" dataUrls="{{images}}"></image-player>
        </view>
        <view class="imageplayer-8-view">
            <image-player dataIndex="5" dataUrls="{{images}}"></image-player>
            <image-player dataIndex="6" dataUrls="{{images}}"></image-player>
            <image-player dataIndex="7" dataUrls="{{images}}"></image-player>
        </view>
    </block>
    <block wx:if="{{images.length==9}}">
        <image-player dataIndex="{{index}}" dataUrls="{{images}}" wx:for="{{images}}" wx:key="{{item}}"></image-player>
    </block>
</view>
