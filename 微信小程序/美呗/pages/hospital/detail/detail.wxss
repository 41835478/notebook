.content-view {
    display: flex;
    flex-direction: column;
    background: white;
    padding-bottom: 110rpx;
}

.swiper image {
    h-vieweight: 50vw;
    width: 100%;
}

.image-view {
    height: 100%;
    width: 100%;
}

.banner-count-text {
    font-size: 30rpx;
    color: #fff;
}

.logo-view {
    width: 100rpx;
    height: 100rpx;
    margin-top: -50rpx;
    align-self: center;
    z-index: 1;
    position: relative;
}

.logo-image {
    width: 100%;
    height: 100%;
    border-radius: 50%;
    border-style: solid;
    border-width: 2rpx;
    border-color: white;
    background: #fff;
}

.hospital-profile-view {
    margin-top: -50rpx;
    display: flex;
    flex-direction: column;
    background-color: #333;
    width: 100%;
    justify-content: center;
    align-items: center;
}

.hospiatl-name-text {
    font-size: 34rpx;
    color: #fff;
    margin-top: 90rpx;
}

.address-view {
    display: flex;
    flex-direction: row;
    margin-top: 35rpx;
    align-items: center;
    justify-content: center;
    padding-left: 30rpx;
    padding-right: 30rpx;
}

.hospital-paper-view {
    display: flex;
    flex-direction: row;
    margin-top: 35rpx;
    align-items: center;
    justify-content: center;
    padding-left: 30rpx;
    padding-right: 30rpx;
}

.hospital-paper {
    color: #fff;
    font-size: 26rpx;
    font-weight: 400rpx;
    margin-right: 12rpx;
}

.hospital-address-text {
    font-size: 30rpx;
    color: #999;
}

.local-hospital-paper {
    width: 15rpx;
    height: 20rpx;
    position: relative;
    top: 2rpx;
}

.local-logo-image {
    width: 20rpx;
    min-width: 20rpx;
    height: 25rpx;
    margin-left: 10rpx;
    align-self: flex-start;
    margin-top: 10rpx;
}

.hopital-info-view {
    display: flex;
    flex-direction: row;
    justify-content: space-around;
    width: 80%;
    height: 100rpx;
    margin-top: 36rpx;
    margin-bottom: 25rpx;
}

.hospital-info-item-view {
    width: 100rpx;
    height: 100rpx;
    background-color: #333;
    border: solid 1px #999;
    display: flex;
    justify-content: center;
    align-items: center;
    text-align: center;
    border-radius: 50%;
}

.hospital-info-item-view text {
    font-size: 24rpx;
    color: #fff;
}

.hospital-integrity-logo-image {
    width: 38rpx;
    height: 41rpx;
}

.hospistal-content-view {
    display: flex;
    flex-direction: column;
}

.hospistal-title-text {
    color: #333;
    font-weight: bold;
    font-size: 34rpx;
    align-self: center;
    margin-top: 75rpx;
    margin-bottom: 56rpx;
}

.hospistal-content-text {
    font-size: 28rpx;
    line-height: 49rpx;
    color: #333;
    margin-left: 30rpx;
    margin-right: 30rpx;
}

.hospital-score-view {
    display: flex;
    flex-direction: column;
    margin-left: 30rpx;
    margin-right: 30rpx;
}

.hospital-score-all-view {
    margin-top: 68rpx;
    display: flex;
    flex-direction: row;
}

.hospital-score-all-title-text {
    font-size: 34rpx;
    color: #333;
    font-weight: bold;
}

.hospital-score-all-text {
    margin-left: 20rpx;
    font-size: 34rpx;
    color: #ff8083;
    font-weight: bold;
}

.hospital-score-cell-view {
    display: flex;
    flex-direction: column;
    margin-top: 40rpx;
}

.hospital-score-cell-info-view {
    display: flex;
    flex-direction: row;
    justify-content: space-between;
}

.hospital-score-cell-info-view text {
    font-size: 28rpx;
    color: #333;
}

.hospital-score-cell-slider-view {
    height: 15rpx;
    display: flex;
    background-color: #e5e5e5;
    border-radius: 6rpx;
    margin-top: 20rpx;
}

.hospital-score-cell-slider-image {
    background-image: linear-gradient(86deg,#ff5c9b 0%,#ffa66a 100%),linear-gradient(#e5e5e5,#e5e5e5);
    border-radius: 6px;
    height: 100%;
    width: 100%;
}

.photo-view {
    height: 165rpx;
    width: 100%;
    margin-top: 60rpx;
    margin-bottom: 74rpx;
}

.photo-view view {
    height: 100%;
    width: 100%;
    display: flex;
    flex-direction: row;
    flex-wrap: nowrap;
}

.photo-view image {
    height: 100%;
    width: 165rpx;
    margin-left: 30rpx;
    flex-shrink: 0;
}

.photo-view image:last-child {
    padding-right: 30rpx;
}

.section-view {
    display: flex;
    flex-direction: column;
    border-top: 20rpx solid #f0f0f0;
    border-bottom: 1px solid #e1e1e1;
    background: #fff;
}

.header-title-view {
    display: flex;
    flex-direction: row;
    height: 100rpx;
    width: 100%;
    align-items: center;
}

.header-title-tips-view {
    width: 8rpx;
    height: 30rpx;
    background-color: #ff8083;
    margin-left: 30rpx;
}

.header-title-text {
    margin-left: 15rpx;
    font-size: 30rpx;
    color: #333;
    border-width: blod;
}

.header-title-count-text {
    margin-left: 15rpx;
    font-size: 30rpx;
    color: #999;
    border-width: blod;
}

.more-view {
    display: flex;
    justify-content: center;
    align-items: center;
    height: 100rpx;
    width: 100%;
    border-top: 1px solid #e1e1e1;
    background: #fff;
}

.diary-more-view {
    margin-top: -20rpx;
}

.more-view text {
    font-size: 26rpx;
    color: #ff8083;
}

.hospital-list-view {
    background: #f5f5f5;
}

.bottom-view {
    display: flex;
    flex-direction: row;
    position: fixed;
    bottom: 0rpx;
    height: 110rpx;
    width: 100%;
    background-color: #ff8083;
}

.bottom-left-view {
    width: 140rpx;
    height: 100%;
}

.bottom-left-view image {
    width: 62rpx;
    height: 56rpx;
}

.bottom-left-view {
    background-color: rgba(255,255,255,0.5);
    display: flex;
    justify-content: center;
    align-items: center;
}

.bottom-right-view {
    display: flex;
    justify-content: center;
    align-items: center;
    background-color: #ff8083;
    flex-grow: 1;
}

.bottom-right-view text {
    font-size: 34rpx;
    color: #fff;
}