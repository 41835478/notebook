.list-view {
    display: flex;
    flex-direction: column;
    background-color: #f5f5f5;
}

.cell-view {
    display: flex;
    flex-direction: row;
    border-top: 1px solid #f5f5f5;
    background-color: white;
}

.cell-left-view {
    display: flex;
    flex-direction: column;
    width: 160rpx;
    flex-shrink: 0;
    align-items: center;
}

.cell-right-view {
    display: flex;
    flex-direction: column;
}

.cell-head-image {
    width: 100rpx;
    height: 100rpx;
    margin-top: 33rpx;
    border-radius: 50%;
}

.cell-score-text {
    font-size: 34rpx;
    color: #ff8083;
    margin-top: 29rpx;
}

.cell-name-text {
    font-size: 32rpx;
    color: #333;
    margin-top: 35rpx;
    margin-right: 10rpx;
}

.cell-address-text {
    font-size: 26rpx;
    color: #666;
    margin-top: 19rpx;
    margin-right: 10rpx;
}

.cell-case-number-text {
    font-size: 26rpx;
    color: #333;
    margin-top: 42rpx;
    margin-bottom: 35rpx;
}

.refreshFooter {
    margin-top: 20rpx;
}

.flter-view {
    display: flex;
    flex-direction: row;
    width: 100vw;
    height: 103rpx;
    justify-content: center;
    align-items: center;
    background-color: white;
}

.flter-item-view {
    width: 33vw;
    display: flex;
    flex-direction: row;
    justify-content: center;
    align-items: center;
}

.flter-item-sub-view {
    display: flex;
    flex-direction: row;
    justify-content: center;
    align-items: center;
}

.flter-item-sub-view text {
    font-size: 26rpx;
    color: #232323;
}

.flter-item-sub-view image {
    width: 18rpx;
    height: 10rpx;
    margin-left: 8rpx;
}

.flter-item-link-view {
    width: 1rpx;
    height: 18rpx;
    background-color: #c4c4c4;
}