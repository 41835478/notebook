.search-input {
    width: 750rpx;
    background: #fff;
    padding: 30rpx;
    height: 150rpx;
    position: fixed;
    top: 0;
    left: 0;
    z-index: 999;
}

.search-off {
    width: 60rpx;
    height: 90rpx;
    position: absolute;
    right: 30rpx;
    top: 30rpx;
    z-index: 200;
    display: flex;
    justify-content: center;
    align-items: center;
}

.seach-off-icon {
    width: 30rpx;
    height: 30rpx;
}

.content {
    margin-top: 150rpx;
    height: calc(100vh - 150rpx);
    overflow-x: hidden;
    -webkit-overflow-scrolling: touch;
}

.input-style {
    background: #f5f5f5;
    height: 90rpx;
    padding-left: 90rpx;
    border-radius: 5px;
    padding-right: 50rpx;
}

.seach-icon {
    width: 30rpx;
    height: 30rpx;
    position: absolute;
    top: 50%;
    margin-top: -15rpx;
    left: 65rpx;
}

.hots-container {
    padding: 30rpx;
    padding-bottom: 0;
    background: #fff;
}

.title-icon {
    width: 30rpx;
    height: 30rpx;
    margin-right: 10rpx;
}

.search-titles {
    font-size: 14px;
    font-weight: bold;
    padding-bottom: 30rpx;
}

.hots {
    font-size: 0;
}

.hots-item {
    width: 50%;
    padding: 37rpx 0;
    border-top: 1px solid #f5f5f5;
    display: inline-block;
    vertical-align: top;
    font-size: 14px;
}

.hots-item text {
    display: inline-block;
    vertical-align: middle;
    max-width: 230rpx;
    white-space: nowrap;
    text-overflow: ellipsis;
    overflow: hidden;
    color: #333;
}

.hots-icon {
    width: 35rpx;
    height: 35rpx;
    display: inline-block;
    vertical-align: middle;
    margin-left: 20rpx;
}

.hots-item:nth-child(odd) .hots-item-inner {
    border-right: 1px solid #f5f5f5;
}

.hots-item:nth-child(even) .hots-item-inner {
    padding-left: 30rpx;
}

.hisotry-item {
    padding: 30rpx 0;
    color: #333;
    border-top: 1px solid #f5f5f5;
    font-size: 14px;
}

.hisotry-item view {
    width: 70%;
    white-space: nowrap;
    text-overflow: ellipsis;
    overflow: hidden;
}

.del-icon {
    width: 30rpx;
    height: 30rpx;
}

.search-result-list {
    background: #fff;
    margin-bottom: 20rpx;
}

.search-result-list:last-child {
    margin-bottom: 0;
}

.search-result-tilte {
    padding: 30rpx;
    font-weight: bold;
    display: flex;
    align-items: center;
    padding-bottom: 0;
}

.title-border {
    width: 10rpx;
    height: 30rpx;
    background: #ff8083;
    margin-right: 20rpx;
}

.viewmore {
    background: #fff;
    border-top: 1px solid #f5f5f5;
    height: 90rpx;
    color: #ff8083;
}

.list-view {
    display: flex;
    flex-direction: column;
    background-color: #f5f5f5;
}

.cell-view {
    display: flex;
    flex-direction: row;
    border-top: 1px solid #f5f5f5;
    background-color: white;
}

.cell-left-view {
    display: flex;
    flex-direction: column;
    width: 160rpx;
    flex-shrink: 0;
    align-items: center;
}

.cell-right-view {
    display: flex;
    flex-direction: column;
}

.cell-head-image {
    width: 100rpx;
    height: 100rpx;
    margin-top: 33rpx;
    border-radius: 50%;
}

.cell-score-text {
    font-size: 34rpx;
    color: #ff8083;
    margin-top: 29rpx;
}

.cell-name-text {
    font-size: 32rpx;
    color: #333;
    margin-top: 35rpx;
    margin-right: 10rpx;
}

.cell-address-text {
    font-size: 26rpx;
    color: #666;
    margin-top: 19rpx;
    margin-right: 10rpx;
}

.cell-case-number-text {
    font-size: 26rpx;
    color: #333;
    margin-top: 42rpx;
    margin-bottom: 35rpx;
}

.refreshFooter {
    margin-top: 20rpx;
}

.flter-view {
    display: flex;
    flex-direction: row;
    width: 100vw;
    height: 103rpx;
    justify-content: center;
    align-items: center;
    background-color: white;
}

.flter-item-view {
    width: 33vw;
    display: flex;
    flex-direction: row;
    justify-content: center;
    align-items: center;
}

.flter-item-sub-view {
    display: flex;
    flex-direction: row;
    justify-content: center;
    align-items: center;
}

.flter-item-sub-view text {
    font-size: 26rpx;
    color: #232323;
}

.flter-item-sub-view image {
    width: 18rpx;
    height: 10rpx;
    margin-left: 8rpx;
}

.flter-item-link-view {
    width: 1rpx;
    height: 18rpx;
    background-color: #c4c4c4;
}